
load @treesurf/land/code/javascript/base/native-number
  take form native-number

load @treesurf/land/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/form/sink
  take form sink
    save form sink-link-text

form acceptance, name <Acceptance>
  take date, name <date>
    like native-number
    lock free
  take ip, name <ip>
    like native-string
    lock free
  take status, name <status>
    like sink-link-text
      text <accepted>
      text <refused>
    lock free
  take user-agent, name <user_agent>
    like native-string
    lock free

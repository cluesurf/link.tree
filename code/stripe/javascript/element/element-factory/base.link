
load @nerdbond/land/code/javascript/base/element-type
  take form element-type

load @nerdbond/land/code/javascript/stripejs/element/card-element-options
  take form card-element-options

load @nerdbond/land/code/javascript/stripejs/element/iban-element-options
  take form iban-element-options

load @nerdbond/land/code/javascript/stripejs/element/ideal-bank-options
  take form ideal-bank-options

load @nerdbond/land/code/javascript/stripejs/element/payment-button-options
  take form payment-button-options

load @nerdbond/land/code/javascript/stripejs/element/stripe-element
  take form stripe-element

load @nerdbond/seed/code/form/sink
  take form sink
    save form sink-link-text

form element-factory, name <ElementFactory>
  task create, name <create>
    take type, like element-type
    take options
      like sink-link-text
        like card-element-options
        like iban-element-options
        like ideal-bank-options
        like payment-button-options
    free seed, like stripe-element

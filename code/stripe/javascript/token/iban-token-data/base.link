
load @nerdbond/land/code/javascript/base/native-string
  take form native-string

load @nerdbond/seed/code/form/sink
  take form sink
    save form sink-link-text

form iban-token-data, name <IBANTokenData>
  take currency, name <currency>
    like native-string
    lock free
  take account-holder-name, name <account_holder_name>
    like native-string
    lock free
  take account-holder-type, name <account_holder_type>
    like sink-link-text
      text <individual>
      text <company>
    lock free

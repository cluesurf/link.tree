
load @textsurf/land/code/javascript/base/error-type
  take form error-type

load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/land/code/javascript/base/native-undefined
  take form native-undefined

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

form stripe-error, name <StripeError>
  take type, name <type>
    like error-type
    lock free
  take charge, name <charge>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take code, name <code>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take decline-code, name <decline_code>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take doc-url, name <doc_url>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take message, name <message>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take param, name <param>
    like sink-link-text
      like native-string
      like native-undefined
    lock free

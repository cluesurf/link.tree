
load @tunebond/land/code/javascript/base/native-string
  take form native-string

load @tunebond/land/code/javascript/base/native-undefined
  take form native-undefined

load @tunebond/land/code/javascript/stripejs/payment/payment-item
  take form payment-item

load @tunebond/land/code/javascript/stripejs/shipping/shipping-option
  take form shipping-option

load @tunebond/seed/code/form/list
  take form list
    save form list-link-text

load @tunebond/seed/code/form/sink
  take form sink
    save form sink-link-text

form update-options, name <UpdateOptions>
  take currency, name <currency>
    like native-string
    lock free
  take total, name <total>
    like payment-item
    lock free
  take display-items, name <displayItems>
    like sink-link-text
      like list-link-text
        like payment-item
      like native-undefined
    lock free
  take shipping-options, name <shippingOptions>
    like sink-link-text
      like list-link-text
        like shipping-option
      like native-undefined
    lock free


load @wavebond/snow/base/list
  take form list

load @wavebond/snow/base/native-string
  take form native-string

load @wavebond/snow/stripe.com/2020-08-27/base/error
  take form error

load @wavebond/snow/stripe.com/2020-08-27/base/get3d-secure-three-d-secure-body
  take form get3d-secure-three-d-secure-body

load @wavebond/snow/stripe.com/2020-08-27/base/three-d-secure
  take form three-d-secure

boot get3d-secure-three-d-secure, name <Get3dSecureThreeDSecure>
  deed get
  note <Retrieves a 3D Secure object.>

  take three-d-secure
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get3d-secure-three-d-secure-body, like get3d-secure-three-d-secure-body
    void take

  line </v1/3d_secure/{three-d-secure}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get3d-secure-three-d-secure-body

  loot 200
    seed json, like three-d-secure
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>

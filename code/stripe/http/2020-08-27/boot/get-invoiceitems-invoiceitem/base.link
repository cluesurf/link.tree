
load @nerdbond/snow/base/list
  take form list

load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/stripe.com/2020-08-27/base/error
  take form error

load @nerdbond/snow/stripe.com/2020-08-27/base/get-invoiceitems-invoiceitem-body
  take form get-invoiceitems-invoiceitem-body

load @nerdbond/snow/stripe.com/2020-08-27/base/invoiceitem
  take form invoiceitem

boot get-invoiceitems-invoiceitem, name <GetInvoiceitemsInvoiceitem>
  deed get
  note <Retrieves the invoice item with the given ID.>

  take invoiceitem
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-invoiceitems-invoiceitem-body, like get-invoiceitems-invoiceitem-body
    void take

  line </v1/invoiceitems/{invoiceitem}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-invoiceitems-invoiceitem-body

  loot 200
    seed json, like invoiceitem
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>

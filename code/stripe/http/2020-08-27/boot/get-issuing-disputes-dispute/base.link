
load @textsurf/snow/base/list
  take form list

load @textsurf/snow/base/native-string
  take form native-string

load @textsurf/snow/stripe.com/2020-08-27/base/error
  take form error

load @textsurf/snow/stripe.com/2020-08-27/base/get-issuing-disputes-dispute-body
  take form get-issuing-disputes-dispute-body

load @textsurf/snow/stripe.com/2020-08-27/base/issuing-dispute
  take form issuing-dispute

boot get-issuing-disputes-dispute, name <GetIssuingDisputesDispute>
  deed get
  note <Retrieves an Issuing `Dispute` object.>

  take dispute
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-issuing-disputes-dispute-body, like get-issuing-disputes-dispute-body
    void take

  line </v1/issuing/disputes/{dispute}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-issuing-disputes-dispute-body

  loot 200
    seed json, like issuing-dispute
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>


load @tunebond/snow/base/list
  take form list

load @tunebond/snow/base/native-string
  take form native-string

load @tunebond/snow/stripe.com/2020-08-27/base/credit-note
  take form credit-note

load @tunebond/snow/stripe.com/2020-08-27/base/error
  take form error

load @tunebond/snow/stripe.com/2020-08-27/base/get-credit-notes-id-body
  take form get-credit-notes-id-body

boot get-credit-notes-id, name <GetCreditNotesId>
  deed get
  note <Retrieves the credit note object with the given identifier.>

  take id
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-credit-notes-id-body, like get-credit-notes-id-body
    void take

  line </v1/credit_notes/{id}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-credit-notes-id-body

  loot 200
    seed json, like credit-note
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>

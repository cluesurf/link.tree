
load @nerdbond/snow/base/list
  take form list

load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/stripe.com/2020-08-27/base/apple-pay-domain
  take form apple-pay-domain

load @nerdbond/snow/stripe.com/2020-08-27/base/error
  take form error

load @nerdbond/snow/stripe.com/2020-08-27/base/get-apple-pay-domains-domain-body
  take form get-apple-pay-domains-domain-body

boot get-apple-pay-domains-domain, name <GetApplePayDomainsDomain>
  deed get
  note <Retrieve an apple pay domain.>

  take domain
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-apple-pay-domains-domain-body, like get-apple-pay-domains-domain-body
    void take

  line </v1/apple_pay/domains/{domain}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-apple-pay-domains-domain-body

  loot 200
    seed json, like apple-pay-domain
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>

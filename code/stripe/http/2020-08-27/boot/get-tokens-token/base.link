
load @drumwork/snow/base/list
  take form list

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/stripe.com/2020-08-27/base/error
  take form error

load @drumwork/snow/stripe.com/2020-08-27/base/get-tokens-token-body
  take form get-tokens-token-body

load @drumwork/snow/stripe.com/2020-08-27/base/token
  take form token

boot get-tokens-token, name <GetTokensToken>
  note <Retrieves the token with the given ID.>

  take token
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-tokens-token-body, like get-tokens-token-body
    void take

  line </v1/tokens/{token}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-tokens-token-body

  loot 200
    seed json, like token
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>
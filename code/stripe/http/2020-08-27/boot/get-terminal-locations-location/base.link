
load @wavebond/snow/base/list
  take form list

load @wavebond/snow/base/native-string
  take form native-string

load @wavebond/snow/stripe.com/2020-08-27/base/error
  take form error

load @wavebond/snow/stripe.com/2020-08-27/base/get-terminal-locations-location-body
  take form get-terminal-locations-location-body

load @wavebond/snow/stripe.com/2020-08-27/base/terminal-location
  take form terminal-location

boot get-terminal-locations-location, name <GetTerminalLocationsLocation>
  deed get
  note <Retrieves a `Location` object.>

  take location
    like native-string
  take expand
    like list
      like native-string
    note <Specifies which fields in the response should be expanded.>
    void take
  take get-terminal-locations-location-body, like get-terminal-locations-location-body
    void take

  line </v1/terminal/locations/{location}>

  find <expand>, loan expand

  seed x-www-form-urlencoded, loan get-terminal-locations-location-body

  loot 200
    seed json, like terminal-location
    note <Successful response.>
  loot fall
    seed json, like error
    note <Error response.>


form authentication-domain, name <authentication_domain>
  take automatic-security, name <automatic_security>
  take custom-spf, name <custom_spf>
  take default, name <default>
  take dns, name <dns>
    like form
      take dkim1, name <dkim1>
        like form
          take data, name <data>
          take host, name <host>
          take type, name <type>
          take valid, name <valid>
      take dkim2, name <dkim2>
        like form
          take data, name <data>
          take host, name <host>
          take type, name <type>
          take valid, name <valid>
      take mail-cname, name <mail_cname>
        like form
          take data, name <data>
          take host, name <host>
          take type, name <type>
          take valid, name <valid>
  take domain, name <domain>
  take id, name <id>
  take ips, name <ips>
  take legacy, name <legacy>
  take subdomain, name <subdomain>
  take user-id, name <user_id>
  take username, name <username>
  take valid, name <valid>
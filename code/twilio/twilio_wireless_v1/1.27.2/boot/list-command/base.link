
load @wavebond/snow/base/native-integer
  take form native-integer

load @wavebond/snow/base/native-string
  take form native-string

boot list-command, name <ListCommand>
  note <Retrieve a list of Commands from your account.>

  take sim
    like native-string
    note <The \`sid\` or \`unique\_name\` of the \[Sim resources\](https://www.twilio.com/docs/wireless/api/sim-resource) to read.>
    void take
  take status
    like native-string
    note <The status of the resources to read. Can be: \`queued\`, \`sent\`, \`delivered\`, \`received\`, or \`failed\`.>
    void take
  take direction
    like native-string
    note <Only return Commands with this direction value.>
    void take
  take transport
    like native-string
    note <Only return Commands with this transport value. Can be: \`sms\` or \`ip\`.>
    void take
  take page-size
    like native-integer
    note <How many resources to return in each list page. The default is 50, and the maximum is 1000.>
    void take

  line </v1/Commands>

  find <Sim>, loan sim
  find <Status>, loan status
  find <Direction>, loan direction
  find <Transport>, loan transport
  find <PageSize>, loan page-size

  loot 200
    seed json
      like form
        like take commands, name <commands>
        like   void take
        like take meta, name <meta>
        like   like form
        like     take first-page-url, name <first_page_url>
        like       void take
        like     take key, name <key>
        like       void take
        like     take next-page-url, name <next_page_url>
        like       void take
        like     take page, name <page>
        like       void take
        like     take page-size, name <page_size>
        like       void take
        like     take previous-page-url, name <previous_page_url>
        like       void take
        like     take url, name <url>
        like       void take
        like   void take
    note <OK>


load @textsurf/snow/base/native-integer
  take form native-integer

load @textsurf/snow/base/native-string
  take form native-string

boot list-installed-add-on-extension, name <ListInstalledAddOnExtension>
  note <Retrieve a list of Extensions for the Installed Add-on.>

  take installed-add-on-sid
    like native-string
    note <The SID of the InstalledAddOn resource with the extensions to read.>
  take page-size
    like native-integer
    note <How many resources to return in each list page. The default is 50, and the maximum is 1000.>
    void take

  line </marketplace/InstalledAddOns/{installed-add-on-sid}/Extensions>

  find <PageSize>, loan page-size

  loot 200
    seed json
      like form
        like take extensions, name <extensions>
        like   void take
        like take meta, name <meta>
        like   like form
        like     take first-page-url, name <first_page_url>
        like       void take
        like     take key, name <key>
        like       void take
        like     take next-page-url, name <next_page_url>
        like       void take
        like     take page, name <page>
        like       void take
        like     take page-size, name <page_size>
        like       void take
        like     take previous-page-url, name <previous_page_url>
        like       void take
        like     take url, name <url>
        like       void take
        like   void take
    note <OK>


load @tunebond/snow/base/native-integer
  take form native-integer

load @tunebond/snow/base/native-string
  take form native-string

boot list-key, name <ListKey>
  note <Retrieve a list of all Keys credentials belonging to the Fleet.>

  take fleet-sid
    like native-string
  take device-sid
    like native-string
    note <Filters the resulting list of Keys by a unique string identifier of an authenticated Device.>
    void take
  take page-size
    like native-integer
    note <How many resources to return in each list page. The default is 50, and the maximum is 1000.>
    void take

  line </DeployedDevices/Fleets/{fleet-sid}/Keys>

  find <DeviceSid>, loan device-sid
  find <PageSize>, loan page-size

  loot 200
    seed json
      like form
        like take keys, name <keys>
        like   void take
        like take meta, name <meta>
        like   like form
        like     take first-page-url, name <first_page_url>
        like       void take
        like     take key, name <key>
        like       void take
        like     take next-page-url, name <next_page_url>
        like       void take
        like     take page, name <page>
        like       void take
        like     take page-size, name <page_size>
        like       void take
        like     take previous-page-url, name <previous_page_url>
        like       void take
        like     take url, name <url>
        like       void take
        like   void take
    note <OK>


load @treesurf/snow/base/native-integer
  take form native-integer

load @treesurf/snow/base/native-string
  take form native-string

boot list-query, name <ListQuery>
  take assistant-sid
    like native-string
    note <The unique ID of the parent Assistant.>
  take language
    like native-string
    note <An ISO language-country string of the sample.>
    void take
  take model-build
    like native-string
    note <The Model Build Sid or unique name of the Model Build to be queried.>
    void take
  take status
    like native-string
    note <A string that described the query status. The values can be: pending\_review, reviewed, discarded>
    void take
  take page-size
    like native-integer
    note <How many resources to return in each list page. The default is 50, and the maximum is 1000.>
    void take

  line </understand/Assistants/{assistant-sid}/Queries>

  find <Language>, loan language
  find <ModelBuild>, loan model-build
  find <Status>, loan status
  find <PageSize>, loan page-size

  loot 200
    seed json
      like form
        like take meta, name <meta>
        like   like form
        like     take first-page-url, name <first_page_url>
        like       void take
        like     take key, name <key>
        like       void take
        like     take next-page-url, name <next_page_url>
        like       void take
        like     take page, name <page>
        like       void take
        like     take page-size, name <page_size>
        like       void take
        like     take previous-page-url, name <previous_page_url>
        like       void take
        like     take url, name <url>
        like       void take
        like   void take
        like take queries, name <queries>
        like   void take
    note <OK>

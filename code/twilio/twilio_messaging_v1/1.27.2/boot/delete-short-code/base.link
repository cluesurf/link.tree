
load @nerdbond/snow/base/native-string
  take form native-string

boot delete-short-code, name <DeleteShortCode>
  take service-sid
    like native-string
    note <The SID of the \[Service\](https://www.twilio.com/docs/chat/rest/service-resource) to delete the resource from.>
  take sid
    like native-string
    note <The SID of the ShortCode resource to delete.>

  line </v1/Services/{service-sid}/ShortCodes/{sid}>

  loot 204
    note <The resource was deleted successfully.>

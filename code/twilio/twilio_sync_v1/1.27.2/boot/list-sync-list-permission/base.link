
load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

boot list-sync-list-permission, name <ListSyncListPermission>
  note <Retrieve a list of all Permissions applying to a Sync List.>

  take service-sid
    like native-string
    note <The SID of the \[Sync Service\](https://www.twilio.com/docs/sync/api/service) with the Sync List Permission resources to read.>
  take list-sid
    like native-string
    note <The SID of the Sync List with the Sync List Permission resources to read. Can be the Sync List resource&#39;s \`sid\` or its \`unique\_name\`.>
  take page-size
    like native-integer
    note <How many resources to return in each list page. The default is 50, and the maximum is 1000.>
    void take

  line </v1/Services/{service-sid}/Lists/{list-sid}/Permissions>

  find <PageSize>, loan page-size

  loot 200
    seed json
      like form
        like take meta, name <meta>
        like   like form
        like     take first-page-url, name <first_page_url>
        like       void take
        like     take key, name <key>
        like       void take
        like     take next-page-url, name <next_page_url>
        like       void take
        like     take page, name <page>
        like       void take
        like     take page-size, name <page_size>
        like       void take
        like     take previous-page-url, name <previous_page_url>
        like       void take
        like     take url, name <url>
        like       void take
        like   void take
        like take permissions, name <permissions>
        like   void take
    note <OK>
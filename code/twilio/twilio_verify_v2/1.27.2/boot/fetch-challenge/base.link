
load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/twilio.com/twilio_verify_v2/1.27.2/base/verify-v2-service-entity-challenge
  take form verify-v2-service-entity-challenge

boot fetch-challenge, name <FetchChallenge>
  note <Fetch a specific Challenge.>

  take service-sid
    like native-string
    note <The unique SID identifier of the Service.>
  take identity
    like native-string
    note <Customer unique identity for the Entity owner of the Challenges. This identifier should be immutable, not PII, length between 8 and 64 characters, and generated by your external system, such as your user&#39;s UUID, GUID, or SID. It can only contain dash (-) separated alphanumeric characters.>
  take sid
    like native-string
    note <A 34 character string that uniquely identifies this Challenge.>

  line </v2/Services/{service-sid}/Entities/{identity}/Challenges/{sid}>

  loot 200
    seed json, like verify-v2-service-entity-challenge
    note <OK>

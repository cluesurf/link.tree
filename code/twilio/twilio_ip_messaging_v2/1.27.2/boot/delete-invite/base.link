
load @nerdbond/snow/base/native-string
  take form native-string

boot delete-invite, name <DeleteInvite>
  take service-sid
    like native-string
  take channel-sid
    like native-string
  take sid
    like native-string

  line </v2/Services/{service-sid}/Channels/{channel-sid}/Invites/{sid}>


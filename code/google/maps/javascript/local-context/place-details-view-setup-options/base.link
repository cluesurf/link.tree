
load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/local-context/place-details-layout-mode
  take form place-details-layout-mode

load @drumwork/snow/code/javascript/google/maps/local-context/place-details-position
  take form place-details-position

form place-details-view-setup-options, name <google.maps.localContext.PlaceDetailsViewSetupOptions>
  take hides-on-map-click, name <hidesOnMapClick>
    like native-boolean
    lock free
  take layout-mode, name <layoutMode>
    like sink-link-text
      like local-context-place-details-layout-mode
      like native-null
    lock free
  take position, name <position>
    like sink-link-text
      like local-context-place-details-position
      like native-null
    lock free

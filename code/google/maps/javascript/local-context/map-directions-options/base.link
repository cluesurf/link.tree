
load @treesurf/land/code/javascript/base/function
  take form function

load @treesurf/land/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @treesurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @treesurf/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @treesurf/snow/code/javascript/google/maps/maps-event-listener
  take form maps-event-listener

form map-directions-options, name <google.maps.localContext.MapDirectionsOptions>
  take origin
    like sink-link-text
      like lat-lng
      like lat-lng-literal

  task add-listener, name <addListener>
    take event-name, like native-string
    take handler, like function
    free seed, like maps-event-listener

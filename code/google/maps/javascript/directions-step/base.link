
load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/seed/code/form/list
  take form list
    save form list-link-text

load @textsurf/snow/code/javascript/google/maps/directions-step
  take form directions-step

load @textsurf/snow/code/javascript/google/maps/distance
  take form distance

load @textsurf/snow/code/javascript/google/maps/duration
  take form duration

load @textsurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @textsurf/snow/code/javascript/google/maps/transit-details
  take form transit-details

load @textsurf/snow/code/javascript/google/maps/travel-mode
  take form travel-mode

form directions-step, name <google.maps.DirectionsStep>
  take distance, name <distance>
    like distance
    lock free
  take duration, name <duration>
    like duration
    lock free
  take encoded-lat-lngs, name <encoded_lat_lngs>
    like native-string
    lock free
  take end-location, name <end_location>
    like lat-lng
    lock free
  take end-point, name <end_point>
    like lat-lng
    lock free
  take instructions, name <instructions>
    like native-string
    lock free
  take lat-lngs, name <lat_lngs>
    like list-link-text
      like lat-lng
    lock free
  take maneuver, name <maneuver>
    like native-string
    lock free
  take path, name <path>
    like list-link-text
      like lat-lng
    lock free
  take polyline, name <polyline>
    like list-link-text
      like directions-step
    lock free
  take start-location, name <start_location>
    like lat-lng
    lock free
  take start-point, name <start_point>
    like lat-lng
    lock free
  take steps, name <steps>
    like list-link-text
      like directions-step
    lock free
  take transit, name <transit>
    like transit-details
    lock free
  take transit-details, name <transit_details>
    like transit-details
    lock free
  take travel-mode, name <travel_mode>
    like travel-mode
    lock free


load @tunebond/land/code/javascript/base/native-boolean
  take form native-boolean

load @tunebond/land/code/javascript/base/native-null
  take form native-null

load @tunebond/land/code/javascript/base/native-number
  take form native-number

load @tunebond/land/code/javascript/base/native-string
  take form native-string

load @tunebond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @tunebond/snow/code/javascript/google/maps/size
  take form size

form size, name <google.maps.Size>
  take height, like native-number
  take width, like native-number

  task equals, name <equals>
    take other
      like sink-link-text
        like size
        like native-null
    free seed, like native-boolean

  task to-string, name <toString>
    free seed, like native-string

form size-constructor
  hook make
    take width, like native-number
    take height, like native-number
    take width-unit, like native-string
      void take
    take height-unit, like native-string
      void take

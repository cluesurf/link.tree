    save form sink-link-text

load @wavebond/snow/code/javascript/google/maps/map
  take form map

load @wavebond/snow/code/javascript/google/maps/mvc-object
  take form mvc-object

load @wavebond/snow/code/javascript/google/maps/street-view-panorama
  take form street-view-panorama

form info-window-open-options, name <google.maps.InfoWindowOpenOptions>
  take anchor, name <anchor>
    like sink-link-text
      like mvc-object
      like native-null
    lock free
  take map, name <map>
    like sink-link-text
      like map
      like native-null
      like street-view-panorama
    lock free
  take should-focus, name <shouldFocus>
    like sink-link-text
      like native-boolean
      like native-null
    lock free


load @textsurf/land/code/javascript/base/native-null
  take form native-null

load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @textsurf/snow/code/javascript/google/maps/kml-author
  take form kml-author

form kml-feature-data, name <google.maps.KmlFeatureData>
  take author, name <author>
    like sink-link-text
      like kml-author
      like native-null
    lock free
  take description, name <description>
    like native-string
    lock free
  take id, name <id>
    like native-string
    lock free
  take info-window-html, name <infoWindowHtml>
    like native-string
    lock free
  take name, name <name>
    like native-string
    lock free
  take snippet, name <snippet>
    like native-string
    lock free


load @wavebond/land/code/javascript/base/native-null
  take form native-null

load @wavebond/land/code/javascript/base/native-void
  take form native-void

load @wavebond/land/code/javascript/base/promise
  take form promise

load @wavebond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @wavebond/snow/code/javascript/google/maps/street-view-location-request
  take form street-view-location-request

load @wavebond/snow/code/javascript/google/maps/street-view-pano-request
  take form street-view-pano-request


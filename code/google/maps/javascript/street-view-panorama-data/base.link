
load @nerdbond/land/code/javascript/base/native-string
  take form native-string

load @nerdbond/seed/code/form/list
  take form list
    save form list-link-text

load @nerdbond/snow/code/javascript/google/maps/street-view-link
  take form street-view-link

load @nerdbond/snow/code/javascript/google/maps/street-view-location
  take form street-view-location

load @nerdbond/snow/code/javascript/google/maps/street-view-tile-data
  take form street-view-tile-data

form street-view-panorama-data, name <google.maps.StreetViewPanoramaData>
  take copyright, name <copyright>
    like native-string
    lock free
  take image-date, name <imageDate>
    like native-string
    lock free
  take links, name <links>
    like list-link-text
      like street-view-link
    lock free
  take location, name <location>
    like street-view-location
    lock free
  take tiles, name <tiles>
    like street-view-tile-data
    lock free

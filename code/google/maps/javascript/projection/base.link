
load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @drumwork/snow/code/javascript/google/maps/point
  take form point

form projection, name <google.maps.Projection>
  task from-lat-lng-to-point, name <fromLatLngToPoint>
    take lat-lng
      like sink-link-text
        like lat-lng
        like lat-lng-literal
    take point, like point
      void take
    free seed
      like sink-link-text
        like point
        like native-null

  task from-point-to-lat-lng, name <fromPointToLatLng>
    take pixel, like point
    take no-clamp-no-wrap, like native-boolean
      void take
    free seed
      like sink-link-text
        like lat-lng
        like native-null


load @nerdbond/land/code/javascript/base/native-string
  take form native-string

load @nerdbond/seed/code/form/list
  take form list
    save form list-link-text

load @nerdbond/snow/code/javascript/google/maps/places/prediction-substring
  take form prediction-substring

load @nerdbond/snow/code/javascript/google/maps/places/prediction-term
  take form prediction-term

form query-autocomplete-prediction, name <google.maps.places.QueryAutocompletePrediction>
  take description, name <description>
    like native-string
    lock free
  take matched-substrings, name <matched_substrings>
    like list-link-text
      like places-prediction-substring
    lock free
  take place-id, name <place_id>
    like native-string
    lock free
  take terms, name <terms>
    like list-link-text
      like places-prediction-term
    lock free

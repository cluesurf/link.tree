
load @textsurf/seed/code/form/list
  take form list
    save form list-link-text

load @textsurf/snow/code/javascript/google/maps/directions-geocoded-waypoint
  take form directions-geocoded-waypoint

load @textsurf/snow/code/javascript/google/maps/directions-route
  take form directions-route

load @textsurf/snow/code/javascript/google/maps/travel-mode
  take form travel-mode

form directions-result, name <google.maps.DirectionsResult>
  take available-travel-modes, name <available_travel_modes>
    like list-link-text
      like travel-mode
    lock free
  take geocoded-waypoints, name <geocoded_waypoints>
    like list-link-text
      like directions-geocoded-waypoint
    lock free
  take routes, name <routes>
    like list-link-text
      like directions-route
    lock free

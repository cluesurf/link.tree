
load @textsurf/land/code/javascript/base/element
  take form element

load @textsurf/land/code/javascript/base/native-null
  take form native-null

load @textsurf/land/code/javascript/base/native-number
  take form native-number

load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/land/code/javascript/base/native-undefined
  take form native-undefined

load @textsurf/land/code/javascript/base/native-void
  take form native-void

load @textsurf/land/code/javascript/base/text
  take form text

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @textsurf/snow/code/javascript/google/maps/info-window-open-options
  take form info-window-open-options

load @textsurf/snow/code/javascript/google/maps/info-window-options
  take form info-window-options

load @textsurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @textsurf/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @textsurf/snow/code/javascript/google/maps/map
  take form map

load @textsurf/snow/code/javascript/google/maps/mvc-object
  take form mvc-object

load @textsurf/snow/code/javascript/google/maps/street-view-panorama
  take form street-view-panorama

form info-window, name <google.maps.InfoWindow>
  base google-maps-mvc-object
  task close, name <close>
    free seed, like native-void

  task get-content, name <getContent>
    free seed
      like sink-link-text
        like native-string
        like element
        like native-null
        like text
        like native-undefined

  task get-position, name <getPosition>
    free seed
      like sink-link-text
        like lat-lng
        like native-null
        like native-undefined

  task get-z-index, name <getZIndex>
    free seed, like native-number

  task open, name <open>
    take options
      like sink-link-text
        like info-window-open-options
        like native-null
        like map
        like street-view-panorama
      void take
    take anchor
      like sink-link-text
        like mvc-object
        like native-null
      void take
    free seed, like native-void

  task set-content, name <setContent>
    take content
      like sink-link-text
        like native-string
        like element
        like native-null
        like text
      void take
    free seed, like native-void

  task set-options, name <setOptions>
    take options
      like sink-link-text
        like info-window-options
        like native-null
      void take
    free seed, like native-void

  task set-position, name <setPosition>
    take position
      like sink-link-text
        like lat-lng
        like native-null
        like lat-lng-literal
      void take
    free seed, like native-void

  task set-z-index, name <setZIndex>
    take z-index, like native-number
    free seed, like native-void

form info-window-constructor
  hook make
    take opts
      like sink-link-text
        like info-window-options
        like native-null
      void take


load @drumwork/land/code/javascript/base/native-any
  take form native-any

load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/snow/code/javascript/google/maps/map
  take form map

load @drumwork/snow/code/javascript/google/maps/mvc-array
  take form mvc-array

load @drumwork/snow/code/javascript/google/maps/visualization/heatmap-layer-options
  take form heatmap-layer-options

load @drumwork/snow/code/javascript/google/maps/visualization/weighted-location
  take form weighted-location

form heatmap-layer, name <google.maps.visualization.HeatmapLayer>
  base google-maps-mvc-object
  task get-data, name <getData>
    free seed
      like mvc-array
        like native-any

  task get-map, name <getMap>
    free seed
      like sink-link-text
        like map
        like native-undefined

  task set-data, name <setData>
    take data
      like sink-link-text
        like mvc-array
          like native-any
        like list-link-text
          like sink-link-text
            like lat-lng
            like visualization-weighted-location
    free seed, like native-void

  task set-map, name <setMap>
    take map
      like sink-link-text
        like map
        like native-null
    free seed, like native-void

  task set-options, name <setOptions>
    take options
      like sink-link-text
        like visualization-heatmap-layer-options
        like native-null
    free seed, like native-void

form heatmap-layer-constructor
  hook make
    take opts
      like sink-link-text
        like visualization-heatmap-layer-options
        like native-null
      void take


load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/kml-feature-data
  take form kml-feature-data

load @drumwork/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/snow/code/javascript/google/maps/size
  take form size

form kml-mouse-event, name <google.maps.KmlMouseEvent>
  take feature-data, name <featureData>
    like sink-link-text
      like kml-feature-data
      like native-null
    lock free
  take lat-lng, name <latLng>
    like sink-link-text
      like lat-lng
      like native-null
    lock free
  take pixel-offset, name <pixelOffset>
    like sink-link-text
      like size
      like native-null
    lock free

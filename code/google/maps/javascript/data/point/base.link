
load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

form point, name <google.maps.Data.Point>
  task for-each-lat-lng, name <forEachLatLng>
    take callback
      like task
        take a, like lat-lng
        free seed, like native-void
    free seed, like native-void

  task get, name <get>
    free seed, like lat-lng

  task get-type, name <getType>
    free seed, like native-string

form point-constructor
  hook make
    take lat-lng
      like sink-link-text
        like lat-lng
        like lat-lng-literal


load @nerdbond/land/code/javascript/base/element
  take form element

load @nerdbond/land/code/javascript/base/native-any
  take form native-any

load @nerdbond/land/code/javascript/base/native-null
  take form native-null

load @nerdbond/land/code/javascript/base/native-void
  take form native-void

load @nerdbond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @nerdbond/snow/code/javascript/google/maps/map
  take form map

load @nerdbond/snow/code/javascript/google/maps/map-canvas-projection
  take form map-canvas-projection

load @nerdbond/snow/code/javascript/google/maps/map-panes
  take form map-panes

load @nerdbond/snow/code/javascript/google/maps/street-view-panorama
  take form street-view-panorama

form overlay-view, name <google.maps.OverlayView>
  base google-maps-mvc-object
  task draw, name <draw>
    free seed, like native-void

  task get-map, name <getMap>
    free seed
      like sink-link-text
        like map
        like native-null
        like street-view-panorama

  task get-panes, name <getPanes>
    free seed
      like sink-link-text
        like map-panes
        like native-null

  task get-projection, name <getProjection>
    free seed, like map-canvas-projection

  task on-add, name <onAdd>
    free seed, like native-void

  task on-remove, name <onRemove>
    free seed, like native-void

  task set-map, name <setMap>
    take map
      like sink-link-text
        like map
        like native-null
        like street-view-panorama
    free seed, like native-void

form overlay-view-constructor
  task prevent-map-hits-and-gestures-from, name <preventMapHitsAndGesturesFrom>
    take this, like native-any
    take element, like element
    free seed, like native-void

  task prevent-map-hits-from, name <preventMapHitsFrom>
    take this, like native-any
    take element, like element
    free seed, like native-void

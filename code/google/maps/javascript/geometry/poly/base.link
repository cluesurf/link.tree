
load @textsurf/land/code/javascript/base/native-boolean
  take form native-boolean

load @textsurf/land/code/javascript/base/native-number
  take form native-number

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @textsurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @textsurf/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @textsurf/snow/code/javascript/google/maps/polygon
  take form polygon

load @textsurf/snow/code/javascript/google/maps/polyline
  take form polyline

task contains-location, name <containsLocation>
  take point
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take polygon, like polygon
  free seed, like native-boolean

task is-location-on-edge, name <isLocationOnEdge>
  take point
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take poly
    like sink-link-text
      like polygon
      like polyline
  take tolerance, like native-number
    void take
  free seed, like native-boolean


load @drumwork/land/code/javascript/base/native-any
  take form native-any

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @drumwork/snow/code/javascript/google/maps/mvc-array
  take form mvc-array

task decode-path, name <decodePath>
  take encoded-path, like native-string
  free seed
    like list-link-text
      like lat-lng

task encode-path, name <encodePath>
  take path
    like sink-link-text
      like list-link-text
        like sink-link-text
          like lat-lng
          like lat-lng-literal
      like mvc-array
        like native-any
  free seed, like native-string

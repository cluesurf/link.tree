
load @treesurf/land/code/javascript/base/native-any
  take form native-any

load @treesurf/land/code/javascript/base/native-null
  take form native-null

load @treesurf/land/code/javascript/base/native-number
  take form native-number

load @treesurf/seed/code/form/list
  take form list
    save form list-link-text

load @treesurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @treesurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @treesurf/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @treesurf/snow/code/javascript/google/maps/mvc-array
  take form mvc-array

task compute-area, name <computeArea>
  take path
    like sink-link-text
      like list-link-text
        like sink-link-text
          like lat-lng
          like lat-lng-literal
      like mvc-array
        like native-any
  take radius, like native-number
    void take
  free seed, like native-number

task compute-distance-between, name <computeDistanceBetween>
  take from
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take to
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take radius, like native-number
    void take
  free seed, like native-number

task compute-heading, name <computeHeading>
  take from
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take to
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  free seed, like native-number

task compute-length, name <computeLength>
  take path
    like sink-link-text
      like list-link-text
        like sink-link-text
          like lat-lng
          like lat-lng-literal
      like mvc-array
        like native-any
  take radius, like native-number
    void take
  free seed, like native-number

task compute-offset, name <computeOffset>
  take from
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take distance, like native-number
  take heading, like native-number
  take radius, like native-number
    void take
  free seed, like lat-lng

task compute-offset-origin, name <computeOffsetOrigin>
  take to
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take distance, like native-number
  take heading, like native-number
  take radius, like native-number
    void take
  free seed
    like sink-link-text
      like lat-lng
      like native-null

task compute-signed-area, name <computeSignedArea>
  take loop
    like sink-link-text
      like list-link-text
        like sink-link-text
          like lat-lng
          like lat-lng-literal
      like mvc-array
        like native-any
  take radius, like native-number
    void take
  free seed, like native-number

task interpolate, name <interpolate>
  take from
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take to
    like sink-link-text
      like lat-lng
      like lat-lng-literal
  take fraction, like native-number
  free seed, like lat-lng

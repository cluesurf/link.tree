
load @textsurf/land/code/javascript/base/native-null
  take form native-null

load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @textsurf/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

form street-view-location, name <google.maps.StreetViewLocation>
  take description, name <description>
    like sink-link-text
      like native-string
      like native-null
    lock free
  take lat-lng, name <latLng>
    like sink-link-text
      like lat-lng
      like native-null
    lock free
  take pano, name <pano>
    like native-string
    lock free
  take short-description, name <shortDescription>
    like sink-link-text
      like native-string
      like native-null
    lock free

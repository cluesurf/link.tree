
load @wavebond/land/code/javascript/base/float32-array
  take form float32-array

load @wavebond/land/code/javascript/base/float64-array
  take form float64-array

load @wavebond/land/code/javascript/base/native-number
  take form native-number

load @wavebond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @wavebond/snow/code/javascript/google/maps/camera-params
  take form camera-params

load @wavebond/snow/code/javascript/google/maps/lat-lng
  take form lat-lng

load @wavebond/snow/code/javascript/google/maps/lat-lng-altitude-literal
  take form lat-lng-altitude-literal

load @wavebond/snow/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

form coordinate-transformer, name <google.maps.CoordinateTransformer>
  task from-lat-lng-altitude, name <fromLatLngAltitude>
    take lat-lng-or-lat-lng-altitude
      like sink-link-text
        like lat-lng
        like lat-lng-literal
        like lat-lng-altitude-literal
    take altitude-or-rotations
      like sink-link-text
        like native-number
        like float32-array
      void take
    take rotations-or-scale, like float32-array
      void take
    take scale, like float32-array
      void take
    free seed, like float64-array

  task get-camera-params, name <getCameraParams>
    free seed, like camera-params

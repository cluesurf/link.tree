
load @textsurf/land/code/javascript/base/native-boolean
  take form native-boolean

load @textsurf/land/code/javascript/base/native-null
  take form native-null

load @textsurf/land/code/javascript/base/native-string
  take form native-string

load @textsurf/seed/code/form/sink
  take form sink
    save form sink-link-text

load @textsurf/snow/code/javascript/google/maps/kml-author
  take form kml-author

form kml-layer-metadata, name <google.maps.KmlLayerMetadata>
  take author, name <author>
    like sink-link-text
      like kml-author
      like native-null
    lock free
  take description, name <description>
    like native-string
    lock free
  take has-screen-overlays, name <hasScreenOverlays>
    like native-boolean
    lock free
  take name, name <name>
    like native-string
    lock free
  take snippet, name <snippet>
    like native-string
    lock free

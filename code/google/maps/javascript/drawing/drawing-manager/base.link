
load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/snow/code/javascript/google/maps/drawing/drawing-manager-options
  take form drawing-manager-options

load @drumwork/snow/code/javascript/google/maps/drawing/overlay-type
  take form overlay-type

load @drumwork/snow/code/javascript/google/maps/map
  take form map

form drawing-manager, name <google.maps.drawing.DrawingManager>
  base google-maps-mvc-object
  task get-drawing-mode, name <getDrawingMode>
    free seed
      like sink-link-text
        like drawing-overlay-type
        like native-null

  task get-map, name <getMap>
    free seed
      like sink-link-text
        like map
        like native-null

  task set-drawing-mode, name <setDrawingMode>
    take drawing-mode
      like sink-link-text
        like drawing-overlay-type
        like native-null
    free seed, like native-void

  task set-map, name <setMap>
    take map
      like sink-link-text
        like map
        like native-null
    free seed, like native-void

  task set-options, name <setOptions>
    take options
      like sink-link-text
        like drawing-drawing-manager-options
        like native-null
    free seed, like native-void

form drawing-manager-constructor
  hook make
    take options
      like sink-link-text
        like drawing-drawing-manager-options
        like native-null
      void take

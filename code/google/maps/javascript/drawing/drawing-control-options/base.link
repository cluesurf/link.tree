
load @nerdbond/land/code/javascript/base/native-null
  take form native-null

load @nerdbond/seed/code/form/list
  take form list
    save form list-link-text

load @nerdbond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @nerdbond/snow/code/javascript/google/maps/control-position
  take form control-position

load @nerdbond/snow/code/javascript/google/maps/drawing/overlay-type
  take form overlay-type

form drawing-control-options, name <google.maps.drawing.DrawingControlOptions>
  take drawing-modes, name <drawingModes>
    like sink-link-text
      like list-link-text
        like drawing-overlay-type
      like native-null
    lock free
  take position, name <position>
    like sink-link-text
      like control-position
      like native-null
    lock free

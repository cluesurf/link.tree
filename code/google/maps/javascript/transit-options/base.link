
load @wavebond/land/code/javascript/base/date
  take form date

load @wavebond/land/code/javascript/base/native-null
  take form native-null

load @wavebond/seed/code/form/list
  take form list
    save form list-link-text

load @wavebond/seed/code/form/sink
  take form sink
    save form sink-link-text

load @wavebond/snow/code/javascript/google/maps/transit-mode
  take form transit-mode

load @wavebond/snow/code/javascript/google/maps/transit-route-preference
  take form transit-route-preference

form transit-options, name <google.maps.TransitOptions>
  take arrival-time, name <arrivalTime>
    like sink-link-text
      like date
      like native-null
    lock free
  take departure-time, name <departureTime>
    like sink-link-text
      like date
      like native-null
    lock free
  take modes, name <modes>
    like sink-link-text
      like list-link-text
        like transit-mode
      like native-null
    lock free
  take routing-preference, name <routingPreference>
    like sink-link-text
      like transit-route-preference
      like native-null
    lock free

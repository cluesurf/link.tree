
load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/googleapis.com/androidpublisher/v3/base/expansion-files-upload-response
  take form expansion-files-upload-response

boot androidpublisher-edits-expansionfiles-upload, name <androidpublisher.edits.expansionfiles.upload>


load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/googleapis.com/dfareporting/v3.5/base/file-list
  take form file-list

boot dfareporting-files-list, name <dfareporting.files.list>
  deed get
  note <Lists files for a user profile.>

  take profile-id
    like native-string
    note <The Campaign Manager 360 user profile ID.>
  take max-results
    like native-integer
    note <Maximum number of results to return.>
    void take
  take page-token
    like native-string
    note <The value of the nextToken from the previous result page.>
    void take
  take scope
    like native-string
    note <The scope that defines which results are returned.>
    void take
  take sort-field
    like native-string
    note <The field by which to sort the list.>
    void take
  take sort-order
    like native-string
    note <Order of sorted results.>
    void take

  line </dfareporting/v3.5/userprofiles/{profile-id}/files>

  find <maxResults>, loan max-results
  find <pageToken>, loan page-token
  find <scope>, loan scope
  find <sortField>, loan sort-field
  find <sortOrder>, loan sort-order

  loot 200
    seed json, like file-list
    note <Successful response>
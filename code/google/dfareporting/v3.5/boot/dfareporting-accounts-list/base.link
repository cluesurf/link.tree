
load @nerdbond/snow/base/list
  take form list

load @nerdbond/snow/base/native-boolean
  take form native-boolean

load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

load @nerdbond/snow/googleapis.com/dfareporting/v3.5/base/accounts-list-response
  take form accounts-list-response

boot dfareporting-accounts-list, name <dfareporting.accounts.list>
  deed get
  note <Retrieves the list of accounts, possibly filtered. This method supports paging.>

  take profile-id
    like native-string
    note <User profile ID associated with this request.>
  take active
    like native-boolean
    note <Select only active accounts. Don't set this field to select both active and non-active accounts.>
    void take
  take ids
    like list
      like native-string
    note <Select only accounts with these IDs.>
    void take
  take max-results
    like native-integer
    note <Maximum number of results to return.>
    void take
  take page-token
    like native-string
    note <Value of the nextPageToken from the previous result page.>
    void take
  take search-string
    like native-string
    note <Allows searching for objects by name or ID. Wildcards (*) are allowed. For example, "account*2015" will return objects with names like "account June 2015", "account April 2015", or simply "account 2015". Most of the searches also add wildcards implicitly at the start and the end of the search string. For example, a search string of "account" will match objects with name "my account", "account 2015", or simply "account".>
    void take
  take sort-field
    like native-string
    note <Field by which to sort the list.>
    void take
  take sort-order
    like native-string
    note <Order of sorted results.>
    void take

  line </dfareporting/v3.5/userprofiles/{profile-id}/accounts>

  find <active>, loan active
  find <ids>, loan ids
  find <maxResults>, loan max-results
  find <pageToken>, loan page-token
  find <searchString>, loan search-string
  find <sortField>, loan sort-field
  find <sortOrder>, loan sort-order

  loot 200
    seed json, like accounts-list-response
    note <Successful response>
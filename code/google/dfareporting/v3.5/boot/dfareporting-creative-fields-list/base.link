
load @drumwork/snow/base/list
  take form list

load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/googleapis.com/dfareporting/v3.5/base/creative-fields-list-response
  take form creative-fields-list-response

boot dfareporting-creative-fields-list, name <dfareporting.creativeFields.list>
  deed get
  note <Retrieves a list of creative fields, possibly filtered. This method supports paging.>

  take profile-id
    like native-string
    note <User profile ID associated with this request.>
  take advertiser-ids
    like list
      like native-string
    note <Select only creative fields that belong to these advertisers.>
    void take
  take ids
    like list
      like native-string
    note <Select only creative fields with these IDs.>
    void take
  take max-results
    like native-integer
    note <Maximum number of results to return.>
    void take
  take page-token
    like native-string
    note <Value of the nextPageToken from the previous result page.>
    void take
  take search-string
    like native-string
    note <Allows searching for creative fields by name or ID. Wildcards (*) are allowed. For example, "creativefield*2015" will return creative fields with names like "creativefield June 2015", "creativefield April 2015", or simply "creativefield 2015". Most of the searches also add wild-cards implicitly at the start and the end of the search string. For example, a search string of "creativefield" will match creative fields with the name "my creativefield", "creativefield 2015", or simply "creativefield".>
    void take
  take sort-field
    like native-string
    note <Field by which to sort the list.>
    void take
  take sort-order
    like native-string
    note <Order of sorted results.>
    void take

  line </dfareporting/v3.5/userprofiles/{profile-id}/creativeFields>

  find <advertiserIds>, loan advertiser-ids
  find <ids>, loan ids
  find <maxResults>, loan max-results
  find <pageToken>, loan page-token
  find <searchString>, loan search-string
  find <sortField>, loan sort-field
  find <sortOrder>, loan sort-order

  loot 200
    seed json, like creative-fields-list-response
    note <Successful response>

load @drumwork/snow/base/list
  take form list

load @drumwork/snow/base/native-boolean
  take form native-boolean

load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/googleapis.com/blogger/v3/base/page-list
  take form page-list

boot blogger-pages-list, name <blogger.pages.list>
  deed get
  note <Lists pages.>

  take blog-id
    like native-string
  take fetch-bodies
    like native-boolean
    void take
  take max-results
    like native-integer
    void take
  take page-token
    like native-string
    void take
  take status
    like list
      like native-string
    void take
  take view
    like native-string
    void take

  line </v3/blogs/{blog-id}/pages>

  find <fetchBodies>, loan fetch-bodies
  find <maxResults>, loan max-results
  find <pageToken>, loan page-token
  find <status>, loan status
  find <view>, loan view

  loot 200
    seed json, like page-list
    note <Successful response>
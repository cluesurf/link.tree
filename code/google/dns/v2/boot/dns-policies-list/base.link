
load @treesurf/snow/base/native-integer
  take form native-integer

load @treesurf/snow/base/native-string
  take form native-string

load @treesurf/snow/googleapis.com/dns/v2/base/policies-list-response
  take form policies-list-response

boot dns-policies-list, name <dns.policies.list>
  deed get
  note <Enumerates all Policies associated with a project.>

  take project
    like native-string
    note <Identifies the project addressed by this request.>
  take location
    like native-string
    note <Specifies the location of the resource. This information will be used for routing and will be part of the resource name.>
  take max-results
    like native-integer
    note <Optional. Maximum number of results to be returned. If unspecified, the server decides how many results to return.>
    void take
  take page-token
    like native-string
    note <Optional. A tag returned by a previous list request that was truncated. Use this parameter to continue a previous list request.>
    void take

  line </dns/v2/projects/{project}/locations/{location}/policies>

  find <maxResults>, loan max-results
  find <pageToken>, loan page-token

  loot 200
    seed json, like policies-list-response
    note <Successful response>

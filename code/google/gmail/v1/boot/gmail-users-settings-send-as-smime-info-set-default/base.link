
load @treesurf/snow/base/native-string
  take form native-string

boot gmail-users-settings-send-as-smime-info-set-default, name <gmail.users.settings.sendAs.smimeInfo.setDefault>
  deed post
  note <Sets the default S/MIME config for the specified send-as alias.>

  take user-id
    like native-string
    note <The user's email address. The special value `me` can be used to indicate the authenticated user.>
  take send-as-email
    like native-string
    note <The email address that appears in the "From:" header for mail sent using this alias.>
  take id
    like native-string
    note <The immutable ID for the SmimeInfo.>

  line </gmail/v1/users/{user-id}/settings/sendAs/{send-as-email}/smimeInfo/{id}/setDefault>

  loot 200
    note <Successful response>

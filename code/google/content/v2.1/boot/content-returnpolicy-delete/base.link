
load @tunebond/snow/base/native-string
  take form native-string

boot content-returnpolicy-delete, name <content.returnpolicy.delete>
  deed delete
  note <Deletes a return policy for the given Merchant Center account.>

  take merchant-id
    like native-string
    note <The Merchant Center account from which to delete the given return policy.>
  take return-policy-id
    like native-string
    note <Return policy ID generated by Google.>

  line </{merchant-id}/returnpolicy/{return-policy-id}>

  loot 200
    note <Successful response>

load @wavebond/snow/base/native-file
  take form native-file

load @wavebond/snow/base/native-string
  take form native-string

boot upload-brand-theme-logo, name <uploadBrandThemeLogo>
  deed post
  note <Updates the logo for your Theme>

  take brand-id
    like native-string
  take theme-id
    like native-string
  take file
    like native-file

  line </api/v1/brands/{brand-id}/themes/{theme-id}/logo>

  file file

  loot 200
    note <Success>
  loot 400
    note <Bad Request>
  loot 404
    note <Not Found>


load @nerdbond/snow/base/native-file
  take form native-file

load @nerdbond/snow/base/native-string
  take form native-string

boot upload-brand-theme-favicon, name <uploadBrandThemeFavicon>
  deed post
  note <Updates the favicon for your theme>

  take brand-id
    like native-string
  take theme-id
    like native-string
  take file
    like native-file

  line </api/v1/brands/{brand-id}/themes/{theme-id}/favicon>

  file file

  loot 201
    note <Created>
  loot 400
    note <Bad Request>
  loot 404
    note <Not Found>

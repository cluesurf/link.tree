
load @tunebond/snow/amazonaws.com/transcribe/2017-10-26/base/bad-request-exception
  take form bad-request-exception

load @tunebond/snow/amazonaws.com/transcribe/2017-10-26/base/conflict-exception
  take form conflict-exception

load @tunebond/snow/amazonaws.com/transcribe/2017-10-26/base/create-language-model-response
  take form create-language-model-response

load @tunebond/snow/amazonaws.com/transcribe/2017-10-26/base/internal-failure-exception
  take form internal-failure-exception

load @tunebond/snow/amazonaws.com/transcribe/2017-10-26/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @tunebond/snow/base/native-string
  take form native-string

boot create-language-model, name <CreateLanguageModel>
  deed post
  note <Creates a new custom language model. Use Amazon S3 prefixes to provide the location of your input files. The time it takes to create your model depends on the size of your training data.>

  take x-amz-target
    like native-string
  take create-language-model-request, like create-language-model-request

  line </#X-Amz-Target=Transcribe.CreateLanguageModel>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan create-language-model-request

  loot 200
    seed json, like create-language-model-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
  loot 482
    seed json, like internal-failure-exception
    note <InternalFailureException>
  loot 483
    seed json, like conflict-exception
    note <ConflictException>

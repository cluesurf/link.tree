
load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/update-assessment-control-body
  take form update-assessment-control-body

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/update-assessment-control-response
  take form update-assessment-control-response

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot update-assessment-control, name <UpdateAssessmentControl>
  deed put
  note <Updates a control within an assessment in Audit Manager.>

  take assessment-id
    like native-string
    note <The identifier for the specified assessment.>
  take control-set-id
    like native-string
    note <The identifier for the specified control set.>
  take control-id
    like native-string
    note <The identifier for the specified control.>
  take update-assessment-control-body, like update-assessment-control-body

  line </assessments/{assessment-id}/controlSets/{control-set-id}/controls/{control-id}>

  seed json, loan update-assessment-control-body

  loot 200
    seed json, like update-assessment-control-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like internal-server-exception
    note <InternalServerException>
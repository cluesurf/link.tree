
load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/associate-assessment-report-evidence-folder-body
  take form associate-assessment-report-evidence-folder-body

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/associate-assessment-report-evidence-folder-response
  take form associate-assessment-report-evidence-folder-response

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/auditmanager/2017-07-25/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot associate-assessment-report-evidence-folder, name <AssociateAssessmentReportEvidenceFolder>
  deed put
  note <Associates an evidence folder to the specified assessment report in Audit Manager.>

  take assessment-id
    like native-string
    note <The identifier for the specified assessment.>
  take associate-assessment-report-evidence-folder-body, like associate-assessment-report-evidence-folder-body

  line </assessments/{assessment-id}/associateToAssessmentReport>

  seed json, loan associate-assessment-report-evidence-folder-body

  loot 200
    seed json, like associate-assessment-report-evidence-folder-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/access-denied-exception
  take form access-denied-exception

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/create-assessment-report-body
  take form create-assessment-report-body

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/create-assessment-report-response
  take form create-assessment-report-response

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/internal-server-exception
  take form internal-server-exception

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/auditmanager/2017-07-25/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-string
  take form native-string

boot create-assessment-report, name <CreateAssessmentReport>
  deed post
  note <Creates an assessment report for the specified assessment.>

  take assessment-id
    like native-string
    note <The identifier for the specified assessment.>
  take create-assessment-report-body, like create-assessment-report-body

  line </assessments/{assessment-id}/reports>

  seed json, loan create-assessment-report-body

  loot 200
    seed json, like create-assessment-report-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
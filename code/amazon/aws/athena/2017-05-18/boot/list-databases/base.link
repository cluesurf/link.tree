
load @nerdbond/snow/amazonaws.com/athena/2017-05-18/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/athena/2017-05-18/base/invalid-request-exception
  take form invalid-request-exception

load @nerdbond/snow/amazonaws.com/athena/2017-05-18/base/list-databases-output
  take form list-databases-output

load @nerdbond/snow/amazonaws.com/athena/2017-05-18/base/metadata-exception
  take form metadata-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot list-databases, name <ListDatabases>
  deed post
  note <Lists the databases in the specified data catalog.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-databases-input, like list-databases-input

  line </#X-Amz-Target=AmazonAthena.ListDatabases>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-databases-input

  loot 200
    seed json, like list-databases-output
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like invalid-request-exception
    note <InvalidRequestException>
  loot 482
    seed json, like metadata-exception
    note <MetadataException>

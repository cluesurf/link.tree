
load @wavebond/snow/amazonaws.com/athena/2017-05-18/base/internal-server-exception
  take form internal-server-exception

load @wavebond/snow/amazonaws.com/athena/2017-05-18/base/invalid-request-exception
  take form invalid-request-exception

load @wavebond/snow/amazonaws.com/athena/2017-05-18/base/list-prepared-statements-output
  take form list-prepared-statements-output

load @wavebond/snow/base/native-string
  take form native-string

boot list-prepared-statements, name <ListPreparedStatements>
  deed post
  note <Lists the prepared statements in the specfied workgroup.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-prepared-statements-input, like list-prepared-statements-input

  line </#X-Amz-Target=AmazonAthena.ListPreparedStatements>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-prepared-statements-input

  loot 200
    seed json, like list-prepared-statements-output
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like invalid-request-exception
    note <InvalidRequestException>

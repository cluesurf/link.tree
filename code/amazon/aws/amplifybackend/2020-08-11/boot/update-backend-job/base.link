
load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/bad-request-exception
  take form bad-request-exception

load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/gateway-timeout-exception
  take form gateway-timeout-exception

load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/not-found-exception
  take form not-found-exception

load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/too-many-requests-exception
  take form too-many-requests-exception

load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/update-backend-job-body
  take form update-backend-job-body

load @tunebond/snow/amazonaws.com/amplifybackend/2020-08-11/base/update-backend-job-response
  take form update-backend-job-response

load @tunebond/snow/base/native-string
  take form native-string

boot update-backend-job, name <UpdateBackendJob>
  deed post
  note <Updates a specific job.>

  take app-id
    like native-string
    note <The app ID.>
  take backend-environment-name
    like native-string
    note <The name of the backend environment.>
  take job-id
    like native-string
    note <The ID for the job.>
  take update-backend-job-body, like update-backend-job-body

  line </backend/{app-id}/job/{backend-environment-name}/{job-id}>

  seed json, loan update-backend-job-body

  loot 200
    seed json, like update-backend-job-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like gateway-timeout-exception
    note <GatewayTimeoutException>
  loot 482
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>

load @nerdbond/snow/amazonaws.com/amplifybackend/2020-08-11/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/amplifybackend/2020-08-11/base/gateway-timeout-exception
  take form gateway-timeout-exception

load @nerdbond/snow/amazonaws.com/amplifybackend/2020-08-11/base/generate-backend-api-models-body
  take form generate-backend-api-models-body

load @nerdbond/snow/amazonaws.com/amplifybackend/2020-08-11/base/generate-backend-api-models-response
  take form generate-backend-api-models-response

load @nerdbond/snow/amazonaws.com/amplifybackend/2020-08-11/base/not-found-exception
  take form not-found-exception


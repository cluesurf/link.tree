load @nerdbond/snow/base/native-string
  take form native-string

boot generate-backend-api-models, name <GenerateBackendAPIModels>
  deed post
  note <Generates a model schema for an existing backend API resource.>

  take app-id
    like native-string
    note <The app ID.>
  take backend-environment-name
    like native-string
    note <The name of the backend environment.>
  take generate-backend-api-models-body, like generate-backend-api-models-body

  line </backend/{app-id}/api/{backend-environment-name}/generateModels>

  seed json, loan generate-backend-api-models-body

  loot 200
    seed json, like generate-backend-api-models-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like gateway-timeout-exception
    note <GatewayTimeoutException>
  loot 482
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/bad-request-exception
  take form bad-request-exception

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/gateway-timeout-exception
  take form gateway-timeout-exception

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/list-backend-jobs-body
  take form list-backend-jobs-body

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/list-backend-jobs-response
  take form list-backend-jobs-response

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/not-found-exception
  take form not-found-exception

load @drumwork/snow/amazonaws.com/amplifybackend/2020-08-11/base/too-many-requests-exception
  take form too-many-requests-exception

load @drumwork/snow/base/native-string
  take form native-string

boot list-backend-jobs, name <ListBackendJobs>
  note <Lists the jobs for the backend of an Amplify app.>

  take app-id
    like native-string
    note <The app ID.>
  take backend-environment-name
    like native-string
    note <The name of the backend environment.>
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-backend-jobs-body, like list-backend-jobs-body

  line </backend/{app-id}/job/{backend-environment-name}>

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-backend-jobs-body

  loot 200
    seed json, like list-backend-jobs-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like gateway-timeout-exception
    note <GatewayTimeoutException>
  loot 482
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>
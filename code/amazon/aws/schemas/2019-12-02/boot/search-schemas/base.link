
load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/forbidden-exception
  take form forbidden-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/internal-server-error-exception
  take form internal-server-error-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/search-schemas-response
  take form search-schemas-response

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/service-unavailable-exception
  take form service-unavailable-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/unauthorized-exception
  take form unauthorized-exception

load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

boot search-schemas, name <SearchSchemas>
  deed get
  note <Search the schemas>

  take registry-name
    like native-string
    note <The name of the registry.>
  take keywords
    like native-string
    note <Specifying this limits the results to only schemas that include the provided keywords.>
  take limit
    like native-integer
    void take
  take next-token
    like native-string
    note <The token that specifies the next page of results to return. To request the first page, leave NextToken empty. The token will expire in 24 hours, and cannot be shared with other accounts.>
    void take
  take limit
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </v1/registries/name/{registry-name}/schemas/search#keywords>

  find <keywords>, loan keywords
  find <limit>, loan limit
  find <nextToken>, loan next-token
  find <Limit>, loan limit
  find <NextToken>, loan next-token

  loot 200
    seed json, like search-schemas-response
    note <Success>
  loot 480
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 481
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 482
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 483
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 484
    seed json, like forbidden-exception
    note <ForbiddenException>

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/forbidden-exception
  take form forbidden-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/internal-server-error-exception
  take form internal-server-error-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/service-unavailable-exception
  take form service-unavailable-exception

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/update-schema-body
  take form update-schema-body

load @nerdbond/snow/amazonaws.com/schemas/2019-12-02/base/update-schema-response
  take form update-schema-response

load @nerdbond/snow/base/native-string
  take form native-string

boot update-schema, name <UpdateSchema>
  deed put
  note <Updates the schema definition

Inactive schemas will be deleted after two years.>

  take registry-name
    like native-string
    note <The name of the registry.>
  take schema-name
    like native-string
    note <The name of the schema.>
  take update-schema-body, like update-schema-body

  line </v1/registries/name/{registry-name}/schemas/name/{schema-name}>

  seed json, loan update-schema-body

  loot 200
    seed json, like update-schema-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 482
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 483
    seed json, like not-found-exception
    note <NotFoundException>
  loot 484
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
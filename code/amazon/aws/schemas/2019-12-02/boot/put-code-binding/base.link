boot put-code-binding, name <PutCodeBinding>
  deed post
  note <Put code binding URI>

  take language
    like native-string
    note <The language of the code binding.>
  take registry-name
    like native-string
    note <The name of the registry.>
  take schema-name
    like native-string
    note <The name of the schema.>
  take schema-version
    like native-string
    note <Specifying this limits the results to only this schema version.>
    void take

  line </v1/registries/name/{registry-name}/schemas/name/{schema-name}/language/{language}>

  find <schemaVersion>, loan schema-version

  loot 202
    seed json, like put-code-binding-response
    note <Success>
  loot 480
    seed json, like gone-exception
    note <GoneException>
  loot 481
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 482
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 483
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 484
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 485
    seed json, like not-found-exception
    note <NotFoundException>
  loot 486
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
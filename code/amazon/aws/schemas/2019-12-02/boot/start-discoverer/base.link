
load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/bad-request-exception
  take form bad-request-exception

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/forbidden-exception
  take form forbidden-exception

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/internal-server-error-exception
  take form internal-server-error-exception

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/not-found-exception
  take form not-found-exception

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/service-unavailable-exception
  take form service-unavailable-exception

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/start-discoverer-response
  take form start-discoverer-response

load @treesurf/snow/amazonaws.com/schemas/2019-12-02/base/unauthorized-exception
  take form unauthorized-exception

load @treesurf/snow/base/native-string
  take form native-string

boot start-discoverer, name <StartDiscoverer>
  deed post
  note <Starts the discoverer>

  take discoverer-id
    like native-string
    note <The ID of the discoverer.>

  line </v1/discoverers/id/{discoverer-id}/start>

  loot 200
    seed json, like start-discoverer-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 483
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 484
    seed json, like not-found-exception
    note <NotFoundException>
  loot 485
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
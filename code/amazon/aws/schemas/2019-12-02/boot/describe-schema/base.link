
load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/bad-request-exception
  take form bad-request-exception

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/describe-schema-response
  take form describe-schema-response

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/forbidden-exception
  take form forbidden-exception

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/internal-server-error-exception
  take form internal-server-error-exception

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/not-found-exception
  take form not-found-exception

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/service-unavailable-exception
  take form service-unavailable-exception

load @tunebond/snow/amazonaws.com/schemas/2019-12-02/base/unauthorized-exception
  take form unauthorized-exception

load @tunebond/snow/base/native-string
  take form native-string

boot describe-schema, name <DescribeSchema>
  deed get
  note <Retrieve the schema definition.>

  take registry-name
    like native-string
    note <The name of the registry.>
  take schema-name
    like native-string
    note <The name of the schema.>
  take schema-version
    like native-string
    note <Specifying this limits the results to only this schema version.>
    void take

  line </v1/registries/name/{registry-name}/schemas/name/{schema-name}>

  find <schemaVersion>, loan schema-version

  loot 200
    seed json, like describe-schema-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 483
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 484
    seed json, like not-found-exception
    note <NotFoundException>
  loot 485
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>

load @textsurf/snow/amazonaws.com/schemas/2019-12-02/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/schemas/2019-12-02/base/create-schema-body
  take form create-schema-body

load @textsurf/snow/amazonaws.com/schemas/2019-12-02/base/create-schema-response
  take form create-schema-response

load @textsurf/snow/amazonaws.com/schemas/2019-12-02/base/forbidden-exception
  take form forbidden-exception

load @textsurf/snow/amazonaws.com/schemas/2019-12-02/base/internal-server-error-exception
  take form internal-server-error-exception


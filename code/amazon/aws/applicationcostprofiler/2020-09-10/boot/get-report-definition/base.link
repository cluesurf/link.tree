
load @textsurf/snow/amazonaws.com/applicationcostprofiler/2020-09-10/base/access-denied-exception
  take form access-denied-exception

load @textsurf/snow/amazonaws.com/applicationcostprofiler/2020-09-10/base/get-report-definition-result
  take form get-report-definition-result

load @textsurf/snow/amazonaws.com/applicationcostprofiler/2020-09-10/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/applicationcostprofiler/2020-09-10/base/throttling-exception
  take form throttling-exception

load @textsurf/snow/amazonaws.com/applicationcostprofiler/2020-09-10/base/validation-exception
  take form validation-exception

load @textsurf/snow/base/native-string
  take form native-string

boot get-report-definition, name <GetReportDefinition>
  deed get
  note <Retrieves the definition of a report already configured in AWS Application Cost Profiler.>

  take report-id
    like native-string
    note <ID of the report to retrieve.>

  line </reportDefinition/{report-id}>

  loot 200
    seed json, like get-report-definition-result
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 482
    seed json, like validation-exception
    note <ValidationException>
  loot 483
    seed json, like access-denied-exception
    note <AccessDeniedException>
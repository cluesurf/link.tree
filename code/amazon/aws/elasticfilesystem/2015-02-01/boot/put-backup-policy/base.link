
load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/backup-policy-description
  take form backup-policy-description

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/bad-request
  take form bad-request

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/file-system-not-found
  take form file-system-not-found

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/incorrect-file-system-life-cycle-state
  take form incorrect-file-system-life-cycle-state

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/internal-server-error
  take form internal-server-error

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/put-backup-policy-body
  take form put-backup-policy-body

load @wavebond/snow/amazonaws.com/elasticfilesystem/2015-02-01/base/validation-exception
  take form validation-exception

load @wavebond/snow/base/native-string
  take form native-string

boot put-backup-policy, name <PutBackupPolicy>
  deed put
  note <Updates the file system's backup policy. Use this action to start or stop automatic backups of the file system.>

  take file-system-id
    like native-string
    note <Specifies which EFS file system to update the backup policy for.>
  take put-backup-policy-body, like put-backup-policy-body

  line </2015-02-01/file-systems/{file-system-id}/backup-policy>

  seed json, loan put-backup-policy-body

  loot 200
    seed json, like backup-policy-description
    note <Success>
  loot 480
    seed json, like bad-request
    note <BadRequest>
  loot 481
    seed json, like file-system-not-found
    note <FileSystemNotFound>
  loot 482
    seed json, like incorrect-file-system-life-cycle-state
    note <IncorrectFileSystemLifeCycleState>
  loot 483
    seed json, like internal-server-error
    note <InternalServerError>
  loot 484
    seed json, like validation-exception
    note <ValidationException>
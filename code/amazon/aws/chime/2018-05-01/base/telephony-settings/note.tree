
load @termsurf/snow/base/boolean
  take form boolean

form telephony-settings, name <TelephonySettings>
  note <Settings that allow management of telephony permissions for an Amazon Chime user, such as inbound and outbound calling and text messaging.>
  take inbound-calling, name <InboundCalling>
    like boolean
    note <Allows or denies inbound calling.>
  take outbound-calling, name <OutboundCalling>
    like boolean
    note <Allows or denies outbound calling.>
  take sms, name <SMS>
    like boolean
    note <Allows or denies SMS messaging.>
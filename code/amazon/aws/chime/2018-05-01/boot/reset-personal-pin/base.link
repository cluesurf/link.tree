load @treesurf/snow/base/native-string
  take form native-string

boot reset-personal-pin, name <ResetPersonalPIN>
  deed post
  note <Resets the personal meeting PIN for the specified user on an Amazon Chime account. Returns the User object with the updated personal meeting PIN.>

  take account-id
    like native-string
    note <The Amazon Chime account ID.>
  take user-id
    like native-string
    note <The user ID.>
  take operation
    like native-string

  line </accounts/{account-id}/users/{user-id}#operation=reset-personal-pin>

  find <operation>, loan operation

  loot 200
    seed json, like reset-personal-pin-response
    note <Success>
  loot 480
    seed json, like unauthorized-client-exception
    note <UnauthorizedClientException>
  loot 481
    seed json, like not-found-exception
    note <NotFoundException>
  loot 482
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 484
    seed json, like throttled-client-exception
    note <ThrottledClientException>
  loot 485
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 486
    seed json, like service-failure-exception
    note <ServiceFailureException>
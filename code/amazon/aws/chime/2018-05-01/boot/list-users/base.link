boot list-users, name <ListUsers>
  note <Lists the users that belong to the specified Amazon Chime account. You can specify an email address to list only the user that the email address belongs to.>

  take account-id
    like native-string
    note <The Amazon Chime account ID.>
  take user-email
    like native-string
    note <Optional. The user email address used to filter results. Maximum 1.>
    void take
  take user-type
    like native-string
    note <The user type.>
    void take
  take max-results
    like native-integer
    note <The maximum number of results to return in a single call. Defaults to 100.>
    void take
  take next-token
    like native-string
    note <The token to use to retrieve the next page of results.>
    void take
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </accounts/{account-id}/users>

  find <user-email>, loan user-email
  find <user-type>, loan user-type
  find <max-results>, loan max-results
  find <next-token>, loan next-token
  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-users-response
    note <Success>
  loot 480

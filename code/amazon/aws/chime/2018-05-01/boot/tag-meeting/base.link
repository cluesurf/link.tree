boot tag-meeting, name <TagMeeting>
  note <Applies the specified tags to the specified Amazon Chime SDK meeting.>

  take meeting-id
    like native-string
    note <The Amazon Chime SDK meeting ID.>
  take operation
    like native-string
  take tag-meeting-body, like tag-meeting-body

  line </meetings/{meeting-id}/tags#operation=add>

  find <operation>, loan operation

  seed json, loan tag-meeting-body

  loot 204
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 482

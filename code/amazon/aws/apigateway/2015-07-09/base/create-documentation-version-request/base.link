
load @drumwork/snow/base/string
  take form string

form create-documentation-version-request, name <CreateDocumentationVersionRequest>
  note <Creates a new documentation version of a given API.>
  take documentation-version, name <documentationVersion>
    like string
    note <[Required] The version identifier of the new snapshot.>
  take stage-name, name <stageName>
    like string
    void take
    note <The stage name to be associated with the new documentation snapshot.>
  take description, name <description>
    like string
    void take
    note <A description about the new documentation snapshot.>
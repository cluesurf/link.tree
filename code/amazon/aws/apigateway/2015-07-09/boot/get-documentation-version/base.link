
load @nerdbond/snow/amazonaws.com/apigateway/2015-07-09/base/documentation-version
  take form documentation-version

load @nerdbond/snow/amazonaws.com/apigateway/2015-07-09/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/apigateway/2015-07-09/base/too-many-requests-exception
  take form too-many-requests-exception

load @nerdbond/snow/amazonaws.com/apigateway/2015-07-09/base/unauthorized-exception
  take form unauthorized-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-documentation-version, name <GetDocumentationVersion>
  deed get
  take restapi-id
    like native-string
    note <[Required] The string identifier of the associated RestApi.>
  take doc-version
    like native-string
    note <[Required] The version identifier of the to-be-retrieved documentation snapshot.>

  line </restapis/{restapi-id}/documentation/versions/{doc-version}>

  loot 200
    seed json, like documentation-version
    note <Success>
  loot 480
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 481
    seed json, like not-found-exception
    note <NotFoundException>
  loot 482
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>

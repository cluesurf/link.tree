boot get-integration-response, name <GetIntegrationResponse>
  note <Represents a get integration response.>

  take restapi-id
    like native-string
    note <[Required] The string identifier of the associated RestApi.>
  take resource-id
    like native-string
    note <[Required] Specifies a get integration response request's resource identifier.>
  take http-method
    like native-string
    note <[Required] Specifies a get integration response request's HTTP method.>
  take status-code
    like native-string
    note <[Required] Specifies a get integration response request's status code.>

  line </restapis/{restapi-id}/resources/{resource-id}/methods/{http-method}/integration/responses/{status-code}>

  loot 200
    seed json, like integration-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like not-found-exception
    note <NotFoundException>
  loot 482
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 483
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
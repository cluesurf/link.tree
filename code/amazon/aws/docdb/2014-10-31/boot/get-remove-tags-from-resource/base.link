
load @textsurf/snow/amazonaws.com/docdb/2014-10-31/base/db-cluster-not-found-fault
  take form db-cluster-not-found-fault

load @textsurf/snow/amazonaws.com/docdb/2014-10-31/base/db-instance-not-found-fault
  take form db-instance-not-found-fault

load @textsurf/snow/amazonaws.com/docdb/2014-10-31/base/db-snapshot-not-found-fault
  take form db-snapshot-not-found-fault

load @textsurf/snow/base/list
  take form list

load @textsurf/snow/base/native-string
  take form native-string

load @textsurf/snow/base/string
  take form string

boot get-remove-tags-from-resource, name <GET_RemoveTagsFromResource>
  deed get
  note <Removes metadata tags from an Amazon DocumentDB resource.>

  take resource-name
    like native-string
    note <The Amazon DocumentDB resource that the tags are removed from. This value is an Amazon Resource Name (ARN).>
  take tag-keys
    like list
      like string
    note <The tag key (name) of the tag to be removed.>
  take action
    like native-string
  take version
    like native-string

  line </#Action=RemoveTagsFromResource>

  find <ResourceName>, loan resource-name
  find <TagKeys>, loan tag-keys
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    note <Success>
  loot 480
    seed xml, like db-instance-not-found-fault
    note <DBInstanceNotFoundFault>
  loot 481
    seed xml, like db-snapshot-not-found-fault
    note <DBSnapshotNotFoundFault>
  loot 482
    seed xml, like db-cluster-not-found-fault
    note <DBClusterNotFoundFault>
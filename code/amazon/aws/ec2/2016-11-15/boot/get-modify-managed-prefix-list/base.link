
load @tunebond/snow/amazonaws.com/ec2/2016-11-15/base/add-prefix-list-entry
  take form add-prefix-list-entry

load @tunebond/snow/amazonaws.com/ec2/2016-11-15/base/modify-managed-prefix-list-result
  take form modify-managed-prefix-list-result

load @tunebond/snow/amazonaws.com/ec2/2016-11-15/base/remove-prefix-list-entry
  take form remove-prefix-list-entry

load @tunebond/snow/base/list
  take form list

load @tunebond/snow/base/native-boolean
  take form native-boolean

load @tunebond/snow/base/native-integer
  take form native-integer

load @tunebond/snow/base/native-string


load @wavebond/snow/base/integer
  take form integer

load @wavebond/snow/base/string
  take form string

form purchase-request, name <PurchaseRequest>
  note <Describes a request to purchase Scheduled Instances.>
  take instance-count, name <InstanceCount>
    like integer
    note <The number of instances.>
  take purchase-token, name <PurchaseToken>
    like string
    note <The purchase token.>


load @wavebond/snow/base/integer
  take form integer

load @wavebond/snow/base/string
  take form string

form target-configuration, name <TargetConfiguration>
  note <Information about the Convertible Reserved Instance offering.>
  take instance-count, name <InstanceCount>
    like integer
    void take
    note <The number of instances the Convertible Reserved Instance offering can be applied to. This parameter is reserved and cannot be specified in a request>
    host xml
      host name, text <instanceCount>
  take offering-id, name <OfferingId>
    like string
    void take
    note <The ID of the Convertible Reserved Instance offering.>
    host xml
      host name, text <offeringId>

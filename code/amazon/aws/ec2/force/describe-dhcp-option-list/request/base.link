
field request
  field base-request

  mount dhcp-options-id, chain string
  mount dry-run, field boolean
  mount filter, chain filter
  mount max-results, field integer
  mount next-token, field string

  state query, field query
    mount dhcp-options-id, share dhcp-options-id
    mount dry-run, share dry-run
    mount filter, share filter
    mount max-results, share max-results
    mount next-token, share next-token

field query
  mount dhcp-options-id
  mount dry-run
  mount filter
  mount max-results
  mount next-token

  state |DhcpOptionsId.:count|, share block
    drive dhcp-options-id
  state |DryRun|, share dry-run
  state |Filter.:count|, share block
    drive filter
  state |MaxResults|, share max-results
  state |NextToken|, share next-token

load @tunebond/snow/amazonaws.com/autoscaling/2011-01-01/base/invalid-next-token
  take form invalid-next-token

load @tunebond/snow/amazonaws.com/autoscaling/2011-01-01/base/launch-configurations-type
  take form launch-configurations-type

load @tunebond/snow/amazonaws.com/autoscaling/2011-01-01/base/resource-contention-fault
  take form resource-contention-fault

load @tunebond/snow/base/native-string
  take form native-string

boot post-describe-launch-configurations, name <POST_DescribeLaunchConfigurations>
  deed post
  note <Gets information about the launch configurations in the account and Region.>

  take max-records
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take action
    like native-string
  take version
    like native-string
  take launch-configuration-names-type, like launch-configuration-names-type
    void take

  line </#Action=DescribeLaunchConfigurations>

  find <MaxRecords>, loan max-records
  find <NextToken>, loan next-token
  find <Action>, loan action
  find <Version>, loan version

  seed xml, loan launch-configuration-names-type

  loot 200
    seed xml, like launch-configurations-type
    note <Success>
  loot 480
    seed xml, like invalid-next-token
    note <InvalidNextToken>
  loot 481
    seed xml, like resource-contention-fault
    note <ResourceContentionFault>
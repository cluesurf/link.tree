
load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/concurrent-modification-exception
  take form concurrent-modification-exception

load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/too-many-requests-exception
  take form too-many-requests-exception

load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/update-contact-body
  take form update-contact-body

load @nerdbond/snow/amazonaws.com/sesv2/2019-09-27/base/update-contact-response
  take form update-contact-response

load @nerdbond/snow/base/native-string
  take form native-string

boot update-contact, name <UpdateContact>
  deed put
  note <Updates a contact's preferences for a list. It is not necessary to specify all existing topic preferences in the TopicPreferences object, just the ones that need updating.>

  take contact-list-name
    like native-string
    note <The name of the contact list.>
  take email-address
    like native-string
    note <The contact's email addres.>
  take update-contact-body, like update-contact-body

  line </v2/email/contact-lists/{contact-list-name}/contacts/{email-address}>

  seed json, loan update-contact-body

  loot 200
    seed json, like update-contact-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 482
    seed json, like not-found-exception
    note <NotFoundException>
  loot 483
    seed json, like concurrent-modification-exception
    note <ConcurrentModificationException>
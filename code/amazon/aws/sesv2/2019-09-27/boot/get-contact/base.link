
load @treesurf/snow/amazonaws.com/sesv2/2019-09-27/base/bad-request-exception
  take form bad-request-exception

load @treesurf/snow/amazonaws.com/sesv2/2019-09-27/base/get-contact-response
  take form get-contact-response

load @treesurf/snow/amazonaws.com/sesv2/2019-09-27/base/not-found-exception
  take form not-found-exception

load @treesurf/snow/amazonaws.com/sesv2/2019-09-27/base/too-many-requests-exception
  take form too-many-requests-exception

load @treesurf/snow/base/native-string
  take form native-string

boot get-contact, name <GetContact>
  deed get
  note <Returns a contact from a contact list.>

  take contact-list-name
    like native-string
    note <The name of the contact list to which the contact belongs.>
  take email-address
    like native-string
    note <The contact's email addres.>

  line </v2/email/contact-lists/{contact-list-name}/contacts/{email-address}>

  loot 200
    seed json, like get-contact-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 482
    seed json, like not-found-exception
    note <NotFoundException>
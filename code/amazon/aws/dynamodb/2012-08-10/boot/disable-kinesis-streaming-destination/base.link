
load @wavebond/snow/amazonaws.com/dynamodb/2012-08-10/base/internal-server-error
  take form internal-server-error

load @wavebond/snow/amazonaws.com/dynamodb/2012-08-10/base/kinesis-streaming-destination-output
  take form kinesis-streaming-destination-output

load @wavebond/snow/amazonaws.com/dynamodb/2012-08-10/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @wavebond/snow/amazonaws.com/dynamodb/2012-08-10/base/resource-in-use-exception
  take form resource-in-use-exception

load @wavebond/snow/amazonaws.com/dynamodb/2012-08-10/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/base/native-string
  take form native-string

boot disable-kinesis-streaming-destination, name <DisableKinesisStreamingDestination>
  deed post
  note <Stops replication from the DynamoDB table to the Kinesis data stream. This is done without deleting either of the resources.>

  take x-amz-target
    like native-string
  take kinesis-streaming-destination-input, like kinesis-streaming-destination-input

  line </#X-Amz-Target=DynamoDB_20120810.DisableKinesisStreamingDestination>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan kinesis-streaming-destination-input

  loot 200
    seed json, like kinesis-streaming-destination-output
    note <Success>
  loot 480
    seed json, like internal-server-error
    note <InternalServerError>
  loot 481
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
  loot 482
    seed json, like resource-in-use-exception
    note <ResourceInUseException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
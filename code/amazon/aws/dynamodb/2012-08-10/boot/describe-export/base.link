
load @tunebond/snow/amazonaws.com/dynamodb/2012-08-10/base/describe-export-output
  take form describe-export-output

load @tunebond/snow/amazonaws.com/dynamodb/2012-08-10/base/export-not-found-exception
  take form export-not-found-exception

load @tunebond/snow/amazonaws.com/dynamodb/2012-08-10/base/internal-server-error
  take form internal-server-error

load @tunebond/snow/amazonaws.com/dynamodb/2012-08-10/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @tunebond/snow/base/native-string
  take form native-string

boot describe-export, name <DescribeExport>
  deed post
  note <Describes an existing table export.>

  take x-amz-target
    like native-string
  take describe-export-input, like describe-export-input

  line </#X-Amz-Target=DynamoDB_20120810.DescribeExport>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan describe-export-input

  loot 200
    seed json, like describe-export-output
    note <Success>
  loot 480
    seed json, like export-not-found-exception
    note <ExportNotFoundException>
  loot 481
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
  loot 482
    seed json, like internal-server-error
    note <InternalServerError>
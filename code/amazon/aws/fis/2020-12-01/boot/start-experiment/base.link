
load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/conflict-exception
  take form conflict-exception

load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/service-quota-exceeded-exception
  take form service-quota-exceeded-exception

load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/start-experiment-body
  take form start-experiment-body

load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/start-experiment-response
  take form start-experiment-response

load @nerdbond/snow/amazonaws.com/fis/2020-12-01/base/validation-exception
  take form validation-exception

boot start-experiment, name <StartExperiment>
  deed post
  note <Starts running an experiment from the specified experiment template.>

  take start-experiment-body, like start-experiment-body

  line </experiments>

  seed json, loan start-experiment-body

  loot 200
    seed json, like start-experiment-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like conflict-exception
    note <ConflictException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
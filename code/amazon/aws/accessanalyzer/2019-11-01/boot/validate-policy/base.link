
load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/validate-policy-body
  take form validate-policy-body

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/validate-policy-response
  take form validate-policy-response

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

boot validate-policy, name <ValidatePolicy>
  deed post
  note <Requests the validation of a policy and returns a list of findings. The findings help you identify issues and provide actionable recommendations to resolve the issue and enable you to author functional policies that meet security best practices.>

  take max-results
    like native-integer
    note <The maximum number of results to return in the response.>
    void take
  take next-token
    like native-string
    note <A token used for pagination of results returned.>
    void take
  take validate-policy-body, like validate-policy-body

  line </policy/validation>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token

  seed json, loan validate-policy-body

  loot 200
    seed json, like validate-policy-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 483
    seed json, like access-denied-exception
    note <AccessDeniedException>

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/get-finding-response
  take form get-finding-response

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/accessanalyzer/2019-11-01/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-finding, name <GetFinding>
  deed get
  note <Retrieves information about the specified finding.>

  take id
    like native-string
    note <The ID of the finding to retrieve.>
  take analyzer-arn
    like native-string
    note <The [ARN of the analyzer](https://docs.aws.amazon.com/IAM/latest/UserGuide/access-analyzer-getting-started.html#permission-resources) that generated the finding.>

  line </finding/{id}#analyzerArn>

  find <analyzerArn>, loan analyzer-arn

  loot 200
    seed json, like get-finding-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like access-denied-exception
    note <AccessDeniedException>
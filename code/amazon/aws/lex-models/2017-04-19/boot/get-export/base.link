
load @treesurf/snow/amazonaws.com/lex-models/2017-04-19/base/bad-request-exception
  take form bad-request-exception

load @treesurf/snow/amazonaws.com/lex-models/2017-04-19/base/get-export-response
  take form get-export-response

load @treesurf/snow/amazonaws.com/lex-models/2017-04-19/base/internal-failure-exception
  take form internal-failure-exception

load @treesurf/snow/amazonaws.com/lex-models/2017-04-19/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @treesurf/snow/amazonaws.com/lex-models/2017-04-19/base/not-found-exception
  take form not-found-exception

load @treesurf/snow/base/native-string
  take form native-string

boot get-export, name <GetExport>
  deed get
  note <Exports the contents of a Amazon Lex resource in a specified format.>

  take name
    like native-string
    note <The name of the bot to export.>
  take version
    like native-string
    note <The version of the bot to export.>
  take resource-type
    like native-string
    note <The type of resource to export.>
  take export-type
    like native-string
    note <The format of the exported data.>

  line </exports/#name&version&resourceType&exportType>

  find <name>, loan name
  find <version>, loan version
  find <resourceType>, loan resource-type
  find <exportType>, loan export-type

  loot 200
    seed json, like get-export-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
  loot 482
    seed json, like internal-failure-exception
    note <InternalFailureException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>
boot create-grant, name <CreateGrant>
  deed post
  note <Creates a grant for the specified license. A grant shares the use of license entitlements with specific AWS accounts.>

  take x-amz-target
    like native-string
  take create-grant-request, like create-grant-request

  line </#X-Amz-Target=AWSLicenseManager.CreateGrant>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan create-grant-request

  loot 200
    seed json, like create-grant-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 482
    seed json, like resource-limit-exceeded-exception
    note <ResourceLimitExceededException>
  loot 483
    seed json, like server-internal-exception
    note <ServerInternalException>
  loot 484
    seed json, like authorization-exception
    note <AuthorizationException>
  loot 485
    seed json, like rate-limit-exceeded-exception
    note <RateLimitExceededException>
  loot 486
    seed json, like access-denied-exception
    note <AccessDeniedException>
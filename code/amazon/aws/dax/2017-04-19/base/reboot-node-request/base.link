
load @textsurf/snow/base/string
  take form string

form reboot-node-request, name <RebootNodeRequest>
  take cluster-name, name <ClusterName>
    like string
    note <The name of the DAX cluster containing the node to be rebooted.>
  take node-id, name <NodeId>
    like string
    note <The system-assigned ID of the node to be rebooted.>
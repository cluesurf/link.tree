
load @treesurf/snow/amazonaws.com/application-insights/2018-11-25/base/internal-server-exception
  take form internal-server-exception

load @treesurf/snow/amazonaws.com/application-insights/2018-11-25/base/list-applications-response
  take form list-applications-response

load @treesurf/snow/amazonaws.com/application-insights/2018-11-25/base/validation-exception
  take form validation-exception

load @treesurf/snow/base/native-string
  take form native-string

boot list-applications, name <ListApplications>
  deed post
  note <Lists the IDs of the applications that you are monitoring.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-applications-request, like list-applications-request

  line </#X-Amz-Target=EC2WindowsBarleyService.ListApplications>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-applications-request

  loot 200
    seed json, like list-applications-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
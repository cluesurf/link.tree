
load @nerdbond/snow/base/string
  take form string

form provisioned-capacity-description, name <ProvisionedCapacityDescription>
  note <The definition for a provisioned capacity unit.>
  take capacity-id, name <CapacityId>
    like string
    void take
    note <The ID that identifies the provisioned capacity unit.>
  take start-date, name <StartDate>
    like string
    void take
    note <The date that the provisioned capacity unit was purchased, in Universal Coordinated Time (UTC).>
  take expiration-date, name <ExpirationDate>
    like string
    void take
    note <The date that the provisioned capacity unit expires, in Universal Coordinated Time (UTC).>

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/list-installed-components-response
  take form list-installed-components-response

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/greengrassv2/2020-11-30/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

boot list-installed-components, name <ListInstalledComponents>
  deed get
  note <Retrieves a paginated list of the components that a Greengrass core device runs.>

  take core-device-thing-name
    like native-string
    note <The name of the core device. This is also the name of the IoT thing.>
  take max-results
    like native-integer
    note <The maximum number of results to be returned per paginated request.>
    void take
  take next-token
    like native-string
    note <The token to be used for the next set of paginated results.>
    void take

  line </greengrass/v2/coreDevices/{core-device-thing-name}/installedComponents>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token

  loot 200
    seed json, like list-installed-components-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 484
    seed json, like throttling-exception
    note <ThrottlingException>
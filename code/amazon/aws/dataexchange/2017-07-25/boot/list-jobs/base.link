
load @wavebond/snow/amazonaws.com/dataexchange/2017-07-25/base/internal-server-exception
  take form internal-server-exception

load @wavebond/snow/amazonaws.com/dataexchange/2017-07-25/base/list-jobs-response
  take form list-jobs-response

load @wavebond/snow/amazonaws.com/dataexchange/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/amazonaws.com/dataexchange/2017-07-25/base/throttling-exception
  take form throttling-exception

load @wavebond/snow/amazonaws.com/dataexchange/2017-07-25/base/validation-exception
  take form validation-exception

load @wavebond/snow/base/native-integer
  take form native-integer

load @wavebond/snow/base/native-string
  take form native-string

boot list-jobs, name <ListJobs>
  deed get
  note <This operation lists your jobs sorted by CreatedAt in descending order.>

  take data-set-id
    like native-string
    note <The unique identifier for a data set.>
    void take
  take max-results
    like native-integer
    note <The maximum number of results returned by a single call.>
    void take
  take next-token
    like native-string
    note <The token value retrieved from a previous call to access the next page of results.>
    void take
  take revision-id
    like native-string
    note <The unique identifier for a revision.>
    void take
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </v1/jobs>

  find <dataSetId>, loan data-set-id
  find <maxResults>, loan max-results
  find <nextToken>, loan next-token
  find <revisionId>, loan revision-id
  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-jobs-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 482
    seed json, like validation-exception
    note <ValidationException>
  loot 483
    seed json, like internal-server-exception
    note <InternalServerException>


load @tunebond/snow/amazonaws.com/opensearch/2021-01-01/base/base-exception
  take form base-exception

load @tunebond/snow/amazonaws.com/opensearch/2021-01-01/base/internal-exception
  take form internal-exception

load @tunebond/snow/amazonaws.com/opensearch/2021-01-01/base/list-versions-response
  take form list-versions-response

load @tunebond/snow/amazonaws.com/opensearch/2021-01-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/opensearch/2021-01-01/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-integer
  take form native-integer

load @tunebond/snow/base/native-string
  take form native-string

boot list-versions, name <ListVersions>
  deed get
  note <List all supported versions of OpenSearch and Elasticsearch.>

  take max-results
    like native-integer
    note <Set this value to limit the number of results returned. Value must be greater than 10 or it won't be honored.>
    void take
  take next-token
    like native-string
    void take
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </2021-01-01/opensearch/versions>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token
  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-versions-response
    note <Success>
  loot 480
    seed json, like base-exception
    note <BaseException>
  loot 481
    seed json, like internal-exception
    note <InternalException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like validation-exception
    note <ValidationException>
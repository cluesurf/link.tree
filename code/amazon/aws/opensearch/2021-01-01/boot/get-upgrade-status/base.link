boot get-upgrade-status, name <GetUpgradeStatus>
  deed get
  note <Retrieves the latest status of the last upgrade or upgrade eligibility check performed on the domain.>

  take domain-name
    like native-string

  line </2021-01-01/opensearch/upgradeDomain/{domain-name}/status>

  loot 200
    seed json, like get-upgrade-status-response
    note <Success>
  loot 480
    seed json, like base-exception
    note <BaseException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like disabled-operation-exception
    note <DisabledOperationException>
  loot 483
    seed json, like validation-exception
    note <ValidationException>
  loot 484
    seed json, like internal-exception
    note <InternalException>
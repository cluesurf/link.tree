
load @tunebond/snow/base/integer
  take form integer

load @tunebond/snow/base/string
  take form string

form connectivity-info, name <ConnectivityInfo>
  note <Information about a Greengrass core's connectivity.>
  take host-address, name <HostAddress>
    like string
    void take
    note <The endpoint for the Greengrass core. Can be an IP address or DNS.>
  take id, name <Id>
    like string
    void take
    note <The ID of the connectivity information.>
  take metadata, name <Metadata>
    like string
    void take
    note <Metadata for this endpoint.>
  take port-number, name <PortNumber>
    like integer
    void take
    note <The port of the Greengrass core. Usually 8883.>

load @treesurf/snow/base/string
  take form string

form get-group-certificate-authority-response, name <GetGroupCertificateAuthorityResponse>
  take group-certificate-authority-arn, name <GroupCertificateAuthorityArn>
    like string
    void take
    note <The ARN of the certificate authority for the group.>
  take group-certificate-authority-id, name <GroupCertificateAuthorityId>
    like string
    void take
    note <The ID of the certificate authority for the group.>
  take pem-encoded-certificate, name <PemEncodedCertificate>
    like string
    void take
    note <The PEM encoded certificate for the group.>

load @tunebond/snow/base/string
  take form string

form create-domain-request, name <CreateDomainRequest>
  take domain-name, name <DomainName>
    like string
    note <The name of the domain to create. The name can range between 3 and 255 characters and can contain the following characters: a-z, A-Z, 0-9, '_', '-', and '.'.>

load @wavebond/snow/amazonaws.com/importexport/2010-06-01/base/invalid-access-key-id-exception
  take form invalid-access-key-id-exception

load @wavebond/snow/amazonaws.com/importexport/2010-06-01/base/invalid-parameter-exception
  take form invalid-parameter-exception

load @wavebond/snow/amazonaws.com/importexport/2010-06-01/base/invalid-version-exception

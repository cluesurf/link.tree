
load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/tag-key
  take form tag-key

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/untag-resource-response
  take form untag-resource-response

load @nerdbond/snow/base/list
  take form list

load @nerdbond/snow/base/native-string
  take form native-string

boot untag-resource, name <UntagResource>
  deed delete
  note <Removes one or more tags from the specified Amazon Connect Customer Profiles resource. In Connect Customer Profiles, domains, profile object types, and integrations can be tagged.>

  take resource-arn
    like native-string
    note <The ARN of the resource from which you are removing tags.>
  take tag-keys
    like list
      like tag-key
    note <The list of tag keys to remove from the resource.>

  line </tags/{resource-arn}#tagKeys>

  find <tagKeys>, loan tag-keys

  loot 200
    seed json, like untag-resource-response
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
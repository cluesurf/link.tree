
load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/delete-domain-response
  take form delete-domain-response

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/customer-profiles/2020-08-15/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot delete-domain, name <DeleteDomain>
  deed delete
  note <Deletes a specific domain and all of its customer data, such as customer profile attributes and their related objects.>

  take domain-name
    like native-string
    note <The unique name of the domain.>

  line </domains/{domain-name}>

  loot 200
    seed json, like delete-domain-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like internal-server-exception
    note <InternalServerException>
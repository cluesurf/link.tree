load @drumwork/snow/base/native-string
  take form native-string

boot delete-profile-object-type, name <DeleteProfileObjectType>
  note <Removes a ProfileObjectType from a specific domain as well as removes all the ProfileObjects of that type. It also disables integrations from this specific ProfileObjectType. In addition, it scrubs all of the fields of the standard profile that were populated from this ProfileObjectType.>

  take domain-name
    like native-string
    note <The unique name of the domain.>
  take object-type-name
    like native-string
    note <The name of the profile object type.>

  line </domains/{domain-name}/object-types/{object-type-name}>

  loot 200
    seed json, like delete-profile-object-type-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like internal-server-exception
    note <InternalServerException>
load @termsurf/snow/base/native-string
  take form native-string

boot get-repository-permissions-policy, name <GetRepositoryPermissionsPolicy>
  deed get
  note <Returns the resource policy that is set on a repository.>

  take domain
    like native-string
    note <The name of the domain containing the repository whose associated resource policy is to be retrieved.>
  take domain-owner
    like native-string
    note <The 12-digit account number of the AWS account that owns the domain. It does not include dashes or spaces.>
    void take
  take repository
    like native-string
    note <The name of the repository whose associated resource policy is to be retrieved.>

  line </v1/repository/permissions/policy#domain&repository>

  find <domain>, loan domain
  find <domain-owner>, loan domain-owner
  find <repository>, loan repository

  loot 200
    seed json, like get-repository-permissions-policy-result
    note <Success>
  loot 480
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like validation-exception
    note <ValidationException>
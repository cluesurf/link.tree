boot create-repository, name <CreateRepository>
  deed post
  note <Creates a repository.>

  take domain
    like native-string
    note <The name of the domain that contains the created repository.>
  take domain-owner
    like native-string
    note <The 12-digit account number of the AWS account that owns the domain. It does not include dashes or spaces.>
    void take
  take repository
    like native-string
    note <The name of the repository to create.>
  take create-repository-body, like create-repository-body

  line </v1/repository#domain&repository>

  find <domain>, loan domain
  find <domain-owner>, loan domain-owner
  find <repository>, loan repository

  seed json, loan create-repository-body

  loot 200
    seed json, like create-repository-result
    note <Success>
  loot 480
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 481
    seed json, like conflict-exception
    note <ConflictException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 484
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
  loot 485
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 486
    seed json, like validation-exception
    note <ValidationException>

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/forbidden-exception
  take form forbidden-exception

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/internal-server-error-exception
  take form internal-server-error-exception

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/method-not-allowed-exception
  take form method-not-allowed-exception

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/not-found-exception
  take form not-found-exception

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/tag-body
  take form tag-body

load @textsurf/snow/amazonaws.com/resource-groups/2017-11-27/base/tag-output

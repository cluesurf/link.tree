
load @textsurf/snow/amazonaws.com/cloudsearch/2011-02-01/base/base-exception
  take form base-exception

load @textsurf/snow/amazonaws.com/cloudsearch/2011-02-01/base/describe-index-fields-response
  take form describe-index-fields-response

load @textsurf/snow/amazonaws.com/cloudsearch/2011-02-01/base/field-name
  take form field-name

load @textsurf/snow/amazonaws.com/cloudsearch/2011-02-01/base/internal-exception
  take form internal-exception

load @textsurf/snow/amazonaws.com/cloudsearch/2011-02-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/base/list
  take form list

load @textsurf/snow/base/native-string
  take form native-string

boot get-describe-index-fields, name <GET_DescribeIndexFields>
  deed get
  note <Gets information about the index fields configured for the search domain. Can be limited to specific fields by name. Shows all fields by default.>

  take domain-name
    like native-string
  take field-names
    like list
      like field-name
    note <Limits the `DescribeIndexFields` response to the specified fields.>
    void take
  take action
    like native-string
  take version
    like native-string

  line </#Action=DescribeIndexFields>

  find <DomainName>, loan domain-name
  find <FieldNames>, loan field-names
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like describe-index-fields-response
    note <Success>
  loot 409
    seed xml, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 480
    seed xml, like base-exception
    note <BaseException>
  loot 500
    seed xml, like internal-exception
    note <InternalException>

load @tunebond/snow/amazonaws.com/cloudsearch/2013-01-01/base/base-exception
  take form base-exception

load @tunebond/snow/amazonaws.com/cloudsearch/2013-01-01/base/delete-expression-response
  take form delete-expression-response

load @tunebond/snow/amazonaws.com/cloudsearch/2013-01-01/base/internal-exception
  take form internal-exception

load @tunebond/snow/amazonaws.com/cloudsearch/2013-01-01/base/invalid-type-exception
  take form invalid-type-exception

load @tunebond/snow/amazonaws.com/cloudsearch/2013-01-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/base/native-string
  take form native-string

boot get-delete-expression, name <GET_DeleteExpression>
  deed get
  note <Removes an `Expression` from the search domain. For more information, see [Configuring Expressions](http://docs.aws.amazon.com/cloudsearch/latest/developerguide/configuring-expressions.html) in the _Amazon CloudSearch Developer Guide_.>

  take domain-name
    like native-string
  take expression-name
    like native-string
    note <The name of the `Expression` to delete.>
  take action
    like native-string
  take version
    like native-string

  line </#Action=DeleteExpression>

  find <DomainName>, loan domain-name
  find <ExpressionName>, loan expression-name
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like delete-expression-response
    note <Success>
  loot 480
    seed xml, like base-exception
    note <BaseException>
  loot 481
    seed xml, like internal-exception
    note <InternalException>
  loot 482
    seed xml, like invalid-type-exception
    note <InvalidTypeException>
  loot 483
    seed xml, like resource-not-found-exception
    note <ResourceNotFoundException>
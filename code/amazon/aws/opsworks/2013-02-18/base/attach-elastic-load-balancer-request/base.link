
load @nerdbond/snow/base/string
  take form string

form attach-elastic-load-balancer-request, name <AttachElasticLoadBalancerRequest>
  take elastic-load-balancer-name, name <ElasticLoadBalancerName>
    like string
    note <The Elastic Load Balancing instance's name.>
  take layer-id, name <LayerId>
    like string
    note <The ID of the layer to which the Elastic Load Balancing instance is to be attached.>

load @termsurf/snow/base/string
  take form string

form register-rds-db-instance-request, name <RegisterRdsDbInstanceRequest>
  take stack-id, name <StackId>
    like string
    note <The stack ID.>
  take rds-db-instance-arn, name <RdsDbInstanceArn>
    like string
    note <The Amazon RDS instance's ARN.>
  take db-user, name <DbUser>
    like string
    note <The database's master user name.>
  take db-password, name <DbPassword>
    like string
    note <The database password.>
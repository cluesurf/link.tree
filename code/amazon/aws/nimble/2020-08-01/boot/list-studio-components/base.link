  take form list

load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/base/string
  take form string

boot list-studio-components, name <ListStudioComponents>
  deed get
  note <Lists the StudioComponents in a studio.>

  take studio-id
    like native-string
    note <The studio ID.>
  take max-results
    like native-integer
    note <The maximum number of results to be returned per request.>
    void take
  take next-token
    like native-string
    note <The token for the next set of results, or null if there are no more results.>
    void take
  take states
    like list
      like string
    note <A list of states.>
    void take
  take types
    like list
      like string
    note <The types.>
    void take

  line </2020-08-01/studios/{studio-id}/studio-components>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token
  find <states>, loan states
  find <types>, loan types

  loot 200
    seed json, like list-studio-components-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 482
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
  loot 483
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 484
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 485
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 486
    seed json, like conflict-exception
    note <ConflictException>
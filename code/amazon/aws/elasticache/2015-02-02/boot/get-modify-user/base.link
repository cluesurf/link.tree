
load @tunebond/snow/amazonaws.com/elasticache/2015-02-02/base/invalid-parameter-combination-exception
  take form invalid-parameter-combination-exception

load @tunebond/snow/amazonaws.com/elasticache/2015-02-02/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @tunebond/snow/amazonaws.com/elasticache/2015-02-02/base/invalid-user-state-fault
  take form invalid-user-state-fault

load @tunebond/snow/amazonaws.com/elasticache/2015-02-02/base/user
  take form user

load @tunebond/snow/amazonaws.com/elasticache/2015-02-02/base/user-not-found-fault
  take form user-not-found-fault

load @tunebond/snow/base/list
  take form list

load @tunebond/snow/base/native-boolean
  take form native-boolean

load @tunebond/snow/base/native-string
  take form native-string

load @tunebond/snow/base/string
  take form string

boot get-modify-user, name <GET_ModifyUser>
  deed get
  note <Changes user password(s) and/or access string.>

  take user-id
    like native-string
    note <The ID of the user.>
  take access-string
    like native-string
    note <Access permissions string used for this user.>
    void take
  take append-access-string
    like native-string
    note <Adds additional user permissions to the access string.>
    void take
  take passwords
    like list
      like string
    note <The passwords belonging to the user. You are allowed up to two.>
    void take
  take no-password-required
    like native-boolean
    note <Indicates no password is required for the user.>
    void take
  take action
    like native-string
  take version
    like native-string

  line </#Action=ModifyUser>

  find <UserId>, loan user-id
  find <AccessString>, loan access-string
  find <AppendAccessString>, loan append-access-string
  find <Passwords>, loan passwords
  find <NoPasswordRequired>, loan no-password-required
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like user
    note <Success>
  loot 480
    seed xml, like user-not-found-fault
    note <UserNotFoundFault>
  loot 481
    seed xml, like invalid-user-state-fault
    note <InvalidUserStateFault>
  loot 482
    seed xml, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 483
    seed xml, like invalid-parameter-combination-exception
    note <InvalidParameterCombinationException>

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/access-denied-exception
  take form access-denied-exception

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/internal-server-exception
  take form internal-server-exception

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/list-executions-output
  take form list-executions-output

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/resource-not-found-exception
  take form resource-not-found-exception

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/throttling-exception
  take form throttling-exception

load @treesurf/snow/amazonaws.com/snow-device-management/2021-08-04/base/validation-exception
  take form validation-exception

load @treesurf/snow/base/native-integer
  take form native-integer

load @treesurf/snow/base/native-string
  take form native-string

boot list-executions, name <ListExecutions>
  deed get
  note <Returns the status of tasks for one or more target devices.>

  take max-results
    like native-integer
    note <The maximum number of tasks to list per page.>
    void take
  take next-token
    like native-string
    note <A pagination token to continue to the next page of tasks.>
    void take
  take state
    like native-string
    note <A structure used to filter the tasks by their current state.>
    void take
  take task-id
    like native-string
    note <The ID of the task.>

  line </executions#taskId>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token
  find <state>, loan state
  find <taskId>, loan task-id

  loot 200
    seed json, like list-executions-output
    note <Success>
  loot 480
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like validation-exception
    note <ValidationException>
  loot 484
    seed json, like access-denied-exception
    note <AccessDeniedException>
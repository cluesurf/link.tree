
load @wavebond/snow/amazonaws.com/snow-device-management/2021-08-04/base/access-denied-exception
  take form access-denied-exception

load @wavebond/snow/amazonaws.com/snow-device-management/2021-08-04/base/internal-server-exception
  take form internal-server-exception

load @wavebond/snow/amazonaws.com/snow-device-management/2021-08-04/base/list-devices-output
  take form list-devices-output

load @wavebond/snow/amazonaws.com/snow-device-management/2021-08-04/base/throttling-exception
  take form throttling-exception

load @wavebond/snow/amazonaws.com/snow-device-management/2021-08-04/base/validation-exception
  take form validation-exception

load @wavebond/snow/base/native-integer
  take form native-integer

load @wavebond/snow/base/native-string
  take form native-string

boot list-devices, name <ListDevices>
  deed get
  note <Returns a list of all devices on your Amazon Web Services account that have Amazon Web Services Snow Device Management enabled in the Amazon Web Services Region where the command is run.>

  take job-id
    like native-string
    note <The ID of the job used to order the device.>
    void take
  take max-results
    like native-integer
    note <The maximum number of devices to list per page.>
    void take
  take next-token
    like native-string
    note <A pagination token to continue to the next page of results.>
    void take

  line </managed-devices>

  find <jobId>, loan job-id
  find <maxResults>, loan max-results
  find <nextToken>, loan next-token

  loot 200
    seed json, like list-devices-output
    note <Success>
  loot 480
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like validation-exception
    note <ValidationException>
  loot 483
    seed json, like access-denied-exception
    note <AccessDeniedException>

load @termsurf/snow/base/string
  take form string

form software-information, name <SoftwareInformation>
  note <Information about the software on the device.>
  take install-state, name <installState>
    like string
    void take
    note <The state of the software that is installed or that is being installed on the device.>
  take installed-version, name <installedVersion>
    like string
    void take
    note <The version of the software currently installed on the device.>
  take installing-version, name <installingVersion>
    like string
    void take
    note <The version of the software being installed on the device.>
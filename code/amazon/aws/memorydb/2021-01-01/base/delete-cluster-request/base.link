
load @nerdbond/snow/base/string
  take form string

form delete-cluster-request, name <DeleteClusterRequest>
  take cluster-name, name <ClusterName>
    like string
    note <The name of the cluster to be deleted>
  take final-snapshot-name, name <FinalSnapshotName>
    like string
    void take
    note <The user-supplied name of a final cluster snapshot. This is the unique name that identifies the snapshot. MemoryDB creates the snapshot, and then deletes the cluster immediately afterward.>
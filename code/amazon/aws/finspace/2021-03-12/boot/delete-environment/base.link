
load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/delete-environment-response
  take form delete-environment-response

load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/finspace/2021-03-12/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot delete-environment, name <DeleteEnvironment>
  deed delete
  note <Delete an FinSpace environment.>

  take environment-id
    like native-string
    note <The identifier for the FinSpace environment.>

  line </environment/{environment-id}>

  loot 200
    seed json, like delete-environment-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like validation-exception
    note <ValidationException>

load @textsurf/snow/amazonaws.com/finspace/2021-03-12/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/finspace/2021-03-12/base/invalid-request-exception
  take form invalid-request-exception

load @textsurf/snow/amazonaws.com/finspace/2021-03-12/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/amazonaws.com/finspace/2021-03-12/base/tag-key
  take form tag-key

load @textsurf/snow/amazonaws.com/finspace/2021-03-12/base/untag-resource-response
  take form untag-resource-response

load @textsurf/snow/base/list
  take form list

load @textsurf/snow/base/native-string
  take form native-string

boot untag-resource, name <UntagResource>
  deed delete
  note <Removes metadata tags from a FinSpace resource.>

  take resource-arn
    like native-string
    note <A FinSpace resource from which you want to remove a tag or tags. The value for this parameter is an Amazon Resource Name (ARN).>
  take tag-keys
    like list
      like tag-key
    note <The tag keys (names) of one or more tags to be removed.>

  line </tags/{resource-arn}#tagKeys>

  find <tagKeys>, loan tag-keys

  loot 200
    seed json, like untag-resource-response
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like invalid-request-exception
    note <InvalidRequestException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
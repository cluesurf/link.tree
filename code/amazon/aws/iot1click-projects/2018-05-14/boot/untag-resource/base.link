
load @nerdbond/snow/amazonaws.com/iot1click-projects/2018-05-14/base/internal-failure-exception
  take form internal-failure-exception

load @nerdbond/snow/amazonaws.com/iot1click-projects/2018-05-14/base/invalid-request-exception
  take form invalid-request-exception

load @nerdbond/snow/amazonaws.com/iot1click-projects/2018-05-14/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/iot1click-projects/2018-05-14/base/tag-key
  take form tag-key

load @nerdbond/snow/amazonaws.com/iot1click-projects/2018-05-14/base/untag-resource-response
  take form untag-resource-response

load @nerdbond/snow/base/list
  take form list

load @nerdbond/snow/base/native-string
  take form native-string

boot untag-resource, name <UntagResource>
  deed delete
  note <Removes one or more tags (metadata key/value pairs) from a resource.>

  take resource-arn
    like native-string
    note <The ARN of the resource whose tag you want to remove.>
  take tag-keys
    like list
      like tag-key
    note <The keys of those tags which you want to remove.>

  line </tags/{resource-arn}#tagKeys>

  find <tagKeys>, loan tag-keys

  loot 200
    seed json, like untag-resource-response
    note <Success>
  loot 480
    seed json, like internal-failure-exception
    note <InternalFailureException>
  loot 481
    seed json, like invalid-request-exception
    note <InvalidRequestException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>

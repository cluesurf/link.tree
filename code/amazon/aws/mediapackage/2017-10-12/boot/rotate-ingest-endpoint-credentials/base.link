boot rotate-ingest-endpoint-credentials, name <RotateIngestEndpointCredentials>
  deed put
  note <Rotate the IngestEndpoint's username and password, as specified by the IngestEndpoint's id.>

  take id
    like native-string
    note <The ID of the channel the IngestEndpoint is on.>
  take ingest-endpoint-id
    like native-string
    note <The id of the IngestEndpoint whose credentials should be rotated>

  line </channels/{id}/ingest_endpoints/{ingest-endpoint-id}/credentials>

  loot 200
    seed json, like rotate-ingest-endpoint-credentials-response
    note <Success>
  loot 480
    seed json, like unprocessable-entity-exception
    note <UnprocessableEntityException>
  loot 481

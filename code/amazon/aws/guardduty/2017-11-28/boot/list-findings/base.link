
load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/internal-server-error-exception
  take form internal-server-error-exception

load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/list-findings-body
  take form list-findings-body

load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/list-findings-response
  take form list-findings-response

load @nerdbond/snow/base/native-string
  take form native-string

boot list-findings, name <ListFindings>
  deed post
  note <Lists Amazon GuardDuty findings for the specified detector ID.>

  take detector-id
    like native-string
    note <The ID of the detector that specifies the GuardDuty service whose findings you want to list.>
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-findings-body, like list-findings-body

  line </detector/{detector-id}/findings>

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-findings-body

  loot 200
    seed json, like list-findings-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>

load @tunebond/snow/amazonaws.com/guardduty/2017-11-28/base/accept-invitation-body
  take form accept-invitation-body

load @tunebond/snow/amazonaws.com/guardduty/2017-11-28/base/accept-invitation-response
  take form accept-invitation-response

load @tunebond/snow/amazonaws.com/guardduty/2017-11-28/base/bad-request-exception
  take form bad-request-exception

load @tunebond/snow/amazonaws.com/guardduty/2017-11-28/base/internal-server-error-exception
  take form internal-server-error-exception

load @tunebond/snow/base/native-string
  take form native-string

boot accept-invitation, name <AcceptInvitation>
  deed post
  note <Accepts the invitation to be monitored by a GuardDuty administrator account.>

  take detector-id
    like native-string
    note <The unique ID of the detector of the GuardDuty member account.>
  take accept-invitation-body, like accept-invitation-body

  line </detector/{detector-id}/master>

  seed json, loan accept-invitation-body

  loot 200
    seed json, like accept-invitation-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
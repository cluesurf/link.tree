
load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/get-filter-response
  take form get-filter-response

load @nerdbond/snow/amazonaws.com/guardduty/2017-11-28/base/internal-server-error-exception
  take form internal-server-error-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-filter, name <GetFilter>
  deed get
  note <Returns the details of the filter specified by the filter name.>

  take detector-id
    like native-string
    note <The unique ID of the detector that the filter is associated with.>
  take filter-name
    like native-string
    note <The name of the filter you want to get.>

  line </detector/{detector-id}/filter/{filter-name}>

  loot 200
    seed json, like get-filter-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
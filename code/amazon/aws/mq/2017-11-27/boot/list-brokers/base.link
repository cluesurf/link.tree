
load @textsurf/snow/amazonaws.com/mq/2017-11-27/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/mq/2017-11-27/base/forbidden-exception
  take form forbidden-exception

load @textsurf/snow/amazonaws.com/mq/2017-11-27/base/internal-server-error-exception
  take form internal-server-error-exception

load @textsurf/snow/amazonaws.com/mq/2017-11-27/base/list-brokers-response
  take form list-brokers-response

load @textsurf/snow/base/native-integer
  take form native-integer

load @textsurf/snow/base/native-string
  take form native-string

boot list-brokers, name <ListBrokers>
  deed get
  note <Returns a list of all brokers.>

  take max-results
    like native-integer
    note <The maximum number of brokers that Amazon MQ can return per page (20 by default). This value must be an integer from 5 to 100.>
    void take
  take next-token
    like native-string
    note <The token that specifies the next page of results Amazon MQ should return. To request the first page, leave nextToken empty.>
    void take
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </v1/brokers>

  find <maxResults>, loan max-results
  find <nextToken>, loan next-token
  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-brokers-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 482
    seed json, like forbidden-exception
    note <ForbiddenException>

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/authorization-error-exception
  take form authorization-error-exception

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/concurrent-access-exception
  take form concurrent-access-exception

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/invalid-parameter-exception
  take form invalid-parameter-exception

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/list-tags-for-resource-response
  take form list-tags-for-resource-response

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/sns/2010-03-31/base/tag-policy-exception
  take form tag-policy-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-list-tags-for-resource, name <GET_ListTagsForResource>
  deed get
  note <List all tags added to the specified Amazon SNS topic. For an overview, see [Amazon SNS Tags](https://docs.aws.amazon.com/sns/latest/dg/sns-tags.html) in the _Amazon Simple Notification Service Developer Guide_.>

  take resource-arn
    like native-string
    note <The ARN of the topic for which to list tags.>
  take action
    like native-string
  take version
    like native-string

  line </#Action=ListTagsForResource>

  find <ResourceArn>, loan resource-arn
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like list-tags-for-resource-response
    note <Success>
  loot 480
    seed xml, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed xml, like tag-policy-exception
    note <TagPolicyException>
  loot 482
    seed xml, like invalid-parameter-exception
    note <InvalidParameterException>
  loot 483
    seed xml, like authorization-error-exception
    note <AuthorizationErrorException>
  loot 484
    seed xml, like concurrent-access-exception
    note <ConcurrentAccessException>
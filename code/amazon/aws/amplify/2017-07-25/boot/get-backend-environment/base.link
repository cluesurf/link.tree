
load @drumwork/snow/amazonaws.com/amplify/2017-07-25/base/bad-request-exception
  take form bad-request-exception

load @drumwork/snow/amazonaws.com/amplify/2017-07-25/base/get-backend-environment-result
  take form get-backend-environment-result

load @drumwork/snow/amazonaws.com/amplify/2017-07-25/base/internal-failure-exception
  take form internal-failure-exception

load @drumwork/snow/amazonaws.com/amplify/2017-07-25/base/not-found-exception
  take form not-found-exception

load @drumwork/snow/amazonaws.com/amplify/2017-07-25/base/unauthorized-exception
  take form unauthorized-exception

load @drumwork/snow/base/native-string
  take form native-string

boot get-backend-environment, name <GetBackendEnvironment>
  deed get
  note <Returns a backend environment for an Amplify app.>

  take app-id
    like native-string
    note <The unique id for an Amplify app.>
  take environment-name
    like native-string
    note <The name for the backend environment.>

  line </apps/{app-id}/backendenvironments/{environment-name}>

  loot 200
    seed json, like get-backend-environment-result
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like not-found-exception
    note <NotFoundException>
  loot 483
    seed json, like internal-failure-exception
    note <InternalFailureException>
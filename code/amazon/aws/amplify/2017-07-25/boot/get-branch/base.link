
load @tunebond/snow/amazonaws.com/amplify/2017-07-25/base/bad-request-exception
  take form bad-request-exception

load @tunebond/snow/amazonaws.com/amplify/2017-07-25/base/get-branch-result
  take form get-branch-result

load @tunebond/snow/amazonaws.com/amplify/2017-07-25/base/internal-failure-exception
  take form internal-failure-exception

load @tunebond/snow/amazonaws.com/amplify/2017-07-25/base/not-found-exception
  take form not-found-exception

load @tunebond/snow/amazonaws.com/amplify/2017-07-25/base/unauthorized-exception
  take form unauthorized-exception

load @tunebond/snow/base/native-string
  take form native-string

boot get-branch, name <GetBranch>
  deed get
  note <Returns a branch for an Amplify app.>

  take app-id
    like native-string
    note <The unique ID for an Amplify app.>
  take branch-name
    like native-string
    note <The name for the branch.>

  line </apps/{app-id}/branches/{branch-name}>

  loot 200
    seed json, like get-branch-result
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like not-found-exception
    note <NotFoundException>
  loot 483
    seed json, like internal-failure-exception
    note <InternalFailureException>
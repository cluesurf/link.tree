
load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/get-job-result
  take form get-job-result

load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/internal-failure-exception
  take form internal-failure-exception

load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/amplify/2017-07-25/base/unauthorized-exception
  take form unauthorized-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-job, name <GetJob>
  deed get
  note <Returns a job for a branch of an Amplify app.>

  take app-id
    like native-string
    note <The unique ID for an Amplify app.>
  take branch-name
    like native-string
    note <The branch name for the job.>
  take job-id
    like native-string
    note <The unique ID for the job.>

  line </apps/{app-id}/branches/{branch-name}/jobs/{job-id}>

  loot 200
    seed json, like get-job-result
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like internal-failure-exception
    note <InternalFailureException>
  loot 483
    seed json, like not-found-exception
    note <NotFoundException>
  loot 484
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
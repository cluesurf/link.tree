
load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/internal-failure-exception
  take form internal-failure-exception

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/not-found-exception
  take form not-found-exception

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/start-job-body
  take form start-job-body

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/start-job-result
  take form start-job-result

load @textsurf/snow/amazonaws.com/amplify/2017-07-25/base/unauthorized-exception
  take form unauthorized-exception

load @textsurf/snow/base/native-string
  take form native-string

boot start-job, name <StartJob>
  deed post
  note <Starts a new job for a branch of an Amplify app.>

  take app-id
    like native-string
    note <The unique ID for an Amplify app.>
  take branch-name
    like native-string
    note <The branch name for the job.>
  take start-job-body, like start-job-body

  line </apps/{app-id}/branches/{branch-name}/jobs>

  seed json, loan start-job-body

  loot 200
    seed json, like start-job-result
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like unauthorized-exception
    note <UnauthorizedException>
  loot 482
    seed json, like internal-failure-exception
    note <InternalFailureException>
  loot 483
    seed json, like not-found-exception
    note <NotFoundException>
  loot 484
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
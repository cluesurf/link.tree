
load @nerdbond/snow/base/string
  take form string

form hit-layout-parameter, name <HITLayoutParameter>
  note <The HITLayoutParameter data structure defines parameter values used with a HITLayout. A HITLayout is a reusable Amazon Mechanical Turk project template used to provide Human Intelligence Task (HIT) question data for CreateHIT.>
  take name, name <Name>
    like string
    note <The name of the parameter in the HITLayout.>
  take value, name <Value>
    like string
    note <The value substituted for the parameter referenced in the HITLayout.>
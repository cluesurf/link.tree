
load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/stop-job-run-response
  take form stop-job-run-response

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/validation-exception
  take form validation-exception

load @wavebond/snow/base/native-string
  take form native-string

boot stop-job-run, name <StopJobRun>
  deed post
  note <Stops a particular run of a job.>

  take name
    like native-string
    note <The name of the job to be stopped.>
  take run-id
    like native-string
    note <The ID of the job run to be stopped.>

  line </jobs/{name}/jobRun/{run-id}/stopJobRun>

  loot 200
    seed json, like stop-job-run-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/publish-recipe-body
  take form publish-recipe-body

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/publish-recipe-response
  take form publish-recipe-response

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/service-quota-exceeded-exception
  take form service-quota-exceeded-exception

load @wavebond/snow/amazonaws.com/databrew/2017-07-25/base/validation-exception
  take form validation-exception

load @wavebond/snow/base/native-string
  take form native-string

boot publish-recipe, name <PublishRecipe>
  deed post
  note <Publishes a new version of a DataBrew recipe.>

  take name
    like native-string
    note <The name of the recipe to be published.>
  take publish-recipe-body, like publish-recipe-body

  line </recipes/{name}/publishRecipe>

  seed json, loan publish-recipe-body

  loot 200
    seed json, like publish-recipe-response
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
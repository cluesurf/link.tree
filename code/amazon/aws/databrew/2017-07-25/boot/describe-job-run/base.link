
load @textsurf/snow/amazonaws.com/databrew/2017-07-25/base/describe-job-run-response
  take form describe-job-run-response

load @textsurf/snow/amazonaws.com/databrew/2017-07-25/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/amazonaws.com/databrew/2017-07-25/base/validation-exception
  take form validation-exception

load @textsurf/snow/base/native-string
  take form native-string

boot describe-job-run, name <DescribeJobRun>
  deed get
  note <Represents one run of a DataBrew job.>

  take name
    like native-string
    note <The name of the job being processed during this run.>
  take run-id
    like native-string
    note <The unique identifier of the job run.>

  line </jobs/{name}/jobRun/{run-id}>

  loot 200
    seed json, like describe-job-run-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>
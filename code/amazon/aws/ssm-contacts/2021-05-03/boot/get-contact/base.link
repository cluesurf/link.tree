
load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/data-encryption-exception
  take form data-encryption-exception

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/get-contact-result
  take form get-contact-result

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-contact, name <GetContact>
  deed post
  note <Retrieves information about the specified contact or escalation plan.>

  take x-amz-target
    like native-string
  take get-contact-request, like get-contact-request

  line </#X-Amz-Target=SSMContacts.GetContact>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan get-contact-request

  loot 200
    seed json, like get-contact-result
    note <Success>
  loot 480
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like validation-exception
    note <ValidationException>
  loot 485
    seed json, like data-encryption-exception
    note <DataEncryptionException>
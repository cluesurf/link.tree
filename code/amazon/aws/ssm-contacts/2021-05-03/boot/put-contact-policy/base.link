load @drumwork/snow/base/native-string
  take form native-string

boot put-contact-policy, name <PutContactPolicy>
  note <Adds a resource to the specified contact or escalation plan.>

  take x-amz-target
    like native-string
  take put-contact-policy-request, like put-contact-policy-request

  line </#X-Amz-Target=SSMContacts.PutContactPolicy>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan put-contact-policy-request

  loot 200
    seed json, like put-contact-policy-result
    note <Success>
  loot 480
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 481
    seed json, like conflict-exception
    note <ConflictException>
  loot 482
    seed json, like validation-exception
    note <ValidationException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 484
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 485
    seed json, like internal-server-exception
    note <InternalServerException>
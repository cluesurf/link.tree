
load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/access-denied-exception
  take form access-denied-exception

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/data-encryption-exception
  take form data-encryption-exception

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/internal-server-exception
  take form internal-server-exception

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/start-engagement-result
  take form start-engagement-result

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/throttling-exception
  take form throttling-exception

load @tunebond/snow/amazonaws.com/ssm-contacts/2021-05-03/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-string
  take form native-string

boot start-engagement, name <StartEngagement>
  deed post
  note <Starts an engagement to a contact or escalation plan. The engagement engages each contact specified in the incident.>

  take x-amz-target
    like native-string
  take start-engagement-request, like start-engagement-request

  line </#X-Amz-Target=SSMContacts.StartEngagement>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan start-engagement-request

  loot 200
    seed json, like start-engagement-result
    note <Success>
  loot 480
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 481
    seed json, like data-encryption-exception
    note <DataEncryptionException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 484
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 485
    seed json, like validation-exception
    note <ValidationException>
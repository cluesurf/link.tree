
load @tunebond/snow/amazonaws.com/location/2020-11-19/base/access-denied-exception
  take form access-denied-exception

load @tunebond/snow/amazonaws.com/location/2020-11-19/base/get-device-position-response
  take form get-device-position-response

load @tunebond/snow/amazonaws.com/location/2020-11-19/base/internal-server-exception
  take form internal-server-exception

load @tunebond/snow/amazonaws.com/location/2020-11-19/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/location/2020-11-19/base/throttling-exception
  take form throttling-exception

load @tunebond/snow/amazonaws.com/location/2020-11-19/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-string
  take form native-string

boot get-device-position, name <GetDevicePosition>
  deed get
  note <Retrieves a device's most recent position according to its sample time.

Device positions are deleted after 30 days.>

  take device-id
    like native-string
    note <The device whose position you want to retrieve.>
  take tracker-name
    like native-string
    note <The tracker resource receiving the position update.>

  line </tracking/v0/trackers/{tracker-name}/devices/{device-id}/positions/latest>

  loot 200
    seed json, like get-device-position-response
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like validation-exception
    note <ValidationException>
  loot 484
    seed json, like throttling-exception
    note <ThrottlingException>

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/access-denied-exception
  take form access-denied-exception

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/get-map-sprites-response
  take form get-map-sprites-response

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/resource-not-found-exception
  take form resource-not-found-exception

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/throttling-exception
  take form throttling-exception

load @nerdbond/snow/amazonaws.com/location/2020-11-19/base/validation-exception
  take form validation-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-map-sprites, name <GetMapSprites>
  deed get
  note <Retrieves the sprite sheet corresponding to a map resource. The sprite sheet is a PNG image paired with a JSON document describing the offsets of individual icons that will be displayed on a rendered map.>

  take file-name
    like native-string
    note <The name of the sprite ﬁle. Use the following ﬁle names for the sprite sheet:

- `sprites.png`

- `sprites@2x.png` for high pixel density displays

For the JSON document contain image offsets. Use the following ﬁle names:

- `sprites.json`

- `sprites@2x.json` for high pixel density displays>
  take map-name
    like native-string
    note <The map resource associated with the sprite ﬁle.>

  line </maps/v0/maps/{map-name}/sprites/{file-name}>

  loot 200
    seed json, like get-map-sprites-response
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like validation-exception
    note <ValidationException>
  loot 484
    seed json, like throttling-exception
    note <ThrottlingException>
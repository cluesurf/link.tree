load @textsurf/snow/base/native-string
  take form native-string

boot list-template-versions, name <ListTemplateVersions>
  deed get
  note <Retrieves information about all the versions of a specific message template.>

  take template-name
    like native-string
    note <The name of the message template. A template name must start with an alphanumeric character and can contain a maximum of 128 characters. The characters can be alphanumeric characters, underscores (_), or hyphens (-). Template names are case sensitive.>
  take template-type
    like native-string
    note <The type of channel that the message template is designed for. Valid values are: EMAIL, PUSH, SMS, and VOICE.>
  take next-token
    like native-string
    note <The string that specifies which page of results to return in a paginated response. This parameter is not supported for application, campaign, and journey metrics.>
    void take
  take page-size
    like native-string
    note <The maximum number of items to include in each page of a paginated response. This parameter is not supported for application, campaign, and journey metrics.>
    void take

  line </v1/templates/{template-name}/{template-type}/versions>

  find <next-token>, loan next-token
  find <page-size>, loan page-size

  loot 200
    seed json, like list-template-versions-response
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 482
    seed json, like payload-too-large-exception
    note <PayloadTooLargeException>
  loot 483
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 484
    seed json, like not-found-exception
    note <NotFoundException>
  loot 485
    seed json, like method-not-allowed-exception
    note <MethodNotAllowedException>
  loot 486
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
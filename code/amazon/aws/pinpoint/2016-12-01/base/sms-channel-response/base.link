
load @tunebond/snow/base/boolean
  take form boolean

load @tunebond/snow/base/integer
  take form integer

load @tunebond/snow/base/string
  take form string

form sms-channel-response, name <SMSChannelResponse>
  note <Provides information about the status and settings of the SMS channel for an application.>
  take application-id, name <ApplicationId>
    like string
    void take
    note <The unique identifier for the application that the SMS channel applies to.>
  take creation-date, name <CreationDate>
    like string
    void take
    note <The date and time, in ISO 8601 format, when the SMS channel was enabled.>
  take enabled, name <Enabled>
    like boolean
    void take
    note <Specifies whether the SMS channel is enabled for the application.>
  take has-credential, name <HasCredential>
    like boolean
    void take
    note <(Not used) This property is retained only for backward compatibility.>
  take id, name <Id>
    like string
    void take
    note <(Deprecated) An identifier for the SMS channel. This property is retained only for backward compatibility.>
  take is-archived, name <IsArchived>
    like boolean
    void take
    note <Specifies whether the SMS channel is archived.>
  take last-modified-by, name <LastModifiedBy>
    like string
    void take
    note <The user who last modified the SMS channel.>
  take last-modified-date, name <LastModifiedDate>
    like string
    void take
    note <The date and time, in ISO 8601 format, when the SMS channel was last modified.>
  take platform, name <Platform>
    like string
    note <The type of messaging or notification platform for the channel. For the SMS channel, this value is SMS.>
  take promotional-messages-per-second, name <PromotionalMessagesPerSecond>
    like integer
    void take
    note <The maximum number of promotional messages that you can send through the SMS channel each second.>
  take sender-id, name <SenderId>
    like string
    void take
    note <The identity that displays on recipients' devices when they receive messages from the SMS channel.>
  take short-code, name <ShortCode>
    like string
    void take
    note <The registered short code to use when you send messages through the SMS channel.>
  take transactional-messages-per-second, name <TransactionalMessagesPerSecond>
    like integer
    void take
    note <The maximum number of transactional messages that you can send through the SMS channel each second.>
  take version, name <Version>
    like integer
    void take
    note <The current version of the SMS channel.>
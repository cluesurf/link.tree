form import-job-response, name <ImportJobResponse>
  note <Provides information about the status and settings of a job that imports endpoint definitions from one or more files. The files can be stored in an Amazon Simple Storage Service (Amazon S3) bucket or uploaded directly from a computer by using the Amazon Pinpoint console.>
  take application-id, name <ApplicationId>
    like string
    note <The unique identifier for the application that's associated with the import job.>
  take completed-pieces, name <CompletedPieces>
    like integer
    void take
    note <The number of pieces that were processed successfully (completed) by the import job, as of the time of the request.>
  take completion-date, name <CompletionDate>
    like string
    void take
    note <The date, in ISO 8601 format, when the import job was completed.>
  take creation-date, name <CreationDate>
    like string
    note <The date, in ISO 8601 format, when the import job was created.>
  take definition, name <Definition>
    like import-job-resource
    note <The resource settings that apply to the import job.>
  take failed-pieces, name <FailedPieces>
    like integer
    void take
    note <The number of pieces that weren't processed successfully (failed) by the import job, as of the time of the request.>
  take failures, name <Failures>
    like list-of-string
    void take
    note <An array of entries, one for each of the first 100 entries that weren't processed successfully (failed) by the import job, if any.>
  take id, name <Id>
    like string
    note <The unique identifier for the import job.>
  take job-status, name <JobStatus>
    like job-status
    note <The status of the import job. The job status is FAILED if Amazon Pinpoint wasn't able to process one or more pieces in the job.>
  take total-failures, name <TotalFailures>
    like integer
    void take
    note <The total number of endpoint definitions that weren't processed successfully (failed) by the import job, typically because an error, such as a syntax error, occurred.>
  take total-pieces, name <TotalPieces>
    like integer
    void take
    note <The total number of pieces that must be processed to complete the import job. Each piece consists of an approximately equal portion of the endpoint definitions that are part of the import job.>
  take total-processed, name <TotalProcessed>
    like integer
    void take
    note <The total number of endpoint definitions that were processed by the import job.>
  take type, name <Type>
    like string
    note <The job type. This value is IMPORT for import jobs.>
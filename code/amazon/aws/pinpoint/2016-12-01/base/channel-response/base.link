
load @drumwork/snow/base/boolean
  take form boolean

load @drumwork/snow/base/integer
  take form integer

load @drumwork/snow/base/string
  take form string

form channel-response, name <ChannelResponse>
  note <Provides information about the general settings and status of a channel for an application.>
  take application-id, name <ApplicationId>
    like string
    void take
    note <The unique identifier for the application.>
  take creation-date, name <CreationDate>
    like string
    void take
    note <The date and time, in ISO 8601 format, when the channel was enabled.>
  take enabled, name <Enabled>
    like boolean
    void take
    note <Specifies whether the channel is enabled for the application.>
  take has-credential, name <HasCredential>
    like boolean
    void take
    note <(Not used) This property is retained only for backward compatibility.>
  take id, name <Id>
    like string
    void take
    note <(Deprecated) An identifier for the channel. This property is retained only for backward compatibility.>
  take is-archived, name <IsArchived>
    like boolean
    void take
    note <Specifies whether the channel is archived.>
  take last-modified-by, name <LastModifiedBy>
    like string
    void take
    note <The user who last modified the channel.>
  take last-modified-date, name <LastModifiedDate>
    like string
    void take
    note <The date and time, in ISO 8601 format, when the channel was last modified.>
  take version, name <Version>
    like integer
    void take
    note <The current version of the channel.>

load @nerdbond/snow/base/boolean
  take form boolean

load @nerdbond/snow/base/integer
  take form integer

load @nerdbond/snow/base/string
  take form string

form baidu-channel-response, name <BaiduChannelResponse>
  note <Provides information about the status and settings of the Baidu (Baidu Cloud Push) channel for an application.>
  take application-id, name <ApplicationId>
    like string
    void take
    note <The unique identifier for the application that the Baidu channel applies to.>
  take creation-date, name <CreationDate>
    like string
    void take
    note <The date and time when the Baidu channel was enabled.>
  take credential, name <Credential>

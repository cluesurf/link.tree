
load @treesurf/snow/base/integer
  take form integer

load @treesurf/snow/base/string
  take form string

form session, name <Session>
  note <Provides information about a session.>
  take duration, name <Duration>
    like integer
    void take
    note <The duration of the session, in milliseconds.>
  take id, name <Id>
    like string
    note <The unique identifier for the session.>
  take start-timestamp, name <StartTimestamp>
    like string
    note <The date and time when the session began.>
  take stop-timestamp, name <StopTimestamp>
    like string
    void take
    note <The date and time when the session ended.>
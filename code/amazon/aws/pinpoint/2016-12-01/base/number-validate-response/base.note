
load @termsurf/snow/base/integer
  take form integer

load @termsurf/snow/base/string
  take form string

form number-validate-response, name <NumberValidateResponse>
  note <Provides information about a phone number.>
  take carrier, name <Carrier>
    like string
    void take
    note <The carrier or service provider that the phone number is currently registered with. In some countries and regions, this value may be the carrier or service provider that the phone number was originally registered with.>
  take city, name <City>
    like string
    void take
    note <The name of the city where the phone number was originally registered.>
  take cleansed-phone-number-e164, name <CleansedPhoneNumberE164>
    like string
    void take
    note <The cleansed phone number, in E.164 format, for the location where the phone number was originally registered.>
  take cleansed-phone-number-national, name <CleansedPhoneNumberNational>
    like string
    void take
    note <The cleansed phone number, in the format for the location where the phone number was originally registered.>
  take country, name <Country>
    like string
    void take
    note <The name of the country or region where the phone number was originally registered.>
  take country-code-iso2, name <CountryCodeIso2>
    like string
    void take
    note <The two-character code, in ISO 3166-1 alpha-2 format, for the country or region where the phone number was originally registered.>
  take country-code-numeric, name <CountryCodeNumeric>
    like string
    void take
    note <The numeric code for the country or region where the phone number was originally registered.>
  take county, name <County>
    like string
    void take
    note <The name of the county where the phone number was originally registered.>
  take original-country-code-iso2, name <OriginalCountryCodeIso2>
    like string
    void take
    note <The two-character code, in ISO 3166-1 alpha-2 format, that was sent in the request body.>
  take original-phone-number, name <OriginalPhoneNumber>
    like string
    void take
    note <The phone number that was sent in the request body.>
  take phone-type, name <PhoneType>
    like string
    void take
    note <The description of the phone type. Valid values are: MOBILE, LANDLINE, VOIP, INVALID, PREPAID, and OTHER.>
  take phone-type-code, name <PhoneTypeCode>
    like integer
    void take
    note <The phone type, represented by an integer. Valid values are: 0 (mobile), 1 (landline), 2 (VoIP), 3 (invalid), 4 (other), and 5 (prepaid).>
  take timezone, name <Timezone>
    like string
    void take
    note <The time zone for the location where the phone number was originally registered.>
  take zip-code, name <ZipCode>
    like string
    void take
    note <The postal or ZIP code for the location where the phone number was originally registered.>
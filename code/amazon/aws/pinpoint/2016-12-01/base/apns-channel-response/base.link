
load @drumwork/snow/base/boolean
  take form boolean

load @drumwork/snow/base/integer
  take form integer

load @drumwork/snow/base/string
  take form string

form apns-channel-response, name <APNSChannelResponse>
  note <Provides information about the status and settings of the APNs (Apple Push Notification service) channel for an application.>
  take application-id, name <ApplicationId>
    like string
    void take
    note <The unique identifier for the application that the APNs channel applies to.>
  take creation-date, name <CreationDate>
    like string
    void take
    note <The date and time when the APNs channel was enabled.>
  take default-authentication-method, name <DefaultAuthenticationMethod>
    like string
    void take
    note <The default authentication method that Amazon Pinpoint uses to authenticate with APNs for this channel, key or certificate.>
  take enabled, name <Enabled>
    like boolean
    void take
    note <Specifies whether the APNs channel is enabled for the application.>
  take has-credential, name <HasCredential>
    like boolean
    void take
    note <(Not used) This property is retained only for backward compatibility.>
  take has-token-key, name <HasTokenKey>
    like boolean
    void take
    note <Specifies whether the APNs channel is configured to communicate with APNs by using APNs tokens. To provide an authentication key for APNs tokens, set the TokenKey property of the channel.>
  take id, name <Id>
    like string
    void take
    note <(Deprecated) An identifier for the APNs channel. This property is retained only for backward compatibility.>
  take is-archived, name <IsArchived>
    like boolean
    void take
    note <Specifies whether the APNs channel is archived.>
  take last-modified-by, name <LastModifiedBy>
    like string
    void take
    note <The user who last modified the APNs channel.>
  take last-modified-date, name <LastModifiedDate>
    like string
    void take
    note <The date and time when the APNs channel was last modified.>
  take platform, name <Platform>
    like string
    note <The type of messaging or notification platform for the channel. For the APNs channel, this value is APNS.>
  take version, name <Version>
    like integer
    void take
    note <The current version of the APNs channel.>
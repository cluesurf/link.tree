
load @drumwork/snow/base/string
  take form string

form authorize-data-share-message, name <AuthorizeDataShareMessage>
  take data-share-arn, name <DataShareArn>
    like string
    note <The Amazon Resource Name (ARN) of the datashare that producers are to authorize sharing for.>
  take consumer-identifier, name <ConsumerIdentifier>
    like string
    note <The identifier of the data consumer that is authorized to access the datashare. This identifier is an AWS account ID.>
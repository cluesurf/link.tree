
load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/create-snapshot-copy-grant-result
  take form create-snapshot-copy-grant-result

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/dependent-service-request-throttling-fault
  take form dependent-service-request-throttling-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/invalid-tag-fault
  take form invalid-tag-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/limit-exceeded-fault
  take form limit-exceeded-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/snapshot-copy-grant-already-exists-fault
  take form snapshot-copy-grant-already-exists-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/snapshot-copy-grant-quota-exceeded-fault
  take form snapshot-copy-grant-quota-exceeded-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/tag

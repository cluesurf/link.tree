
load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/invalid-schedule-fault
  take form invalid-schedule-fault

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/invalid-scheduled-action-fault
  take form invalid-scheduled-action-fault

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/pause-cluster-message
  take form pause-cluster-message

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/resize-cluster-message
  take form resize-cluster-message

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/resume-cluster-message
  take form resume-cluster-message

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/scheduled-action
  take form scheduled-action

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/scheduled-action-not-found-fault
  take form scheduled-action-not-found-fault

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/scheduled-action-type-unsupported-fault
  take form scheduled-action-type-unsupported-fault

load @drumwork/snow/amazonaws.com/redshift/2012-12-01/base/unauthorized-operation
  take form unauthorized-operation


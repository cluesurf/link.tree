
load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/cluster-not-found-fault
  take form cluster-not-found-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/insufficient-cluster-capacity-fault
  take form insufficient-cluster-capacity-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/invalid-cluster-state-fault
  take form invalid-cluster-state-fault

load @nerdbond/snow/amazonaws.com/redshift/2012-12-01/base/resume-cluster-result
  take form resume-cluster-result

load @nerdbond/snow/base/native-string
  take form native-string

boot post-resume-cluster, name <POST_ResumeCluster>
  deed post
  note <Resumes a paused cluster.>

  take action
    like native-string
  take version
    like native-string
  take resume-cluster-message, like resume-cluster-message
    void take

  line </#Action=ResumeCluster>

  find <Action>, loan action
  find <Version>, loan version

  seed xml, loan resume-cluster-message

  loot 200
    seed xml, like resume-cluster-result
    note <Success>
  loot 480
    seed xml, like cluster-not-found-fault
    note <ClusterNotFoundFault>
  loot 481
    seed xml, like invalid-cluster-state-fault
    note <InvalidClusterStateFault>
  loot 482
    seed xml, like insufficient-cluster-capacity-fault
    note <InsufficientClusterCapacityFault>
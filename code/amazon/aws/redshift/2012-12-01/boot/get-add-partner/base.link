
load @tunebond/snow/amazonaws.com/redshift/2012-12-01/base/cluster-not-found-fault
  take form cluster-not-found-fault

load @tunebond/snow/amazonaws.com/redshift/2012-12-01/base/partner-integration-output-message
  take form partner-integration-output-message

load @tunebond/snow/amazonaws.com/redshift/2012-12-01/base/partner-not-found-fault
  take form partner-not-found-fault

load @tunebond/snow/amazonaws.com/redshift/2012-12-01/base/unauthorized-partner-integration-fault
  take form unauthorized-partner-integration-fault

load @tunebond/snow/base/native-string
  take form native-string

boot get-add-partner, name <GET_AddPartner>
  deed get
  note <Adds a partner integration to a cluster. This operation authorizes a partner to push status updates for the specified database. To complete the integration, you also set up the integration on the partner website.>

  take account-id
    like native-string
    note <The Amazon Web Services account ID that owns the cluster.>
  take cluster-identifier
    like native-string
    note <The cluster identifier of the cluster that receives data from the partner.>
  take database-name
    like native-string
    note <The name of the database that receives data from the partner.>
  take partner-name
    like native-string
    note <The name of the partner that is authorized to send data.>
  take action
    like native-string
  take version
    like native-string

  line </#Action=AddPartner>

  find <AccountId>, loan account-id
  find <ClusterIdentifier>, loan cluster-identifier
  find <DatabaseName>, loan database-name
  find <PartnerName>, loan partner-name
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like partner-integration-output-message
    note <Success>
  loot 480
    seed xml, like partner-not-found-fault
    note <PartnerNotFoundFault>
  loot 481
    seed xml, like cluster-not-found-fault
    note <ClusterNotFoundFault>
  loot 482
    seed xml, like unauthorized-partner-integration-fault
    note <UnauthorizedPartnerIntegrationFault>
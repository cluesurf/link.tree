
load @wavebond/snow/amazonaws.com/glue/2017-03-31/base/entity-not-found-exception
  take form entity-not-found-exception

load @wavebond/snow/amazonaws.com/glue/2017-03-31/base/get-connections-response
  take form get-connections-response

load @wavebond/snow/amazonaws.com/glue/2017-03-31/base/glue-encryption-exception
  take form glue-encryption-exception

load @wavebond/snow/amazonaws.com/glue/2017-03-31/base/invalid-input-exception
  take form invalid-input-exception

load @wavebond/snow/amazonaws.com/glue/2017-03-31/base/operation-timeout-exception
  take form operation-timeout-exception

load @wavebond/snow/base/native-string
  take form native-string

boot get-connections, name <GetConnections>
  deed post
  note <Retrieves a list of connection definitions from the Data Catalog.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take get-connections-request, like get-connections-request

  line </#X-Amz-Target=AWSGlue.GetConnections>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan get-connections-request

  loot 200
    seed json, like get-connections-response
    note <Success>
  loot 480
    seed json, like entity-not-found-exception
    note <EntityNotFoundException>
  loot 481
    seed json, like operation-timeout-exception
    note <OperationTimeoutException>
  loot 482
    seed json, like invalid-input-exception
    note <InvalidInputException>
  loot 483
    seed json, like glue-encryption-exception
    note <GlueEncryptionException>
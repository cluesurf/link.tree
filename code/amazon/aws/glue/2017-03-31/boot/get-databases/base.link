
load @nerdbond/snow/amazonaws.com/glue/2017-03-31/base/get-databases-response
  take form get-databases-response

load @nerdbond/snow/amazonaws.com/glue/2017-03-31/base/glue-encryption-exception
  take form glue-encryption-exception

load @nerdbond/snow/amazonaws.com/glue/2017-03-31/base/internal-service-exception
  take form internal-service-exception

load @nerdbond/snow/amazonaws.com/glue/2017-03-31/base/invalid-input-exception
  take form invalid-input-exception

load @nerdbond/snow/amazonaws.com/glue/2017-03-31/base/operation-timeout-exception
  take form operation-timeout-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-databases, name <GetDatabases>
  deed post
  note <Retrieves all databases defined in a given Data Catalog.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take get-databases-request, like get-databases-request

  line </#X-Amz-Target=AWSGlue.GetDatabases>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan get-databases-request

  loot 200
    seed json, like get-databases-response
    note <Success>
  loot 480
    seed json, like invalid-input-exception
    note <InvalidInputException>
  loot 481
    seed json, like internal-service-exception
    note <InternalServiceException>
  loot 482
    seed json, like operation-timeout-exception
    note <OperationTimeoutException>
  loot 483
    seed json, like glue-encryption-exception
    note <GlueEncryptionException>
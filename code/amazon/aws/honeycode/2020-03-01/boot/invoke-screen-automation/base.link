  take form automation-execution-timeout-exception

load @nerdbond/snow/amazonaws.com/honeycode/2020-03-01/base/internal-server-exception
  take form internal-server-exception

load @nerdbond/snow/amazonaws.com/honeycode/2020-03-01/base/invoke-screen-automation-body
  take form invoke-screen-automation-body

load @nerdbond/snow/amazonaws.com/honeycode/2020-03-01/base/invoke-screen-automation-result
  take form invoke-screen-automation-result

load @nerdbond/snow/amazonaws.com/honeycode/2020-03-01/base/request-timeout-exception

boot create-notification, name <CreateNotification>
  note <Creates a notification. You must create the budget before you create the associated notification.>

  take x-amz-target
    like native-string
  take create-notification-request, like create-notification-request

  line </#X-Amz-Target=AWSBudgetServiceGateway.CreateNotification>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan create-notification-request

  loot 200
    seed json, like create-notification-response
    note <Success>
  loot 480
    seed json, like internal-error-exception
    note <InternalErrorException>
  loot 481
    seed json, like invalid-parameter-exception
    note <InvalidParameterException>
  loot 482
    seed json, like not-found-exception
    note <NotFoundException>
  loot 483
    seed json, like creation-limit-exceeded-exception
    note <CreationLimitExceededException>
  loot 484
    seed json, like duplicate-record-exception
    note <DuplicateRecordException>
  loot 485
    seed json, like access-denied-exception
    note <AccessDeniedException>
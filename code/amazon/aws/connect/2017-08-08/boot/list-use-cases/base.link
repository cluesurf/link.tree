boot list-use-cases, name <ListUseCases>
  deed get
  note <Lists the use cases.>

  take instance-id
    like native-string
    note <The identifier of the Amazon Connect instance. You can find the instanceId in the ARN of the instance.>
  take integration-association-id
    like native-string
    note <The identifier for the integration association.>
  take next-token
    like native-string
    note <The token for the next set of results. Use the value returned in the previous response in the next request to retrieve the next set of results.>
    void take
  take max-results
    like native-integer
    note <The maximum number of results to return per page.>
    void take
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </instance/{instance-id}/integration-associations/{integration-association-id}/use-cases>

  find <nextToken>, loan next-token
  find <maxResults>, loan max-results
  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-use-cases-response
    note <Success>

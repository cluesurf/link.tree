
load @tunebond/snow/amazonaws.com/connect/2017-08-08/base/contact-id
  take form contact-id

load @tunebond/snow/amazonaws.com/connect/2017-08-08/base/instance-id
  take form instance-id

form resume-contact-recording-request, name <ResumeContactRecordingRequest>
  take instance-id, name <InstanceId>
    like instance-id
    note <The identifier of the Amazon Connect instance. You can find the instanceId in the ARN of the instance.>
  take contact-id, name <ContactId>


load @wavebond/snow/amazonaws.com/appconfig/2019-10-09/base/bad-request-exception
  take form bad-request-exception

load @wavebond/snow/amazonaws.com/appconfig/2019-10-09/base/deployment
  take form deployment

load @wavebond/snow/amazonaws.com/appconfig/2019-10-09/base/internal-server-exception
  take form internal-server-exception

load @wavebond/snow/amazonaws.com/appconfig/2019-10-09/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/base/native-integer
  take form native-integer

load @wavebond/snow/base/native-string
  take form native-string

boot get-deployment, name <GetDeployment>
  deed get
  note <Retrieve information about a configuration deployment.>

  take application-id
    like native-string
    note <The ID of the application that includes the deployment you want to get.>
  take environment-id
    like native-string
    note <The ID of the environment that includes the deployment you want to get.>
  take deployment-number
    like native-integer
    note <The sequence number of the deployment.>

  line </applications/{application-id}/environments/{environment-id}/deployments/{deployment-number}>

  loot 200
    seed json, like deployment
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like bad-request-exception
    note <BadRequestException>
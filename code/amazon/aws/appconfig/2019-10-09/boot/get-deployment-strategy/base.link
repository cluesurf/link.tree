
load @drumwork/snow/amazonaws.com/appconfig/2019-10-09/base/bad-request-exception
  take form bad-request-exception

load @drumwork/snow/amazonaws.com/appconfig/2019-10-09/base/deployment-strategy
  take form deployment-strategy

load @drumwork/snow/amazonaws.com/appconfig/2019-10-09/base/internal-server-exception
  take form internal-server-exception

load @drumwork/snow/amazonaws.com/appconfig/2019-10-09/base/resource-not-found-exception
  take form resource-not-found-exception

load @drumwork/snow/base/native-string
  take form native-string

boot get-deployment-strategy, name <GetDeploymentStrategy>
  deed get
  note <Retrieve information about a deployment strategy. A deployment strategy defines important criteria for rolling out your configuration to the designated targets. A deployment strategy includes: the overall duration required, a percentage of targets to receive the deployment during each interval, an algorithm that defines how percentage grows, and bake time.>

  take deployment-strategy-id
    like native-string
    note <The ID of the deployment strategy to get.>

  line </deploymentstrategies/{deployment-strategy-id}>

  loot 200
    seed json, like deployment-strategy
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like bad-request-exception
    note <BadRequestException>
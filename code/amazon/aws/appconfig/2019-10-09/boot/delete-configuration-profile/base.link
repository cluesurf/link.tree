
load @textsurf/snow/amazonaws.com/appconfig/2019-10-09/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/appconfig/2019-10-09/base/conflict-exception
  take form conflict-exception

load @textsurf/snow/amazonaws.com/appconfig/2019-10-09/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/appconfig/2019-10-09/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/base/native-string
  take form native-string

boot delete-configuration-profile, name <DeleteConfigurationProfile>
  deed delete
  note <Delete a configuration profile. Deleting a configuration profile does not delete a configuration from a host.>

  take application-id
    like native-string
    note <The application ID that includes the configuration profile you want to delete.>
  take configuration-profile-id
    like native-string
    note <The ID of the configuration profile you want to delete.>

  line </applications/{application-id}/configurationprofiles/{configuration-profile-id}>

  loot 204
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like conflict-exception
    note <ConflictException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like bad-request-exception
    note <BadRequestException>

load @tunebond/snow/base/integer
  take form integer

form customer-connector-info, name <CustomerConnectorInfo>
  note <Inventory data for installed discovery connectors.>
  take active-connectors, name <activeConnectors>
    like integer
    note <Number of active discovery connectors.>
  take healthy-connectors, name <healthyConnectors>
    like integer
    note <Number of healthy discovery connectors.>
  take black-listed-connectors, name <blackListedConnectors>
    like integer
    note <Number of blacklisted discovery connectors.>
  take shutdown-connectors, name <shutdownConnectors>
    like integer
    note <Number of discovery connectors with status SHUTDOWN,>
  take unhealthy-connectors, name <unhealthyConnectors>
    like integer
    note <Number of unhealthy discovery connectors.>
  take total-connectors, name <totalConnectors>
    like integer
    note <Total number of discovery connectors.>
  take unknown-connectors, name <unknownConnectors>
    like integer
    note <Number of unknown discovery connectors.>

load @termsurf/snow/base/integer
  take form integer

form customer-agent-info, name <CustomerAgentInfo>
  note <Inventory data for installed discovery agents.>
  take active-agents, name <activeAgents>
    like integer
    note <Number of active discovery agents.>
  take healthy-agents, name <healthyAgents>
    like integer
    note <Number of healthy discovery agents>
  take black-listed-agents, name <blackListedAgents>
    like integer
    note <Number of blacklisted discovery agents.>
  take shutdown-agents, name <shutdownAgents>
    like integer
    note <Number of discovery agents with status SHUTDOWN.>
  take unhealthy-agents, name <unhealthyAgents>
    like integer
    note <Number of unhealthy discovery agents.>
  take total-agents, name <totalAgents>
    like integer
    note <Total number of discovery agents.>
  take unknown-agents, name <unknownAgents>
    like integer
    note <Number of unknown discovery agents.>
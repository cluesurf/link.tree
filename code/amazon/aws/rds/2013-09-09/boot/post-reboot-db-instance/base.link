
load @nerdbond/snow/amazonaws.com/rds/2013-09-09/base/db-instance-not-found-fault
  take form db-instance-not-found-fault

load @nerdbond/snow/amazonaws.com/rds/2013-09-09/base/invalid-db-instance-state-fault
  take form invalid-db-instance-state-fault

load @nerdbond/snow/amazonaws.com/rds/2013-09-09/base/reboot-db-instance-result
  take form reboot-db-instance-result

load @nerdbond/snow/base/native-string
  take form native-string

boot post-reboot-db-instance, name <POST_RebootDBInstance>
  deed post
  take action
    like native-string
  take version
    like native-string
  take reboot-db-instance-message, like reboot-db-instance-message
    void take

  line </#Action=RebootDBInstance>

  find <Action>, loan action
  find <Version>, loan version

  seed xml, loan reboot-db-instance-message

  loot 200
    seed xml, like reboot-db-instance-result
    note <Success>
  loot 480
    seed xml, like invalid-db-instance-state-fault
    note <InvalidDBInstanceStateFault>
  loot 481
    seed xml, like db-instance-not-found-fault
    note <DBInstanceNotFoundFault>
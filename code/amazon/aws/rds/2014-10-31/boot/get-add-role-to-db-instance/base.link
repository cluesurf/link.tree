
load @textsurf/snow/amazonaws.com/rds/2014-10-31/base/db-instance-not-found-fault
  take form db-instance-not-found-fault

load @textsurf/snow/amazonaws.com/rds/2014-10-31/base/db-instance-role-already-exists-fault
  take form db-instance-role-already-exists-fault

load @textsurf/snow/amazonaws.com/rds/2014-10-31/base/db-instance-role-quota-exceeded-fault
  take form db-instance-role-quota-exceeded-fault

load @textsurf/snow/amazonaws.com/rds/2014-10-31/base/invalid-db-instance-state-fault
  take form invalid-db-instance-state-fault

load @textsurf/snow/base/native-string
  take form native-string

boot get-add-role-to-db-instance, name <GET_AddRoleToDBInstance>
  deed get
  note <Associates an Amazon Web Services Identity and Access Management (IAM) role with a DB instance.

To add a role to a DB instance, the status of the DB instance must be `available`.>

  take db-instance-identifier
    like native-string
    note <The name of the DB instance to associate the IAM role with.>
  take role-arn
    like native-string
    note <The Amazon Resource Name (ARN) of the IAM role to associate with the DB instance, for example `arn:aws:iam::123456789012:role/AccessRole`.>
  take feature-name
    like native-string
    note <The name of the feature for the DB instance that the IAM role is to be associated with. For the list of supported feature names, see DBEngineVersion.>
  take action
    like native-string
  take version
    like native-string

  line </#Action=AddRoleToDBInstance>

  find <DBInstanceIdentifier>, loan db-instance-identifier
  find <RoleArn>, loan role-arn
  find <FeatureName>, loan feature-name
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    note <Success>
  loot 480
    seed xml, like db-instance-not-found-fault
    note <DBInstanceNotFoundFault>
  loot 481
    seed xml, like db-instance-role-already-exists-fault
    note <DBInstanceRoleAlreadyExistsFault>
  loot 482
    seed xml, like invalid-db-instance-state-fault
    note <InvalidDBInstanceStateFault>
  loot 483
    seed xml, like db-instance-role-quota-exceeded-fault
    note <DBInstanceRoleQuotaExceededFault>
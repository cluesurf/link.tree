
load @drumwork/snow/base/string
  take form string

form domain-membership, name <DomainMembership>
  note <An Active Directory Domain membership record associated with the DB instance or cluster.>
  take domain, name <Domain>
    like string
    void take
    note <The identifier of the Active Directory Domain.>
  take status, name <Status>
    like string
    void take
    note <The status of the Active Directory Domain membership for the DB instance or cluster. Values include joined, pending-join, failed, and so on.>
  take fqdn, name <FQDN>
    like string
    void take
    note <The fully qualified domain name of the Active Directory Domain.>
  take iam-role-name, name <IAMRoleName>
    like string
    void take
    note <The name of the IAM role to be used when making API calls to the Directory Service.>
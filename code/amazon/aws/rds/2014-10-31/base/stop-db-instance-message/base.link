
load @drumwork/snow/base/string
  take form string

form stop-db-instance-message, name <StopDBInstanceMessage>
  take db-instance-identifier, name <DBInstanceIdentifier>
    like string
    note <The user-supplied instance identifier.>
  take db-snapshot-identifier, name <DBSnapshotIdentifier>
    like string
    void take
    note <The user-supplied instance identifier of the DB Snapshot created immediately before the DB instance is stopped.>

load @nerdbond/snow/amazonaws.com/rds/2013-02-12/base/db-instance-not-found-fault
  take form db-instance-not-found-fault

load @nerdbond/snow/amazonaws.com/rds/2013-02-12/base/invalid-db-instance-state-fault
  take form invalid-db-instance-state-fault

load @nerdbond/snow/amazonaws.com/rds/2013-02-12/base/promote-read-replica-result
  take form promote-read-replica-result

load @nerdbond/snow/base/native-integer
  take form native-integer

load @nerdbond/snow/base/native-string
  take form native-string

boot get-promote-read-replica, name <GET_PromoteReadReplica>
  deed get
  take db-instance-identifier
    like native-string
  take backup-retention-period
    like native-integer
    void take
  take preferred-backup-window
    like native-string
    void take
  take action
    like native-string
  take version
    like native-string

  line </#Action=PromoteReadReplica>

  find <DBInstanceIdentifier>, loan db-instance-identifier
  find <BackupRetentionPeriod>, loan backup-retention-period
  find <PreferredBackupWindow>, loan preferred-backup-window
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like promote-read-replica-result
    note <Success>
  loot 480
    seed xml, like invalid-db-instance-state-fault
    note <InvalidDBInstanceStateFault>
  loot 481
    seed xml, like db-instance-not-found-fault
    note <DBInstanceNotFoundFault>
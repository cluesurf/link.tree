
load @textsurf/snow/amazonaws.com/rds/2013-02-12/base/create-db-snapshot-result
  take form create-db-snapshot-result

load @textsurf/snow/amazonaws.com/rds/2013-02-12/base/db-instance-not-found-fault
  take form db-instance-not-found-fault

load @textsurf/snow/amazonaws.com/rds/2013-02-12/base/db-snapshot-already-exists-fault
  take form db-snapshot-already-exists-fault

load @textsurf/snow/amazonaws.com/rds/2013-02-12/base/invalid-db-instance-state-fault
  take form invalid-db-instance-state-fault

load @textsurf/snow/amazonaws.com/rds/2013-02-12/base/snapshot-quota-exceeded-fault
  take form snapshot-quota-exceeded-fault

load @textsurf/snow/base/native-string
  take form native-string

boot get-create-db-snapshot, name <GET_CreateDBSnapshot>
  deed get
  take db-snapshot-identifier
    like native-string
  take db-instance-identifier
    like native-string
  take action
    like native-string
  take version
    like native-string

  line </#Action=CreateDBSnapshot>

  find <DBSnapshotIdentifier>, loan db-snapshot-identifier
  find <DBInstanceIdentifier>, loan db-instance-identifier
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like create-db-snapshot-result
    note <Success>
  loot 480
    seed xml, like db-snapshot-already-exists-fault
    note <DBSnapshotAlreadyExistsFault>
  loot 481
    seed xml, like invalid-db-instance-state-fault
    note <InvalidDBInstanceStateFault>
  loot 482
    seed xml, like db-instance-not-found-fault
    note <DBInstanceNotFoundFault>
  loot 483
    seed xml, like snapshot-quota-exceeded-fault
    note <SnapshotQuotaExceededFault>
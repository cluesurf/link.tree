
load @treesurf/snow/amazonaws.com/rds/2013-01-10/base/db-parameter-group-name-message
  take form db-parameter-group-name-message

load @treesurf/snow/amazonaws.com/rds/2013-01-10/base/db-parameter-group-not-found-fault
  take form db-parameter-group-not-found-fault

load @treesurf/snow/amazonaws.com/rds/2013-01-10/base/invalid-db-parameter-group-state-fault
  take form invalid-db-parameter-group-state-fault

load @treesurf/snow/amazonaws.com/rds/2013-01-10/base/parameter
  take form parameter

load @treesurf/snow/base/list
  take form list

load @treesurf/snow/base/native-string
  take form native-string

boot get-modify-db-parameter-group, name <GET_ModifyDBParameterGroup>
  deed get
  take db-parameter-group-name
    like native-string
  take parameters
    like list
      like parameter
  take action
    like native-string
  take version
    like native-string

  line </#Action=ModifyDBParameterGroup>

  find <DBParameterGroupName>, loan db-parameter-group-name
  find <Parameters>, loan parameters
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like db-parameter-group-name-message
    note <Success>
  loot 480
    seed xml, like db-parameter-group-not-found-fault
    note <DBParameterGroupNotFoundFault>
  loot 481
    seed xml, like invalid-db-parameter-group-state-fault
    note <InvalidDBParameterGroupStateFault>
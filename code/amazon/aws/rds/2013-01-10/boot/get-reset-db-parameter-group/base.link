
load @textsurf/snow/amazonaws.com/rds/2013-01-10/base/db-parameter-group-name-message
  take form db-parameter-group-name-message

load @textsurf/snow/amazonaws.com/rds/2013-01-10/base/db-parameter-group-not-found-fault
  take form db-parameter-group-not-found-fault

load @textsurf/snow/amazonaws.com/rds/2013-01-10/base/invalid-db-parameter-group-state-fault
  take form invalid-db-parameter-group-state-fault

load @textsurf/snow/amazonaws.com/rds/2013-01-10/base/parameter
  take form parameter

load @textsurf/snow/base/list
  take form list

load @textsurf/snow/base/native-boolean
  take form native-boolean

load @textsurf/snow/base/native-string
  take form native-string

boot get-reset-db-parameter-group, name <GET_ResetDBParameterGroup>
  deed get
  take db-parameter-group-name
    like native-string
  take reset-all-parameters
    like native-boolean
    void take
  take parameters
    like list
      like parameter
    void take
  take action
    like native-string
  take version
    like native-string

  line </#Action=ResetDBParameterGroup>

  find <DBParameterGroupName>, loan db-parameter-group-name
  find <ResetAllParameters>, loan reset-all-parameters
  find <Parameters>, loan parameters
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like db-parameter-group-name-message
    note <Success>
  loot 480
    seed xml, like invalid-db-parameter-group-state-fault
    note <InvalidDBParameterGroupStateFault>
  loot 481
    seed xml, like db-parameter-group-not-found-fault
    note <DBParameterGroupNotFoundFault>
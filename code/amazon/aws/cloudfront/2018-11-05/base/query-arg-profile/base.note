
load @termsurf/snow/base/string
  take form string

form query-arg-profile, name <QueryArgProfile>
  note <Query argument-profile mapping for field-level encryption.>
  take query-arg, name <QueryArg>
    like string
    note <Query argument for field-level encryption query argument-profile mapping.>
  take profile-id, name <ProfileId>


load @termsurf/snow/base/string
  take form string

form remove-source-identifier-from-subscription-message, name <RemoveSourceIdentifierFromSubscriptionMessage>
  take subscription-name, name <SubscriptionName>
    like string
    note <The name of the event notification subscription you want to remove a source identifier from.>
  take source-identifier, name <SourceIdentifier>
    like string
    note <The source identifier to be removed from the subscription, such as the **DB instance identifier** for a DB instance or the name of a security group.>
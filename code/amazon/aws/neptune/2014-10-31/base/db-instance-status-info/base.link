
load @treesurf/snow/base/boolean
  take form boolean

load @treesurf/snow/base/string
  take form string

form db-instance-status-info, name <DBInstanceStatusInfo>
  note <Provides a list of status information for a DB instance.>
  take status-type, name <StatusType>
    like string
    void take
    note <This value is currently "read replication.">
  take normal, name <Normal>
    like boolean
    void take
    note <Boolean value that is true if the instance is operating normally, or false if the instance is in an error state.>
  take status, name <Status>
    like string
    void take
    note <Status of the DB instance. For a StatusType of read replica, the values can be replicating, error, stopped, or terminated.>
  take message, name <Message>
    like string
    void take
    note <Details of the error if there is an error for the instance. If the instance is not in an error state, this value is blank.>

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/detect-phi-response
  take form detect-phi-response

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/invalid-encoding-exception
  take form invalid-encoding-exception

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/invalid-request-exception
  take form invalid-request-exception

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/service-unavailable-exception
  take form service-unavailable-exception

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/text-size-limit-exceeded-exception
  take form text-size-limit-exceeded-exception

load @textsurf/snow/amazonaws.com/comprehendmedical/2018-10-30/base/too-many-requests-exception
  take form too-many-requests-exception

load @textsurf/snow/base/native-string
  take form native-string

boot detect-phi, name <DetectPHI>
  deed post
  note <Inspects the clinical text for protected health information (PHI) entities and returns the entity category, location, and confidence score for each entity. Amazon Comprehend Medical only detects entities in English language texts.>

  take x-amz-target
    like native-string
  take detect-phi-request, like detect-phi-request

  line </#X-Amz-Target=ComprehendMedical_20181030.DetectPHI>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan detect-phi-request

  loot 200
    seed json, like detect-phi-response
    note <Success>
  loot 480
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 481
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 482
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 483
    seed json, like invalid-request-exception
    note <InvalidRequestException>
  loot 484
    seed json, like invalid-encoding-exception
    note <InvalidEncodingException>
  loot 485
    seed json, like text-size-limit-exceeded-exception
    note <TextSizeLimitExceededException>
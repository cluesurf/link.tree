
load @treesurf/snow/base/string
  take form string

form table-to-reload, name <TableToReload>
  note <Provides the name of the schema and table to be reloaded.>
  take schema-name, name <SchemaName>
    like string
    note <The schema name of the table to be reloaded.>
  take table-name, name <TableName>


load @textsurf/snow/base/string
  take form string

form move-replication-task-message, name <MoveReplicationTaskMessage>
  note <>
  take replication-task-arn, name <ReplicationTaskArn>
    like string
    note <The Amazon Resource Name (ARN) of the task that you want to move.>
  take target-replication-instance-arn, name <TargetReplicationInstanceArn>
    like string


load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/bad-request-exception
  take form bad-request-exception

load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/forbidden-exception
  take form forbidden-exception

load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/internal-server-error-exception
  take form internal-server-error-exception

load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/list-application-dependencies-response
  take form list-application-dependencies-response

load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/not-found-exception
  take form not-found-exception

load @textsurf/snow/amazonaws.com/serverlessrepo/2017-09-08/base/too-many-requests-exception
  take form too-many-requests-exception

load @textsurf/snow/base/native-integer
  take form native-integer

load @textsurf/snow/base/native-string
  take form native-string

boot list-application-dependencies, name <ListApplicationDependencies>
  deed get
  note <Retrieves the list of applications nested in the containing application.>

  take application-id
    like native-string
    note <The Amazon Resource Name (ARN) of the application.>
  take max-items
    like native-integer
    note <The total number of items to return.>
    void take
  take next-token
    like native-string
    note <A token to specify where to start paginating.>
    void take
  take semantic-version
    like native-string
    note <The semantic version of the application to get.>
    void take
  take max-items
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </applications/{application-id}/dependencies>

  find <maxItems>, loan max-items
  find <nextToken>, loan next-token
  find <semanticVersion>, loan semantic-version
  find <MaxItems>, loan max-items
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-application-dependencies-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 482
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 483
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 484
    seed json, like forbidden-exception
    note <ForbiddenException>
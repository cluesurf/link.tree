
load @drumwork/snow/amazonaws.com/serverlessrepo/2017-09-08/base/bad-request-exception
  take form bad-request-exception

load @drumwork/snow/amazonaws.com/serverlessrepo/2017-09-08/base/forbidden-exception
  take form forbidden-exception

load @drumwork/snow/amazonaws.com/serverlessrepo/2017-09-08/base/internal-server-error-exception
  take form internal-server-error-exception

load @drumwork/snow/amazonaws.com/serverlessrepo/2017-09-08/base/list-applications-response
  take form list-applications-response

load @drumwork/snow/amazonaws.com/serverlessrepo/2017-09-08/base/not-found-exception
  take form not-found-exception

load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

boot list-applications, name <ListApplications>
  deed get
  note <Lists applications owned by the requester.>

  take max-items
    like native-integer
    note <The total number of items to return.>
    void take
  take next-token
    like native-string
    note <A token to specify where to start paginating.>
    void take
  take max-items
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take

  line </applications>

  find <maxItems>, loan max-items
  find <nextToken>, loan next-token
  find <MaxItems>, loan max-items
  find <NextToken>, loan next-token

  loot 200
    seed json, like list-applications-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 482
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 483
    seed json, like forbidden-exception
    note <ForbiddenException>
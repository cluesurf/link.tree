
load @wavebond/snow/base/string
  take form string

form application-dependency-summary, name <ApplicationDependencySummary>
  note <A nested application summary.>
  take application-id, name <ApplicationId>
    like string
    note <The Amazon Resource Name (ARN) of the nested application.>
    host xml
      host name, text <applicationId>
  take semantic-version, name <SemanticVersion>
    like string
    note <The semantic version of the nested application.>
    host xml
      host name, text <semanticVersion>

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/access-denied-exception
  take form access-denied-exception

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/conflict-exception
  take form conflict-exception

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/internal-server-exception
  take form internal-server-exception

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/throttling-exception
  take form throttling-exception

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/update-share-invitation-body
  take form update-share-invitation-body

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/update-share-invitation-output
  take form update-share-invitation-output

load @tunebond/snow/amazonaws.com/wellarchitected/2020-03-31/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-string
  take form native-string

boot update-share-invitation, name <UpdateShareInvitation>
  deed patch
  note <Update a workload invitation.>

  take share-invitation-id
    like native-string
    note <The ID assigned to the share invitation.>
  take update-share-invitation-body, like update-share-invitation-body

  line </shareInvitations/{share-invitation-id}>

  seed json, loan update-share-invitation-body

  loot 200
    seed json, like update-share-invitation-output
    note <Success>
  loot 480
    seed json, like validation-exception
    note <ValidationException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like conflict-exception
    note <ConflictException>
  loot 484
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 485
    seed json, like throttling-exception
    note <ThrottlingException>

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/associate-attribute-group-response
  take form associate-attribute-group-response

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/service-quota-exceeded-exception
  take form service-quota-exceeded-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/validation-exception
  take form validation-exception

load @textsurf/snow/base/native-string
  take form native-string

boot associate-attribute-group, name <AssociateAttributeGroup>
  deed put
  note <Associates an attribute group with an application to augment the application's metadata with the group's attributes. This feature enables applications to be described with user-defined details that are machine-readable, such as third-party integrations.>

  take application
    like native-string
    note <The name or ID of the application.>
  take attribute-group
    like native-string
    note <The name or ID of the attribute group that holds the attributes to describe the application.>

  line </applications/{application}/attribute-groups/{attribute-group}>

  loot 200
    seed json, like associate-attribute-group-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 483
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
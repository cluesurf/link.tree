
load @tunebond/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/internal-server-exception
  take form internal-server-exception

load @tunebond/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/list-associated-resources-response
  take form list-associated-resources-response

load @tunebond/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/validation-exception
  take form validation-exception

load @tunebond/snow/base/native-integer
  take form native-integer

load @tunebond/snow/base/native-string
  take form native-string

boot list-associated-resources, name <ListAssociatedResources>
  deed get
  note <Lists all resources that are associated with specified application. Results are paginated.>

  take application
    like native-string
    note <The name or ID of the application.>
  take next-token
    like native-string
    note <The token to use to get the next page of results after a previous API call.>
    void take
  take max-results
    like native-integer
    note <The upper bound of the number of results to return (cannot exceed 25). If this parameter is omitted, it defaults to 25. This value is optional.>
    void take

  line </applications/{application}/resources>

  find <nextToken>, loan next-token
  find <maxResults>, loan max-results

  loot 200
    seed json, like list-associated-resources-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like validation-exception
    note <ValidationException>
  loot 482
    seed json, like internal-server-exception
    note <InternalServerException>
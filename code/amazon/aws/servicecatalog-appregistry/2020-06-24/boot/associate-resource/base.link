
load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/associate-resource-response
  take form associate-resource-response

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/conflict-exception
  take form conflict-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/internal-server-exception
  take form internal-server-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/amazonaws.com/servicecatalog-appregistry/2020-06-24/base/service-quota-exceeded-exception
  take form service-quota-exceeded-exception

load @textsurf/snow/base/native-string
  take form native-string

boot associate-resource, name <AssociateResource>
  deed put
  note <Associates a resource with an application. Both the resource and the application can be specified either by ID or name.>

  take application
    like native-string
    note <The name or ID of the application.>
  take resource-type
    like native-string
    note <The type of resource of which the application will be associated.>
  take resource
    like native-string
    note <The name or ID of the resource of which the application will be associated.>

  line </applications/{application}/resources/{resource-type}/{resource}>

  loot 200
    seed json, like associate-resource-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like internal-server-exception
    note <InternalServerException>
  loot 482
    seed json, like service-quota-exceeded-exception
    note <ServiceQuotaExceededException>
  loot 483
    seed json, like conflict-exception
    note <ConflictException>
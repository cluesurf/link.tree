
load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/bad-request-exception
  take form bad-request-exception

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/delete-route-output
  take form delete-route-output

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/forbidden-exception
  take form forbidden-exception

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/internal-server-error-exception
  take form internal-server-error-exception

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/not-found-exception
  take form not-found-exception

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/resource-in-use-exception
  take form resource-in-use-exception

load @drumwork/snow/amazonaws.com/appmesh/2018-10-01/base/service-unavailable-exception
  take form service-unavailable-exception


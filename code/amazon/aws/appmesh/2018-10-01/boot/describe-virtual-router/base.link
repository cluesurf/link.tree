load @drumwork/snow/base/native-string
  take form native-string

boot describe-virtual-router, name <DescribeVirtualRouter>
  note <Describes an existing virtual router.>

  take mesh-name
    like native-string
    note <The name of the service mesh in which the virtual router resides.>
  take virtual-router-name
    like native-string
    note <The name of the virtual router to describe.>

  line </meshes/{mesh-name}/virtualRouters/{virtual-router-name}>

  loot 200
    seed json, like describe-virtual-router-output
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 482
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 483
    seed json, like not-found-exception
    note <NotFoundException>
  loot 484
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 485
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
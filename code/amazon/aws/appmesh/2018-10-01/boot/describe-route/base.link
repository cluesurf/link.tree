
load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/describe-route-output
  take form describe-route-output

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/forbidden-exception
  take form forbidden-exception

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/internal-server-error-exception
  take form internal-server-error-exception

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/service-unavailable-exception
  take form service-unavailable-exception

load @nerdbond/snow/amazonaws.com/appmesh/2018-10-01/base/too-many-requests-exception
  take form too-many-requests-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot describe-route, name <DescribeRoute>
  deed get
  note <Describes an existing route.>

  take mesh-name
    like native-string
    note <The name of the service mesh in which the route resides.>
  take route-name
    like native-string
    note <The name of the route to describe.>
  take virtual-router-name
    like native-string
    note <The name of the virtual router with which the route is associated.>

  line </meshes/{mesh-name}/virtualRouter/{virtual-router-name}/routes/{route-name}>

  loot 200
    seed json, like describe-route-output
    note <Success>
  loot 480
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 481
    seed json, like forbidden-exception
    note <ForbiddenException>
  loot 482
    seed json, like internal-server-error-exception
    note <InternalServerErrorException>
  loot 483
    seed json, like not-found-exception
    note <NotFoundException>
  loot 484
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 485
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/conflict-exception
  take form conflict-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/internal-failure-exception
  take form internal-failure-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/throttling-exception
  take form throttling-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/unsupported-user-edition-exception
  take form unsupported-user-edition-exception

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/update-analysis-permissions-body
  take form update-analysis-permissions-body

load @wavebond/snow/amazonaws.com/quicksight/2018-04-01/base/update-analysis-permissions-response
  take form update-analysis-permissions-response

load @wavebond/snow/base/native-string
  take form native-string

boot update-analysis-permissions, name <UpdateAnalysisPermissions>
  deed put
  note <Updates the read and write permissions for an analysis.>

  take aws-account-id
    like native-string
    note <The ID of the Amazon Web Services account that contains the analysis whose permissions you're updating. You must be using the Amazon Web Services account that the analysis is in.>
  take analysis-id
    like native-string
    note <The ID of the analysis whose permissions you're updating. The ID is part of the analysis URL.>
  take update-analysis-permissions-body, like update-analysis-permissions-body

  line </accounts/{aws-account-id}/analyses/{analysis-id}/permissions>

  seed json, loan update-analysis-permissions-body

  loot 200
    seed json, like update-analysis-permissions-response
    note <Success>
  loot 480
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 481
    seed json, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 482
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 483
    seed json, like unsupported-user-edition-exception
    note <UnsupportedUserEditionException>
  loot 484
    seed json, like conflict-exception
    note <ConflictException>
  loot 485
    seed json, like internal-failure-exception
    note <InternalFailureException>

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/access-denied-exception
  take form access-denied-exception

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/describe-analysis-response
  take form describe-analysis-response

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/internal-failure-exception
  take form internal-failure-exception

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/throttling-exception
  take form throttling-exception

load @treesurf/snow/amazonaws.com/quicksight/2018-04-01/base/unsupported-user-edition-exception
  take form unsupported-user-edition-exception

load @treesurf/snow/base/native-string
  take form native-string

boot describe-analysis, name <DescribeAnalysis>
  deed get
  note <Provides a summary of the metadata for an analysis.>

  take aws-account-id
    like native-string
    note <The ID of the Amazon Web Services account that contains the analysis. You must be using the Amazon Web Services account that the analysis is in.>
  take analysis-id
    like native-string
    note <The ID of the analysis that you're describing. The ID is part of the URL of the analysis.>

  line </accounts/{aws-account-id}/analyses/{analysis-id}>

  loot 200
    seed json, like describe-analysis-response
    note <Success>
  loot 480
    seed json, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like access-denied-exception
    note <AccessDeniedException>
  loot 483
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 484
    seed json, like unsupported-user-edition-exception
    note <UnsupportedUserEditionException>
  loot 485
    seed json, like internal-failure-exception
    note <InternalFailureException>

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/conflict-exception
  take form conflict-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/create-analysis-body
  take form create-analysis-body

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/create-analysis-response
  take form create-analysis-response

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/internal-failure-exception
  take form internal-failure-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/resource-exists-exception
  take form resource-exists-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/resource-not-found-exception
  take form resource-not-found-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/throttling-exception
  take form throttling-exception

load @drumwork/snow/amazonaws.com/quicksight/2018-04-01/base/unsupported-user-edition-exception
  take form unsupported-user-edition-exception

load @drumwork/snow/base/native-string
  take form native-string

boot create-analysis, name <CreateAnalysis>
  note <Creates an analysis in Amazon QuickSight.>

  take aws-account-id
    like native-string
    note <The ID of the Amazon Web Services account where you are creating an analysis.>
  take analysis-id
    like native-string
    note <The ID for the analysis that you're creating. This ID displays in the URL of the analysis.>
  take create-analysis-body, like create-analysis-body

  line </accounts/{aws-account-id}/analyses/{analysis-id}>

  seed json, loan create-analysis-body

  loot 200
    seed json, like create-analysis-response
    note <Success>
  loot 480
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 481
    seed json, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 482
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 483
    seed json, like resource-exists-exception
    note <ResourceExistsException>
  loot 484
    seed json, like conflict-exception
    note <ConflictException>
  loot 485
    seed json, like unsupported-user-edition-exception
    note <UnsupportedUserEditionException>
  loot 486
    seed json, like internal-failure-exception
    note <InternalFailureException>
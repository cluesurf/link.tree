
load @nerdbond/snow/amazonaws.com/alexaforbusiness/2017-11-09/base/approve-skill-response
  take form approve-skill-response

load @nerdbond/snow/amazonaws.com/alexaforbusiness/2017-11-09/base/concurrent-modification-exception
  take form concurrent-modification-exception

load @nerdbond/snow/amazonaws.com/alexaforbusiness/2017-11-09/base/limit-exceeded-exception
  take form limit-exceeded-exception

load @nerdbond/snow/amazonaws.com/alexaforbusiness/2017-11-09/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot approve-skill, name <ApproveSkill>
  deed post
  note <Associates a skill with the organization under the customer's AWS account. If a skill is private, the user implicitly accepts access to this skill during enablement.>

  take x-amz-target
    like native-string
  take approve-skill-request, like approve-skill-request

  line </#X-Amz-Target=AlexaForBusiness.ApproveSkill>

  hint <X-Amz-Target>, loan x-amz-target

  seed json, loan approve-skill-request

  loot 200
    seed json, like approve-skill-response
    note <Success>
  loot 480
    seed json, like limit-exceeded-exception
    note <LimitExceededException>
  loot 481
    seed json, like not-found-exception
    note <NotFoundException>
  loot 482
    seed json, like concurrent-modification-exception
    note <ConcurrentModificationException>

load @tunebond/snow/amazonaws.com/backup/2018-11-15/base/get-recovery-point-restore-metadata-output
  take form get-recovery-point-restore-metadata-output

load @tunebond/snow/amazonaws.com/backup/2018-11-15/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @tunebond/snow/amazonaws.com/backup/2018-11-15/base/missing-parameter-value-exception
  take form missing-parameter-value-exception

load @tunebond/snow/amazonaws.com/backup/2018-11-15/base/resource-not-found-exception
  take form resource-not-found-exception

load @tunebond/snow/amazonaws.com/backup/2018-11-15/base/service-unavailable-exception


load @drumwork/snow/amazonaws.com/monitoring/2010-08-01/base/internal-service-fault
  take form internal-service-fault

load @drumwork/snow/amazonaws.com/monitoring/2010-08-01/base/invalid-next-token
  take form invalid-next-token

load @drumwork/snow/amazonaws.com/monitoring/2010-08-01/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @drumwork/snow/amazonaws.com/monitoring/2010-08-01/base/list-metric-streams-output
  take form list-metric-streams-output

load @drumwork/snow/amazonaws.com/monitoring/2010-08-01/base/missing-required-parameter-exception
  take form missing-required-parameter-exception

load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

boot get-list-metric-streams, name <GET_ListMetricStreams>
  note <Returns a list of metric streams in this account.>

  take next-token
    like native-string
    note <Include this value, if it was returned by the previous call, to get the next set of metric streams.>
    void take
  take max-results
    like native-integer
    note <The maximum number of results to return in one operation.>
    void take
  take action
    like native-string
  take version
    like native-string

  line </#Action=ListMetricStreams>

  find <NextToken>, loan next-token
  find <MaxResults>, loan max-results
  find <Action>, loan action
  find <Version>, loan version

  loot 200
    seed xml, like list-metric-streams-output
    note <Success>
  loot 480
    seed xml, like invalid-next-token
    note <InvalidNextToken>
  loot 481
    seed xml, like internal-service-fault
    note <InternalServiceFault>
  loot 482
    seed xml, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 483
    seed xml, like missing-required-parameter-exception
    note <MissingRequiredParameterException>

load @nerdbond/snow/amazonaws.com/monitoring/2010-08-01/base/internal-service-fault
  take form internal-service-fault

load @nerdbond/snow/amazonaws.com/monitoring/2010-08-01/base/invalid-next-token
  take form invalid-next-token

load @nerdbond/snow/amazonaws.com/monitoring/2010-08-01/base/invalid-parameter-value-exception
  take form invalid-parameter-value-exception

load @nerdbond/snow/amazonaws.com/monitoring/2010-08-01/base/list-metric-streams-output
  take form list-metric-streams-output

load @nerdbond/snow/amazonaws.com/monitoring/2010-08-01/base/missing-required-parameter-exception
  take form missing-required-parameter-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot post-list-metric-streams, name <POST_ListMetricStreams>
  deed post
  note <Returns a list of metric streams in this account.>

  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take action
    like native-string
  take version
    like native-string
  take list-metric-streams-input, like list-metric-streams-input
    void take

  line </#Action=ListMetricStreams>

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token
  find <Action>, loan action
  find <Version>, loan version

  seed xml, loan list-metric-streams-input

  loot 200
    seed xml, like list-metric-streams-output
    note <Success>
  loot 480
    seed xml, like invalid-next-token
    note <InvalidNextToken>
  loot 481
    seed xml, like internal-service-fault
    note <InternalServiceFault>
  loot 482
    seed xml, like invalid-parameter-value-exception
    note <InvalidParameterValueException>
  loot 483
    seed xml, like missing-required-parameter-exception
    note <MissingRequiredParameterException>
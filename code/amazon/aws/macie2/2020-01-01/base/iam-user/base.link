
load @treesurf/snow/base/string
  take form string

form iam-user, name <IamUser>
  note <Provides information about an Identity and Access Management (IAM) user who performed an action on an affected resource.>
  take account-id, name <accountId>
    like string
    void take
    note <The unique identifier for the Amazon Web Services account that's associated with the IAM user who performed the action.>
  take arn, name <arn>
    like string
    void take
    note <The Amazon Resource Name (ARN) of the principal that performed the action. The last section of the ARN contains the name of the user who performed the action.>
  take principal-id, name <principalId>
    like string
    void take
    note <The unique identifier for the IAM user who performed the action.>
  take user-name, name <userName>
    like string
    void take
    note <The user name of the IAM user who performed the action.>
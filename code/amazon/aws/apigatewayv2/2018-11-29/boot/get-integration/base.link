
load @tunebond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/get-integration-result
  take form get-integration-result

load @tunebond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/not-found-exception
  take form not-found-exception

load @tunebond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/too-many-requests-exception
  take form too-many-requests-exception

load @tunebond/snow/base/native-string
  take form native-string

boot get-integration, name <GetIntegration>
  deed get
  note <Gets an Integration.>

  take api-id
    like native-string
    note <The API identifier.>
  take integration-id
    like native-string
    note <The integration ID.>

  line </v2/apis/{api-id}/integrations/{integration-id}>

  loot 200
    seed json, like get-integration-result
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
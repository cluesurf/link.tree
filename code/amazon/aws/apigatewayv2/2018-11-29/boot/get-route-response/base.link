
load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/get-route-response-response
  take form get-route-response-response

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/too-many-requests-exception
  take form too-many-requests-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot get-route-response, name <GetRouteResponse>
  deed get
  note <Gets a RouteResponse.>

  take api-id
    like native-string
    note <The API identifier.>
  take route-id
    like native-string
    note <The route ID.>
  take route-response-id
    like native-string
    note <The route response ID.>

  line </v2/apis/{api-id}/routes/{route-id}/routeresponses/{route-response-id}>

  loot 200
    seed json, like get-route-response-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
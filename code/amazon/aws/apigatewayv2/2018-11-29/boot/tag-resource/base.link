
load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/bad-request-exception
  take form bad-request-exception

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/conflict-exception
  take form conflict-exception

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/not-found-exception
  take form not-found-exception

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/tag-resource-body
  take form tag-resource-body

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/tag-resource-response
  take form tag-resource-response

load @nerdbond/snow/amazonaws.com/apigatewayv2/2018-11-29/base/too-many-requests-exception
  take form too-many-requests-exception

load @nerdbond/snow/base/native-string
  take form native-string

boot tag-resource, name <TagResource>
  deed post
  note <Creates a new Tag resource to represent a tag.>

  take resource-arn
    like native-string
    note <The resource ARN for the tag.>
  take tag-resource-body, like tag-resource-body

  line </v2/tags/{resource-arn}>

  seed json, loan tag-resource-body

  loot 201
    seed json, like tag-resource-response
    note <Success>
  loot 480
    seed json, like not-found-exception
    note <NotFoundException>
  loot 481
    seed json, like too-many-requests-exception
    note <TooManyRequestsException>
  loot 482
    seed json, like bad-request-exception
    note <BadRequestException>
  loot 483
    seed json, like conflict-exception
    note <ConflictException>
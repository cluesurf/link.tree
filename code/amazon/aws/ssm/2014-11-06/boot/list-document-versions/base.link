
load @tunebond/snow/amazonaws.com/ssm/2014-11-06/base/internal-server-error
  take form internal-server-error

load @tunebond/snow/amazonaws.com/ssm/2014-11-06/base/invalid-document
  take form invalid-document

load @tunebond/snow/amazonaws.com/ssm/2014-11-06/base/invalid-next-token
  take form invalid-next-token

load @tunebond/snow/amazonaws.com/ssm/2014-11-06/base/list-document-versions-result
  take form list-document-versions-result

load @tunebond/snow/base/native-string
  take form native-string

boot list-document-versions, name <ListDocumentVersions>
  deed post
  note <List all versions for a document.>

  take x-amz-target
    like native-string
  take max-results
    like native-string
    note <Pagination limit>
    void take
  take next-token
    like native-string
    note <Pagination token>
    void take
  take list-document-versions-request, like list-document-versions-request

  line </#X-Amz-Target=AmazonSSM.ListDocumentVersions>

  hint <X-Amz-Target>, loan x-amz-target

  find <MaxResults>, loan max-results
  find <NextToken>, loan next-token

  seed json, loan list-document-versions-request

  loot 200
    seed json, like list-document-versions-result
    note <Success>
  loot 480
    seed json, like internal-server-error
    note <InternalServerError>
  loot 481
    seed json, like invalid-next-token
    note <InvalidNextToken>
  loot 482
    seed json, like invalid-document
    note <InvalidDocument>
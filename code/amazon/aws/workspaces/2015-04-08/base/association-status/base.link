
load @nerdbond/snow/base/native-string
  take form native-string

form association-status, name <AssociationStatus>
  like native-string
    text <NOT_ASSOCIATED>
    text <ASSOCIATED_WITH_OWNER_ACCOUNT>
    text <ASSOCIATED_WITH_SHARED_ACCOUNT>
    text <PENDING_ASSOCIATION>
    text <PENDING_DISASSOCIATION>
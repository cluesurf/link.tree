
load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/certificate-validation-exception
  take form certificate-validation-exception

load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/get-pending-job-executions-response
  take form get-pending-job-executions-response

load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/invalid-request-exception
  take form invalid-request-exception

load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/resource-not-found-exception
  take form resource-not-found-exception

load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/service-unavailable-exception
  take form service-unavailable-exception

load @textsurf/snow/amazonaws.com/iot-jobs-data/2017-09-29/base/throttling-exception
  take form throttling-exception

load @textsurf/snow/base/native-string
  take form native-string

boot get-pending-job-executions, name <GetPendingJobExecutions>
  deed get
  note <Gets the list of all jobs for a thing that are not in a terminal status.>

  take thing-name
    like native-string
    note <The name of the thing that is executing the job.>

  line </things/{thing-name}/jobs>

  loot 200
    seed json, like get-pending-job-executions-response
    note <Success>
  loot 480
    seed json, like invalid-request-exception
    note <InvalidRequestException>
  loot 481
    seed json, like resource-not-found-exception
    note <ResourceNotFoundException>
  loot 482
    seed json, like throttling-exception
    note <ThrottlingException>
  loot 483
    seed json, like service-unavailable-exception
    note <ServiceUnavailableException>
  loot 484
    seed json, like certificate-validation-exception
    note <CertificateValidationException>


load @textsurf/snow/base/native-string
  take form native-string

load @textsurf/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @textsurf/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot pins-list, name <pins_list>
  deed get
  note <Lists items pinned to a channel.>

  take token
    like native-string
    note <Authentication token. Requires scope: `pins:read`>
  take channel
    like native-string
    note <Channel to get pinned items for.>

  line </pins.list>

  find <token>, loan token
  find <channel>, loan channel

  loot 200
    seed json
      like list
        like sink
          like form
            take items, name <items>
            take ok, name <ok>
              like defs-ok-true
          like form
            take count, name <count>
            take ok, name <ok>
              like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take callstack, name <callstack>
          void take
        take error, name <error>
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

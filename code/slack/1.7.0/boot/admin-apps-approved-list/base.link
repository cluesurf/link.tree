
load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot admin-apps-approved-list, name <admin_apps_approved_list>
  note <List approved apps for an org or workspace.>

  take token
    like native-string
    note <Authentication token. Requires scope: `admin.apps:read`>
  take limit
    like native-integer
    note <The maximum number of items to return. Must be between 1 - 1000 both inclusive.>
    void take
  take cursor
    like native-string
    note <Set `cursor` to `next_cursor` returned by the previous call to list items in the next page>
    void take
  take team-id
    like native-string
    void take
  take enterprise-id
    like native-string
    void take

  line </admin.apps.approved.list>

  find <token>, loan token
  find <limit>, loan limit
  find <cursor>, loan cursor
  find <team_id>, loan team-id
  find <enterprise_id>, loan enterprise-id

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

load @wavebond/snow/base/native-string
  take form native-string

load @wavebond/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @wavebond/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot apps-uninstall, name <apps_uninstall>
  deed get
  note <Uninstalls your app from a workspace.>

  take token
    like native-string
    note <Authentication token. Requires scope: `none`>
    void take
  take client-id
    like native-string
    note <Issued when you created your application.>
    void take
  take client-secret
    like native-string
    note <Issued when you created your application.>
    void take

  line </apps.uninstall>

  find <token>, loan token
  find <client_id>, loan client-id
  find <client_secret>, loan client-secret

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take callstack, name <callstack>
          void take
        take error, name <error>
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

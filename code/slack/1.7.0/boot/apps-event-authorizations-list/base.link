
load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot apps-event-authorizations-list, name <apps_event_authorizations_list>
  note <Get a list of authorizations for the given event context. Each authorization represents an app installation that the event is visible to.>

  take token
    like native-string
    note <Authentication token. Requires scope: `authorizations:read`>
  take event-context
    like native-string
  take cursor
    like native-string
    void take
  take limit
    like native-integer
    void take

  line </apps.event.authorizations.list>

  hint <token>, loan token

  find <event_context>, loan event-context
  find <cursor>, loan cursor
  find <limit>, loan limit

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>
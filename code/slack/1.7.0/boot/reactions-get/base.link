
load @wavebond/snow/base/form
  take form form

load @wavebond/snow/base/native-boolean
  take form native-boolean

load @wavebond/snow/base/native-string
  take form native-string

load @wavebond/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

boot reactions-get, name <reactions_get>
  deed get
  note <Gets reactions for an item.>

  take token
    like native-string
    note <Authentication token. Requires scope: `reactions:read`>
  take channel
    like native-string
    note <Channel where the message to get reactions for was posted.>
    void take
  take file
    like native-string
    note <File to get reactions for.>
    void take
  take file-comment
    like native-string
    note <File comment to get reactions for.>
    void take
  take full
    like native-boolean
    note <If true always return the complete reaction list.>
    void take
  take timestamp
    like native-string
    note <Timestamp of the message to get reactions for.>
    void take

  line </reactions.get>

  find <token>, loan token
  find <channel>, loan channel
  find <file>, loan file
  find <file_comment>, loan file-comment
  find <full>, loan full
  find <timestamp>, loan timestamp

  loot 200
    seed json
      like form
    note <Typical success response>
  loot fall
    seed json
      like form
        take callstack, name <callstack>
          void take
        take error, name <error>
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

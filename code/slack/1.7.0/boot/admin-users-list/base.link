
load @drumwork/snow/base/native-integer
  take form native-integer

load @drumwork/snow/base/native-string
  take form native-string

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @drumwork/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot admin-users-list, name <admin_users_list>
  deed get
  note <List users on a workspace>

  take token
    like native-string
    note <Authentication token. Requires scope: `admin.users:read`>
  take team-id
    like native-string
    note <The ID (`T1234`) of the workspace.>
  take cursor
    like native-string
    note <Set `cursor` to `next_cursor` returned by the previous call to list items in the next page.>
    void take
  take limit
    like native-integer
    note <Limit for how many users to be retrieved per page>
    void take

  line </admin.users.list>

  hint <token>, loan token

  find <team_id>, loan team-id
  find <cursor>, loan cursor
  find <limit>, loan limit

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>
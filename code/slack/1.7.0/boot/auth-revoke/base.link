
load @tunebond/snow/base/native-boolean
  take form native-boolean

load @tunebond/snow/base/native-string
  take form native-string

load @tunebond/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @tunebond/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot auth-revoke, name <auth_revoke>
  deed get
  note <Revokes a token.>

  take token
    like native-string
    note <Authentication token. Requires scope: `none`>
  take test
    like native-boolean
    note <Setting this parameter to `1` triggers a _testing mode_ where the specified token will not actually be revoked.>
    void take

  line </auth.revoke>

  find <token>, loan token
  find <test>, loan test

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
        take revoked, name <revoked>
    note <Typical success response>
  loot fall
    seed json
      like form
        take callstack, name <callstack>
          void take
        take error, name <error>
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

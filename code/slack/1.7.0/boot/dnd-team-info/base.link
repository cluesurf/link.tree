
load @textsurf/snow/base/native-string
  take form native-string

load @textsurf/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @textsurf/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

boot dnd-team-info, name <dnd_teamInfo>
  deed get
  note <Retrieves the Do Not Disturb status for up to 50 users on a team.>

  take token
    like native-string
    note <Authentication token. Requires scope: `dnd:read`>
    void take
  take users
    like native-string
    note <Comma-separated list of users to fetch Do Not Disturb status for>
    void take

  line </dnd.teamInfo>

  find <token>, loan token
  find <users>, loan users

  loot 200
    seed json
      like form
        take ok, name <ok>
          like defs-ok-true
    note <Typical success response>
  loot fall
    seed json
      like form
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

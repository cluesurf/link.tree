
load @treesurf/snow/base/native-string
  take form native-string

load @treesurf/snow/slack.com/1.7.0/base/defs-ok-false
  take form defs-ok-false

load @treesurf/snow/slack.com/1.7.0/base/defs-ok-true
  take form defs-ok-true

load @treesurf/snow/slack.com/1.7.0/base/objs-paging
  take form objs-paging

boot team-integration-logs, name <team_integrationLogs>
  deed get
  note <Gets the integration logs for the current team.>

  take token
    like native-string
    note <Authentication token. Requires scope: `admin`>
  take app-id
    like native-string
    note <Filter logs to this Slack app. Defaults to all logs.>
    void take
  take change-type
    like native-string
    note <Filter logs with this change type. Defaults to all logs.>
    void take
  take count
    like native-string
    void take
  take page
    like native-string
    void take
  take service-id
    like native-string
    note <Filter logs to this service. Defaults to all logs.>
    void take
  take user
    like native-string
    note <Filter logs generated by this userâ€™s actions. Defaults to all logs.>
    void take

  line </team.integrationLogs>

  find <token>, loan token
  find <app_id>, loan app-id
  find <change_type>, loan change-type
  find <count>, loan count
  find <page>, loan page
  find <service_id>, loan service-id
  find <user>, loan user

  loot 200
    seed json
      like form
        take logs, name <logs>
        take ok, name <ok>
          like defs-ok-true
        take paging, name <paging>
          like objs-paging
    note <Typical success response>
  loot fall
    seed json
      like form
        take callstack, name <callstack>
          void take
        take error, name <error>
        take ok, name <ok>
          like defs-ok-false
    note <Typical error response>

load @treesurf/snow/base/form
  take form form

load @treesurf/snow/base/native-string
  take form native-string

load @treesurf/snow/pager-duty/base/alert
  take form alert

boot update-alert
  deed put
  note <Resolve an alert or associate an alert with a new parent incident.>

  take alert-id
    like native-string
    note <The id of the alert to update.>
  take alert
    like form
      take alert, name <alert>
        like alert
    note <The parameters of the alert to update.>
    void take

  line </incidents/{id}/alerts/{alert-id}/>

  seed json, loan alert

  loot 200
    note <The alert that was updated.>
  loot 401
  loot 403
  loot 404
  loot 429


load @tunebond/snow/base/form
  take form form

load @tunebond/snow/pager-duty/base/integration-reference
  take form integration-reference

load @tunebond/snow/pager-duty/base/service-reference
  take form service-reference

load @tunebond/snow/pager-duty/base/vendor-reference
  take form vendor-reference

form integration, name <Integration>
  like link
    like integration-reference
    like form
      take name, name <name>
        void take
      take service, name <service>
        like service-reference
        void take
      take created-at, name <created_at>
        void take
      take vendor, name <vendor>
        like vendor-reference
        void take

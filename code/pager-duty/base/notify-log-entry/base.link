
load @drumwork/snow/base/form
  take form form

load @drumwork/snow/pager-duty/base/log-entry
  take form log-entry

load @drumwork/snow/pager-duty/base/notify-log-entry-reference
  take form notify-log-entry-reference

load @drumwork/snow/pager-duty/base/user-reference
  take form user-reference

form notify-log-entry, name <NotifyLogEntry>
  like link
    like log-entry
    like notify-log-entry-reference
    like form
      take created-at, name <created_at>
        void take
      take user, name <user>
        like user-reference
        void take
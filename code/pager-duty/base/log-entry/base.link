
load @wavebond/snow/base/form
  take form form

load @wavebond/snow/pager-duty/base/agent
  take form agent

load @wavebond/snow/pager-duty/base/channel
  take form channel

load @wavebond/snow/pager-duty/base/incident-reference
  take form incident-reference

load @wavebond/snow/pager-duty/base/log-entry-reference
  take form log-entry-reference

load @wavebond/snow/pager-duty/base/service-reference
  take form service-reference

form log-entry, name <LogEntry>
  like link
    like log-entry-reference
    like form
      take created-at, name <created_at>
        void take
      take channel, name <channel>
        like channel
        void take
      take agent, name <agent>
        like agent
        void take
      take note, name <note>
        void take
      take contexts, name <contexts>
        void take
      take service, name <service>
        like service-reference
        void take
      take incident, name <incident>
        like incident-reference
        void take
      take teams, name <teams>
        void take
      take event-details, name <event_details>
        like form
          take description, name <description>
            void take
        void take
